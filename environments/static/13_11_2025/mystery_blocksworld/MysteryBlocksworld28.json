            }
        },
        "grounder": {
            "instances": {
                "input": [],
                "output": "instances",
                "system_prompt": "Extract objects, initial facts, and goals for each scenario; deduplicate facts; keep symbol names unchanged; do not emit PDDL.",
                "prompt": "Your name is grounder. From environment->public_information.objects and environment->public_information.scenarios, produce JSON with keys: objects, scenarios (each with name, init, goal, plan_if_any)."
            }
        },
        "auditor": {
            "audit": {
                "input": [
                    "domain_spec",
                    "instances"
                ],
                "output": "audit_report",
                "system_prompt": "Audit temporal and causal consistency; verify Scenario A plan step-by-step against preconditions and effects; remove bookkeeping shortcuts; provide repaired_plan if necessary; provide feasibility guidance for Scenario B; do not emit PDDL.",
                "prompt": "Your name is auditor. Using domain_spec and instances, validate Scenario A plan and produce JSON with keys: validity, issues, causal_explanation, repaired_plan, recommended_constraints, scenario_b_hints."
            }
        },
        "orchestrator": {
            "pddl": {
                "input": [
                    "domain_spec",
                    "instances",
                    "audit_report"
                ],
                "output": "pddl_artifacts",
                "system_prompt": "Integrate all inputs and produce FastDownward-compatible PDDL; include one <domain> block and at least one <problem> block; ensure syntax is valid for FastDownward; keep predicate and action names exactly as modeled.",
                "prompt": "Your name is orchestrator. Produce the final PDDL domain and problem(s) for the FastDownward solver, enclosing the domain in <domain></domain> and each problem in <problem></problem> tags; use domain_spec, instances, and audit_report to ensure correctness; prefer a problem for Scenario B and optionally include Scenario A for verification."
            }
        },
        "constraints": [
            "domain_modeler.spec->auditor.audit",
            "grounder.instances->auditor.audit",
            "domain_modeler.spec->orchestrator.pddl",
            "grounder.instances->orchestrator.pddl",
            "auditor.audit->orchestrator.pddl"
        ]
