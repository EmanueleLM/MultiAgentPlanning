    "workflow": {
        "modeler": {
            "extract_actions": {
                "input": [],
                "output": "action_schemas",
                "system_prompt": "Extract a solver-agnostic STRIPS-like action model from the provided domain description. Return JSON with keys types, predicates, and actions; each action has name, parameters (variables like ?x with type obj), preconditions_pos, effects_add, and effects_del. Preserve exact predicate and action names and do not emit PDDL.",
                "prompt": "Use the planning description to build the JSON action model. Actions are listed at {environment->planning->actions} and predicates at {environment->planning->predicates}. Include a single type obj for all objects."
            }
        },
        "analyst": {
            "validate_plan_a": {
                "input": [
                    "action_schemas"
                ],
                "output": "validation_report_a",
                "system_prompt": "Simulate the candidate plan under a forward STRIPS semantics using the provided action model and Scenario A. Do not emit PDDL. Return JSON with fields is_valid (boolean), goal_satisfied (boolean), trace (list of state transitions with applied action and changed facts), final_state (set or list of facts), and notes (diagnostics).",
                "prompt": "Validate Scenario A using the model in {action_schemas}. Initial facts: {environment->planning->scenarios->A->initial_facts}. Goal facts: {environment->planning->scenarios->A->goal_facts}. Plan: {environment->planning->scenarios->A->candidate_plan}. Assume all unspecified facts are false."
            },
            "prepare_problem_b": {
                "input": [
                    "action_schemas"
                ],
                "output": "problem_b_data",
                "system_prompt": "Prepare canonical problem data for Scenario B using the action model and the environment object set. Do not emit PDDL. Return JSON with keys objects (list), init (list of facts), and goal (list of facts). Ensure objects include the full set from the environment and any symbols referenced by the scenario.",
                "prompt": "Construct the problem data for Scenario B. Objects come from {environment->init->objects}. Initial facts: {environment->planning->scenarios->B->initial_facts}. Goal facts: {environment->planning->scenarios->B->goal_facts}. Preserve exact symbol names."
            }
        },
        "orchestrator": {
            "compile_pddl": {
                "input": [
                    "action_schemas",
                    "problem_b_data",
                    "validation_report_a"
                ],
                "output": "pddl_final",
                "system_prompt": "You are an expert PDDL engineer targeting the FastDownward solver. Using the action_schemas and problem_b_data, produce a single classical STRIPS PDDL domain and a single problem. Name the domain ObfuscatedDeceptiveLogistics10 and the problem problem-b. Include all predicates declared in the model, declare a single type obj, list all objects with type obj, list all initial facts from problem_b_data, and encode the goal as a conjunction. Ensure parameter names and predicate arities match exactly, use only :strips, and enclose the domain between <domain></domain> and the problem between <problem></problem>. Output only these two tagged blocks.",
                "prompt": "Integrate the action model {action_schemas} with the prepared problem data {problem_b_data}. Optionally consult {validation_report_a} to cross-check action semantics. Compile the final PDDL domain and problem for FastDownward exactly as requested in your system prompt."
            }
        },
        "constraints": [
            "modeler.extract_actions->analyst.validate_plan_a",
            "modeler.extract_actions->analyst.prepare_problem_b",
            "modeler.extract_actions->orchestrator.compile_pddl",
            "analyst.prepare_problem_b->orchestrator.compile_pddl",
            "analyst.validate_plan_a->orchestrator.compile_pddl"
        ]
    }
