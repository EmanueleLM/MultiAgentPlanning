(define (domain calendar-scheduling)
  (:requirements :strips)
  (:predicates
    (timeslot ?t)
    (participant ?p)
    (available ?p ?t)
    (meeting-duration-30)
    (scheduled ?t)
  )
  (:action schedule-meeting
    :parameters (?t)
    :precondition (and (timeslot ?t)
                       (available noah ?t)
                       (available teresa ?t)
                       (available bradley ?t)
                       (available philip ?t)
                       (available joyce ?t)
                       (available ryan ?t)
                       (available aaron ?t)
                       (meeting-duration-30)
                  )
    :effect (and (scheduled ?t))
  )
)