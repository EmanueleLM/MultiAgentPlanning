(define (domain meetup-domain)
  (:requirements :typing)
  (:types agent location time)
  (:constants traveler david - agent
              ggp chinatown - location)
  (:predicates
    (at ?a - agent ?l - location ?t - time)
    (next ?t - time ?t2 - time)
    (plus23 ?t - time ?t2 - time)
    (plus105 ?t - time ?t2 - time)
    (available-david ?t - time)
    (meeting-scheduled ?t - time ?t2 - time)
  )

  ;; Traveler waiting step-by-step (retains presence at same location at next minute)
  (:action wait-traveler
    :parameters (?loc - location ?t - time ?t2 - time)
    :precondition (and (at traveler ?loc ?t) (next ?t ?t2))
    :effect (and (at traveler ?loc ?t2))
  )

  ;; Traveler travels from Golden Gate Park to Chinatown taking exactly 23 minutes
  (:action travel-ggp-chinatown
    :parameters (?t - time ?t2 - time)
    :precondition (and (at traveler ggp ?t) (plus23 ?t ?t2))
    :effect (and (at traveler chinatown ?t2))
  )

