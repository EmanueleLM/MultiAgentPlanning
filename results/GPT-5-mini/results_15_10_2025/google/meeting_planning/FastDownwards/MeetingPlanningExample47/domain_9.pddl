(define (domain schedule-meetings-classical)
  (:requirements :typing :negative-preconditions :adl :equality :action-costs)
  (:types person location time)
  (:constants richmond-district nob-hill - location)
  (:predicates
    (at ?p - person ?l - location)
    (current-time ?t - time)
    (succ ?t1 - time ?t2 - time)
    (travel-end17 ?t1 - time ?t2 - time)
    (travel-end14 ?t1 - time ?t2 - time)
    (meet-end ?t1 - time ?t2 - time)
    (paul-available-window ?t1 - time ?t2 - time)
    (met-paul)
  )
  (:functions (total-cost))
  (:action travel-richmond-to-nobhill
    :parameters (?tr - person ?t1 - time ?t2 - time)
    :precondition (and (current-time ?t1) (at ?tr richmond-district) (travel-end17 ?t1 ?t2))
    :effect (and (not (at ?tr richmond-district)) (at ?tr nob-hill) (not (current-time ?t1)) (current-time ?t2) (increase (total-cost) 17))
  )
  (:action travel-nobhill-to-richmond
    :parameters (?tr - person ?t1 - time ?t2 - time)
    :precondition (and (current-time ?t1) (at ?tr nob-hill) (travel-end14 ?t1 ?t2))
    :effect (and (not (at ?tr nob-hill)) (at ?tr richmond-district) (not (current-time ?t1)) (current-time ?t2) (increase (total-cost) 14))
  )
  (:action meet-with-paul
    :parameters (?tr - person ?t1 - time ?t2 - time)
    :precondition (and (current-time ?t1) (at ?tr nob-hill) (meet-end ?t1 ?t2) (paul-available-window ?t1 ?t2) (not (met-paul)))
    :effect (and (met-paul) (not (current-time ?t1)) (current-time ?t2) (increase (total-cost) -1000))
  )
)