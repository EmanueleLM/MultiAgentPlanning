(define (domain three-colleagues)
  (:requirements :typing :negative-preconditions)
  (:types person slot)
  (:predicates
    (available ?p - person ?s - slot)
    (preferred ?s - slot)
    (meeting-scheduled)
    (booked ?s - slot)
  )
  (:action schedule
    :parameters (?s - slot)
    :precondition (and (available alice ?s) (available bob ?s) (available carol ?s) (preferred ?s) (not (meeting-scheduled)))
    :effect (and (meeting-scheduled) (booked ?s))
  )
)