(define (domain three-colleagues)
  (:requirements :typing :negative-preconditions)
  (:types person slot)
  (:predicates
    (available ?p - person ?s - slot)
    (at ?s - slot)
    (succ ?s - slot ?s2 - slot)
    (meeting_scheduled)
    (meeting_at ?s - slot)
  )
  (:action reject-alice
    :parameters (?s - slot ?s2 - slot)
    :precondition (and (at ?s) (succ ?s ?s2) (not (available alice ?s)))
    :effect (and (not (at ?s)) (at ?s2))
  )
  (:action reject-bob
    :parameters (?s - slot ?s2 - slot)
    :precondition (and (at ?s) (succ ?s ?s2) (not (available bob ?s)))
    :effect (and (not (at ?s)) (at ?s2))
  )
  (:action reject-carol
    :parameters (?s - slot ?s2 - slot)
    :precondition (and (at ?s) (succ ?s ?s2) (not (available carol ?s)))
    :effect (and (not (at ?s)) (at ?s2))
  )
  (:action select-current
    :parameters (?s - slot)
    :precondition (and (at ?s) (available alice ?s) (available bob ?s) (available carol ?s))
    :effect (and (meeting_scheduled) (meeting_at ?s))
  )
)