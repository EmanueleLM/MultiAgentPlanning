(define (problem BlocksworldScaling12)
  (:domain blocksworld-multiagent)
  (:objects
    a b c d e f g h i j k l m - block
  )
  (:init
    ;; Stack 1 (on table): B - G - E - M - A (A top)
    (ontable b)
    (on g b)
    (on e g)
    (on m e)
    (on a m)
    ;; Stack 2 (on table): K - J - L - F - C - H - I - D (D top)
    (ontable k)
    (on j k)
    (on l j)
    (on f l)
    (on c f)
    (on h c)
    (on i h)
    (on d i)

    ;; clear = top-of-stack blocks
    (clear a)
    (clear d)

    ;; type labels (vowels & consonants)
    (vowel a)
    (vowel e)
    (vowel i)

    (consonant b)
    (consonant c)
    (consonant d)
    (consonant f)
    (consonant g)
    (consonant h)
    (consonant j)
    (consonant k)
    (consonant l)
    (consonant m)
  )

  ;; Goal: final single stack with A on B, B on C, ..., L on M, and M on the table.
  (:goal (and
    (on a b)
    (on b c)
    (on c d)
    (on d e)
    (on e f)
    (on f g)
    (on g h)
    (on h i)
    (on i j)
    (on j k)
    (on k l)
    (on l m)
    (ontable m)
  ))
)