  (:init
    ;; Initial stacks:
    ;; Stack 1: A (table) -> J -> H -> I -> C -> D (top)
    (ontable A)
    (on J A)
    (on H J)
    (on I H)
    (on C I)
    (on D C)

    ;; Stack 2: M (table) -> E -> F -> B -> N -> L -> K (top)
    (ontable M)
    (on E M)
    (on F E)
    (on B F)
    (on N B)
    (on L N)
    (on K L)

    ;; G on table
    (ontable G)

    ;; clear: top blocks initially
    (clear D)
    (clear K)
    (clear G)

    (handempty)

    ;; step control: start at step0
    (current-step step0)

    ;; Allowed moves per step (encode the exact provided sequence)
    ;; 1. pickup(G)
    (allow-pickup step0 G)
    ;; 2. stack(G, D)
    (allow-stack step1 G D)
    ;; 3. unstack(K, L)
    (allow-unstack step2 K L)
    ;; 4. putdown(K)
    (allow-putdown step3 K)
    ;; 5. pickup(K)
    (allow-pickup step4 K)
    ;; 6. putdown(K)
    (allow-putdown step5 K)
    ;; 7. pickup(K)
    (allow-pickup step6 K)
    ;; 8. stack(K, G)
    (allow-stack step7 K G)
    ;; 9. unstack(L, N)
    (allow-unstack step8 L N)
    ;; 10. stack(L, K)
    (allow-stack step9 L K)
    ;; 11. unstack(L, K)
    (allow-unstack step10 L K)
    ;; 12. stack(L, N)
    (allow-stack step11 L N)
    ;; 13. unstack(L, N)
    (allow-unstack step12 L N)
    ;; 14. stack(L, N)
    (allow-stack step13 L N)
    ;; 15. unstack(K, G)
    (allow-unstack step14 K G)
    ;; 16. stack(K, G)
    (allow-stack step15 K G)
    ;; 17. unstack(K, G)
    (allow-unstack step16 K G)
    ;; 18. putdown(K)
    (allow-putdown step17 K)
    ;; 19. pickup(K)
    (allow-pickup step18 K)
    ;; 20. stack(K, L)
    (allow-stack step19 K L)
    ;; 21. unstack(K, L)
    (allow-unstack step20 K L)
    ;; 22. putdown(K)
