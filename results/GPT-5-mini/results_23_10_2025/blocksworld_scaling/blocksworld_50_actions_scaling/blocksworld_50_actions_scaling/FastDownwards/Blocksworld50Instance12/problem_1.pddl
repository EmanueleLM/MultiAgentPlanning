    (allow-unstack step22 L N)
    ;; 24. stack(L, N)
    (allow-stack step23 L N)
    ;; 25. unstack(G, D)
    (allow-unstack step24 G D)
    ;; 26. stack(G, D)
    (allow-stack step25 G D)
    ;; 27. pickup(K)
    (allow-pickup step26 K)
    ;; 28. putdown(K)
    (allow-putdown step27 K)
    ;; 29. unstack(G, D)
    (allow-unstack step28 G D)
    ;; 30. stack(G, D)
    (allow-stack step29 G D)
    ;; 31. unstack(G, D)
    (allow-unstack step30 G D)
    ;; 32. stack(G, K)
    (allow-stack step31 G K)
    ;; 33. unstack(L, N)
    (allow-unstack step32 L N)
    ;; 34. putdown(L)
    (allow-putdown step33 L)
    ;; 35. unstack(D, C)
    (allow-unstack step34 D C)
    ;; 36. stack(D, G)
    (allow-stack step35 D G)
    ;; 37. pickup(L)
    (allow-pickup step36 L)
    ;; 38. stack(L, C)
    (allow-stack step37 L C)
    ;; 39. unstack(L, C)
    (allow-unstack step38 L C)
    ;; 40. stack(L, C)
    (allow-stack step39 L C)
    ;; 41. unstack(N, B)
    (allow-unstack step40 N B)
