    ;; 49. unstack(N, L)
    (allow-unstack step48 N L)
    ;; 50. stack(N, D)
    (allow-stack step49 N D)
  )

  ;; Goal is to reach completion of the provided action sequence (step50).
  ;; Note: the final arrangement is not explicitly stated in the goal;
  ;; reaching step50 requires performing the exact allowed moves in order.
  (:goal (current-step step50))
)