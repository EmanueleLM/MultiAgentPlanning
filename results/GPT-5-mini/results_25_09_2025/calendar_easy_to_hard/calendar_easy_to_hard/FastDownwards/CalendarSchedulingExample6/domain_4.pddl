(define (domain meeting-scheduling)
  (:requirements :strips :typing :negative-preconditions)
  (:types person slot)
  (:predicates
    (available ?p - person ?s - slot)
    (next ?s1 - slot ?s2 - slot)
    (meeting-scheduled)
    (meeting-start ?s - slot)
  )

  (:action thomas_schedule
    :parameters (?t - person ?d - person ?j - person ?s - slot ?s2 - slot)
    :precondition (and
      (not (meeting-scheduled))
      (next ?s ?s2)
      (available ?t ?s)
      (available ?t ?s2)
      (available ?d ?s)
      (available ?d ?s2)
      (available ?j ?s)
      (available ?j ?s2)
    )
    :effect (and (meeting-scheduled) (meeting-start ?s))
  )

  (:action dylan_schedule
    :parameters (?t - person ?d - person ?j - person ?s - slot ?s2 - slot)
    :precondition (and
      (not (meeting-scheduled))
      (next ?s ?s2)
      (available ?t ?s)
      (available ?t ?s2)
      (available ?d ?s)
      (available ?d ?s2)
      (available ?j ?s)
      (available ?j ?s2)
    )
    :effect (and (meeting-scheduled) (meeting-start ?s))
  )

  (:action jerry_schedule
    :parameters (?t - person ?d - person ?j - person ?s - slot ?s2 - slot)
    :precondition (and
      (not (meeting-scheduled))
      (next ?s ?s2)
      (available ?t ?s)
      (available ?t ?s2)
      (available ?d ?s)
      (available ?d ?s2)
      (available ?j ?s)
      (available ?j ?s2)
    )
    :effect (and (meeting-scheduled) (meeting-start ?s))
  )
)