(define (domain meeting-scheduling)
  (:requirements :strips :typing :negative-preconditions :equality)
  (:types person slot)
  (:predicates
    (participant ?p - person)
    (slot ?s - slot)
    (free-slot ?p - person ?s - slot)
    (meeting-scheduled)
    (scheduled-slot ?s - slot)
    (meeting-with ?p - person)
  )
  (:action schedule-meeting
    :parameters (?s - slot ?d - person ?j - person ?e - person ?p - person)
    :precondition (and
      (slot ?s)
      (participant ?d) (participant ?j) (participant ?e) (participant ?p)
      (free-slot ?d ?s) (free-slot ?j ?s) (free-slot ?e ?s) (free-slot ?p ?s)
      (not (meeting-scheduled))
      (not (= ?d ?j)) (not (= ?d ?e)) (not (= ?d ?p))
      (not (= ?j ?e)) (not (= ?j ?p))
      (not (= ?e ?p))
    )
    :effect (and
      (meeting-scheduled)
      (scheduled-slot ?s)
      (meeting-with ?d) (meeting-with ?j) (meeting-with ?e) (meeting-with ?p)
    )
  )
)