(define (domain meeting-scheduling)
  (:requirements :strips :typing)
  (:types person time)
  (:predicates
    (free ?p - person ?t - time)
    (unscheduled)
    (scheduled)
    (meeting-at ?t - time)
  )
  (:action schedule-at
    :parameters (?t - time)
    :precondition (and
      (unscheduled)
      (free shirley ?t)
      (free jacob ?t)
      (free stephen ?t)
      (free margaret ?t)
      (free mason ?t)
    )
    :effect (and
      (scheduled)
      (not (unscheduled))
      (meeting-at ?t)
      (not (free shirley ?t))
      (not (free jacob ?t))
      (not (free stephen ?t))
      (not (free margaret ?t))
      (not (free mason ?t))
    )
  )
)