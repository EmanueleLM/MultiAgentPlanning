(define (domain meeting-scheduling)
  (:requirements :strips :typing :negative-preconditions :action-costs :fluents :adl)
  (:types person timeslot)
  (:predicates
    (slot ?t - timeslot)
    (available ?p - person ?t - timeslot)
    (meeting-scheduled)
    (meeting-start ?t - timeslot)
    (attending ?p - person ?t - timeslot)
    (is-s09_00 ?t - timeslot)
    (is-s09_30 ?t - timeslot)
    (is-s10_00 ?t - timeslot)
    (is-s10_30 ?t - timeslot)
    (is-s11_00 ?t - timeslot)
    (is-s11_30 ?t - timeslot)
    (is-s12_00 ?t - timeslot)
    (is-s12_30 ?t - timeslot)
    (is-s13_00 ?t - timeslot)
    (is-s13_30 ?t - timeslot)
    (is-s14_00 ?t - timeslot)
    (is-s14_30 ?t - timeslot)
    (is-s15_00 ?t - timeslot)
    (is-s15_30 ?t - timeslot)
    (is-s16_00 ?t - timeslot)
    (is-s16_30 ?t - timeslot)
  )
  (:functions (total-cost))
  (:action schedule-meeting
    :parameters (?t - timeslot ?d - person ?h - person ?a - person ?e - person ?b - person ?deb - person)
    :precondition (and
      (slot ?t)
      (not (meeting-scheduled))
      (available ?d ?t)
      (available ?h ?t)
      (available ?a ?t)
      (available ?e ?t)
      (available ?b ?t)
      (available ?deb ?t)
    )
    :effect (and
      (meeting-scheduled)
      (meeting-start ?t)
      (attending ?d ?t)
      (attending ?h ?t)
      (attending ?a ?t)
      (attending ?e ?t)
      (attending ?b ?t)
      (attending ?deb ?t)
      (when (is-s09_00 ?t) (increase (total-cost) 0))
      (when (is-s09_30 ?t) (increase (total-cost) 1))
      (when (is-s10_00 ?t) (increase (total-cost) 2))
      (when (is-s10_30 ?t) (increase (total-cost) 3))
      (when (is-s11_00 ?t) (increase (total-cost) 4))
      (when (is-s11_30 ?t) (increase (total-cost) 5))
      (when (is-s12_00 ?t) (increase (total-cost) 6))
      (when (is-s12_30 ?t) (increase (total-cost) 7))
      (when (is-s13_00 ?t) (increase (total-cost) 8))
      (when (is-s13_30 ?t) (increase (total-cost) 9))
      (when (is-s14_00 ?t) (increase (total-cost) 10))
      (when (is-s14_30 ?t) (increase (total-cost) 11))
      (when (is-s15_00 ?t) (increase (total-cost) 12))
      (when (is-s15_30 ?t) (increase (total-cost) 13))
      (when (is-s16_00 ?t) (increase (total-cost) 14))
      (when (is-s16_30 ?t) (increase (total-cost) 15))
    )
  )
)