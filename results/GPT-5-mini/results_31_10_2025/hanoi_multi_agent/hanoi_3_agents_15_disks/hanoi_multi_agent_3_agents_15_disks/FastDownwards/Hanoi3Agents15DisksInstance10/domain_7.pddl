(define (domain multiagent-hanoi-3agents)
  (:requirements :typing :negative-preconditions)
  (:types disk peg agent)

  (:predicates
    (onpeg ?d - disk ?p - peg)
    (below-disk ?d - disk ?d2 - disk)
    (below-peg ?d - disk ?p - peg)
    (topdisk ?d - disk)
    (empty ?p - peg)
    (canmove ?a - agent ?d - disk)
    (smaller ?d1 - disk ?d2 - disk)
  )

  (:action agent1-move-disk-to-disk
    :parameters (?a - agent ?d - disk ?below - disk ?from - peg ?t - disk ?to - peg)
    :precondition (and
      (canmove ?a ?d)
      (topdisk ?d)
      (below-disk ?d ?below)
      (onpeg ?d ?from)
      (topdisk ?t)
      (onpeg ?t ?to)
      (smaller ?d ?t)
    )
    :effect (and
      (not (topdisk ?d))
      (not (below-disk ?d ?below))
      (below-disk ?d ?t)
      (not (topdisk ?t))
      (topdisk ?below)
      (not (onpeg ?d ?from))
      (onpeg ?d ?to)
      (not (empty ?to))
    )
  )

  (:action agent1-move-disk-to-empty
    :parameters (?a - agent ?d - disk ?below - disk ?from - peg ?to - peg)
    :precondition (and
      (canmove ?a ?d)
      (topdisk ?d)
      (below-disk ?d ?below)
      (onpeg ?d ?from)
      (empty ?to)
    )
    :effect (and
      (not (topdisk ?d))
      (not (below-disk ?d ?below))
      (below-peg ?d ?to)
      (topdisk ?below)
      (not (onpeg ?d ?from))
      (onpeg ?d ?to)
      (not (empty ?to))
    )
  )

  (:action agent1-move-peg-to-disk
    :parameters (?a - agent ?d - disk ?from - peg ?t - disk ?to - peg)
    :precondition (and
      (canmove ?a ?d)
      (topdisk ?d)
      (below-peg ?d ?from)
      (onpeg ?d ?from)
      (topdisk ?t)
      (onpeg ?t ?to)
      (smaller ?d ?t)
    )
    :effect (and
      (not (topdisk ?d))
      (not (below-peg ?d ?from))
      (below-disk ?d ?t)
      (not (topdisk ?t))
      (not (onpeg ?d ?from))
      (onpeg ?d ?to)
      (empty ?from)
    )
  )

  (:action agent1-move-peg-to-empty
