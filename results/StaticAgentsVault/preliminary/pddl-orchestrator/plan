(open-vault agent-a vault)
(:action open-vault
    :parameters (agent-a vault)
    :precondition (and
        (closed vault)
        (has-key agent-a)
    )
    :effect (and
        (open vault)
        (not
            (closed vault)
        )
    )
)

(enter-vault agent-b vault)

(:action enter-vault
    :parameters (agent-b vault)
    :precondition (and
        (open vault)
        (small agent-b)
        (at agent-b outside)
        (not
            (at agent-b vault)
        )
    )
    :effect (and
        (at agent-b vault)
        (not
            (at agent-b outside)
        )
    )
)

(grab-object agent-b obj1 vault)

(:action grab-object
    :parameters (agent-b obj1 vault)
    :precondition (and
        (at agent-b vault)
        (in obj1 vault)
    )
    :effect (and
        (has-object agent-b)
        (not
            (in obj1 vault)
        )
    )
)