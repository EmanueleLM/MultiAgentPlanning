(define (problem multi-agent-blocks-problem)
  (:domain blocks-multi-agent)
  (:objects
    a e i - vowel
    b c d f g h j - consonant
  )
  (:init
    (ontable c)
    (on i c)
    (on f i)
    (on j f)
    (ontable b)
    (on d b)
    (on a d)
    (on e a)
    (on g e)
    (on h g)
    (clear j)
    (clear h)
    (handempty-vowel)
    (handempty-cons)
  )
  (:goal (and
    (on a b)
    (on b c)
    (on c d)
    (on d e)
    (on e f)
    (on f g)
    (on g h)
    (on h i)
    (on i j)
    (ontable j)
  ))
)