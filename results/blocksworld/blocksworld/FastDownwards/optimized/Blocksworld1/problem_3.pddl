(define (problem multi-agent-blocks-problem)
  (:domain multi-agent-blocks)
  (:objects A B C D E F G H I J K L M N - block)

  (:init
    (= (total-cost) 0)

    (ontable M)
    (ontable C)

    (on B C)
    (on G B)
    (on J G)
    (on I J)
    (on D I)
    (on A D)
    (on H A)

    (on E M)
    (on N E)
    (on L N)
    (on F L)
    (on K F)

    (clear K)
    (clear H)

    (handempty-vowel)
    (handempty-cons)

    (vowel A)
    (vowel E)
    (vowel I)

    (consonant B)
    (consonant C)
    (consonant D)
    (consonant F)
    (consonant G)
    (consonant H)
    (consonant J)
    (consonant K)
    (consonant L)
    (consonant M)
    (consonant N)
  )

  (:goal (and
    (on A B)
    (on B C)
    (on C D)
    (on D E)
    (on E F)
    (on F G)
    (on G H)
    (on H I)
    (on I J)
    (on J K)
    (on K L)
    (on L M)
    (on M N)
    (ontable N)
  ))

  (:metric minimize (total-cost))
)