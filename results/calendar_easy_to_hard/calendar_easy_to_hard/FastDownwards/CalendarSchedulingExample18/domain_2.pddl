(define (domain meeting-scheduling)
  (:requirements :typing)
  (:types participant slot)
  (:predicates
    (is-participant ?p - participant)
    (is-slot ?s - slot)
    (available ?p - participant ?s - slot)
    (earliest-allowed ?s - slot)
    (scheduled ?s - slot)
    (meeting-scheduled)
  )
  (:action schedule-slot
    :parameters (?s - slot ?p1 - participant ?p2 - participant ?p3 - participant ?p4 - participant)
    :precondition (and
      (is-slot ?s)
      (earliest-allowed ?s)
      (not (meeting-scheduled))
      (available ?p1 ?s)
      (available ?p2 ?s)
      (available ?p3 ?s)
      (available ?p4 ?s)
    )
    :effect (and
      (meeting-scheduled)
      (scheduled ?s)
    )
  )
)