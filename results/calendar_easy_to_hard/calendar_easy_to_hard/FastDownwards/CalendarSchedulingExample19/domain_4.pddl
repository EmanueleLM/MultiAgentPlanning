(define (domain meeting-scheduling)
  (:requirements :strips :typing :conditional-effects :quantified-preconditions :fluents :action-costs)
  (:types agent slot)
  (:constants marie janice elijah theresa - agent)

  (:predicates
    (available ?a - agent ?s - slot)
    (meeting-scheduled ?s - slot)
    (unscheduled)
    (slot-cost-0 ?s - slot)
    (slot-cost-1 ?s - slot)
    (slot-cost-2 ?s - slot)
    (slot-cost-3 ?s - slot)
    (slot-cost-4 ?s - slot)
    (slot-cost-5 ?s - slot)
    (slot-cost-6 ?s - slot)
    (slot-cost-7 ?s - slot)
    (slot-cost-8 ?s - slot)
    (slot-cost-9 ?s - slot)
    (slot-cost-10 ?s - slot)
    (slot-cost-11 ?s - slot)
    (slot-cost-12 ?s - slot)
    (slot-cost-13 ?s - slot)
    (slot-cost-14 ?s - slot)
    (slot-cost-15 ?s - slot)
  )

  (:functions (total-cost))

  (:action schedule-meeting
    :parameters (?s - slot)
    :precondition (and
      (unscheduled)
      (available marie ?s)
      (available janice ?s)
      (available elijah ?s)
      (available theresa ?s)
    )
    :effect (and
      (meeting-scheduled ?s)
      (not (unscheduled))
      (when (slot-cost-0 ?s) (increase (total-cost) 0))
      (when (slot-cost-1 ?s) (increase (total-cost) 1))
      (when (slot-cost-2 ?s) (increase (total-cost) 2))
      (when (slot-cost-3 ?s) (increase (total-cost) 3))
      (when (slot-cost-4 ?s) (increase (total-cost) 4))
      (when (slot-cost-5 ?s) (increase (total-cost) 5))
      (when (slot-cost-6 ?s) (increase (total-cost) 6))
      (when (slot-cost-7 ?s) (increase (total-cost) 7))
      (when (slot-cost-8 ?s) (increase (total-cost) 8))
      (when (slot-cost-9 ?s) (increase (total-cost) 9))
      (when (slot-cost-10 ?s) (increase (total-cost) 10))
      (when (slot-cost-11 ?s) (increase (total-cost) 11))
      (when (slot-cost-12 ?s) (increase (total-cost) 12))
      (when (slot-cost-13 ?s) (increase (total-cost) 13))
      (when (slot-cost-14 ?s) (increase (total-cost) 14))
      (when (slot-cost-15 ?s) (increase (total-cost) 15))
    )
  )
)