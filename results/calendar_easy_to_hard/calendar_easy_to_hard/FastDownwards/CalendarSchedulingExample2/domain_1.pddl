(define (domain meeting-scheduling)
  (:requirements :typing :negative-preconditions)
  (:types person slot)
  (:predicates
    (available ?p - person ?s - slot)
    (proposed ?p - person ?s - slot)
    (amy_pref_slot ?s - slot)
    (unscheduled)
    (scheduled)
    (scheduled-at ?s - slot)
  )
  (:action propose-roy
    :parameters (?s - slot)
    :precondition (and (available roy ?s) (not (proposed roy ?s)))
    :effect (proposed roy ?s)
  )
  (:action propose-kathryn
    :parameters (?s - slot)
    :precondition (and (available kathryn ?s) (not (proposed kathryn ?s)))
    :effect (proposed kathryn ?s)
  )
  (:action propose-amy
    :parameters (?s - slot)
    :precondition (and (available amy ?s) (amy_pref_slot ?s) (not (proposed amy ?s)))
    :effect (proposed amy ?s)
  )
  (:action orchestrator-finalize
    :parameters (?s - slot)
    :precondition (and (unscheduled) (proposed roy ?s) (proposed kathryn ?s) (proposed amy ?s))
    :effect (and (scheduled) (scheduled-at ?s) (not (unscheduled)))
  )
)