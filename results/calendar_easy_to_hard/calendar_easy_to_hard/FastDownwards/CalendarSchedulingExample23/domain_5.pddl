(define (domain meeting-scheduling)
  (:requirements :typing :adl :fluents :action-costs :negative-preconditions :equality)
  (:types participant slot meeting)
  (:predicates
    (free ?p - participant ?s - slot)
    (assigned ?m - meeting)
    (scheduled ?m - meeting ?s - slot)
    (cost1 ?s - slot)
    (cost2 ?s - slot)
    (cost3 ?s - slot)
    (cost4 ?s - slot)
    (cost5 ?s - slot)
    (cost6 ?s - slot)
    (cost7 ?s - slot)
    (cost8 ?s - slot)
    (cost9 ?s - slot)
    (cost10 ?s - slot)
    (cost11 ?s - slot)
    (cost12 ?s - slot)
    (cost13 ?s - slot)
    (cost14 ?s - slot)
    (cost15 ?s - slot)
    (cost16 ?s - slot)
  )
  (:functions (total-cost))
  (:action schedule
    :parameters (?m - meeting ?s - slot ?p1 - participant ?p2 - participant ?p3 - participant ?p4 - participant ?p5 - participant)
    :precondition (and
      (not (assigned ?m))
      (free ?p1 ?s) (free ?p2 ?s) (free ?p3 ?s) (free ?p4 ?s) (free ?p5 ?s)
      (not (= ?p1 ?p2)) (not (= ?p1 ?p3)) (not (= ?p1 ?p4)) (not (= ?p1 ?p5))
      (not (= ?p2 ?p3)) (not (= ?p2 ?p4)) (not (= ?p2 ?p5))
      (not (= ?p3 ?p4)) (not (= ?p3 ?p5))
      (not (= ?p4 ?p5))
    )
    :effect (and
      (assigned ?m)
      (scheduled ?m ?s)
      (not (free ?p1 ?s)) (not (free ?p2 ?s)) (not (free ?p3 ?s)) (not (free ?p4 ?s)) (not (free ?p5 ?s))
      (when (cost1 ?s)   (increase (total-cost) 1))
      (when (cost2 ?s)   (increase (total-cost) 2))
      (when (cost3 ?s)   (increase (total-cost) 3))
      (when (cost4 ?s)   (increase (total-cost) 4))
      (when (cost5 ?s)   (increase (total-cost) 5))
      (when (cost6 ?s)   (increase (total-cost) 6))
      (when (cost7 ?s)   (increase (total-cost) 7))
      (when (cost8 ?s)   (increase (total-cost) 8))
      (when (cost9 ?s)   (increase (total-cost) 9))
      (when (cost10 ?s)  (increase (total-cost) 10))
      (when (cost11 ?s)  (increase (total-cost) 11))
      (when (cost12 ?s)  (increase (total-cost) 12))
      (when (cost13 ?s)  (increase (total-cost) 13))
      (when (cost14 ?s)  (increase (total-cost) 14))
      (when (cost15 ?s)  (increase (total-cost) 15))
      (when (cost16 ?s)  (increase (total-cost) 16))
    )
  )
)