(define (domain calendar-scheduling)
  (:requirements :typing :negative-preconditions)
  (:types agent time)
  (:constants julia joshua nicholas david melissa - agent)
  (:predicates
    (free ?a - agent ?t - time)
    (scheduled)
  )
  (:action schedule
    :parameters (?t - time)
    :precondition (and
      (free julia ?t)
      (free joshua ?t)
      (free nicholas ?t)
      (free david ?t)
      (free melissa ?t)
      (not (scheduled))
    )
    :effect (and (scheduled))
  )
)