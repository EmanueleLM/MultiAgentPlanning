(define (domain meeting-scheduling)
  (:requirements :strips :negative-preconditions :typing)
  (:types participant slot)
  (:predicates
    (free ?p - participant ?s - slot)       ; participant ?p is free at slot ?s
    (all-free ?s - slot)                   ; every participant is free at slot ?s
    (scheduled ?s - slot)                  ; meeting has been scheduled at slot ?s
    (meeting-scheduled)                    ; meeting has been scheduled (global goal)
  )

  ;; Earliest-first scheduling is enforced by making each schedule action
  ;; require that no earlier slot is fully free. There is one action per slot.
  ;; Each action requires (all-free slot-X) and requires that every earlier
  ;; slot does NOT have (all-free earlier-slot).
  ;; Effects mark the meeting scheduled and clear per-participant frees for that slot.

  (:action schedule-0900
    :precondition (and (all-free slot-0900))
    :effect (and (meeting-scheduled)
                 (scheduled slot-0900)
                 ;; consume frees for this slot
                 (not (all-free slot-0900))
    )
  )

  (:action schedule-0930
    :precondition (and (all-free slot-0930)
                       (not (all-free slot-0900)))
    :effect (and (meeting-scheduled)
                 (scheduled slot-0930)
                 (not (all-free slot-0930))
    )
  )

  (:action schedule-1000
    :precondition (and (all-free slot-1000)
                       (not (all-free slot-0900))
                       (not (all-free slot-0930)))
    :effect (and (meeting-scheduled)
                 (scheduled slot-1000)
                 (not (all-free slot-1000))
    )
  )

  (:action schedule-1030
