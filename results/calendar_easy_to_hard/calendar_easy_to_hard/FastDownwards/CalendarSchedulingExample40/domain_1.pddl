(define (domain meeting-scheduling)
  (:requirements :typing :adl :universal-preconditions :negative-preconditions :action-costs :fluents)
  (:types participant slot)
  (:predicates
    (busy ?p - participant ?s - slot)
    (feasible ?s - slot)
    (assigned ?s - slot)
    (meeting-scheduled)
    (before ?s1 - slot ?s2 - slot)
    (rank1 ?s - slot) (rank2 ?s - slot) (rank3 ?s - slot) (rank4 ?s - slot)
    (rank5 ?s - slot) (rank6 ?s - slot) (rank7 ?s - slot) (rank8 ?s - slot)
    (rank9 ?s - slot) (rank10 ?s - slot) (rank11 ?s - slot) (rank12 ?s - slot)
    (rank13 ?s - slot) (rank14 ?s - slot) (rank15 ?s - slot) (rank16 ?s - slot)
  )
  (:functions (total-cost))
  (:action identify-feasible
    :parameters (?s - slot)
    :precondition (and
      (not (feasible ?s))
      (forall (?p - participant) (not (busy ?p ?s)))
    )
    :effect (and
      (feasible ?s)
      (when (rank1 ?s) (increase (total-cost) 0))
      (when (rank2 ?s) (increase (total-cost) 1))
      (when (rank3 ?s) (increase (total-cost) 2))
      (when (rank4 ?s) (increase (total-cost) 3))
      (when (rank5 ?s) (increase (total-cost) 4))
      (when (rank6 ?s) (increase (total-cost) 5))
      (when (rank7 ?s) (increase (total-cost) 6))
      (when (rank8 ?s) (increase (total-cost) 7))
      (when (rank9 ?s) (increase (total-cost) 8))
      (when (rank10 ?s) (increase (total-cost) 9))
      (when (rank11 ?s) (increase (total-cost) 10))
      (when (rank12 ?s) (increase (total-cost) 11))
      (when (rank13 ?s) (increase (total-cost) 12))
      (when (rank14 ?s) (increase (total-cost) 13))
      (when (rank15 ?s) (increase (total-cost) 14))
      (when (rank16 ?s) (increase (total-cost) 15))
    )
  )
  (:action assign-meeting
    :parameters (?s - slot)
    :precondition (and
      (feasible ?s)
      (not (meeting-scheduled))
      (forall (?t - slot) (or (not (before ?t ?s)) (not (feasible ?t))))
    )
    :effect (and (meeting-scheduled) (assigned ?s))
  )
)