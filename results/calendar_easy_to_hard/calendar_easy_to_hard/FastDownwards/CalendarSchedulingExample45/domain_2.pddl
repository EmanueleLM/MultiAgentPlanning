(define (domain calendar_scheduling_example45)
  (:requirements :strips :typing)
  (:types person slot)
  (:constants emily brian gerald julia logan judith michael - person)
  (:predicates
    (available ?p - person ?s - slot)
    (not_scheduled)
    (meeting_at ?s - slot)
  )
  (:action schedule
    :parameters (?s - slot)
    :precondition (and
      (not_scheduled)
      (available emily ?s)
      (available brian ?s)
      (available gerald ?s)
      (available julia ?s)
      (available logan ?s)
      (available judith ?s)
      (available michael ?s)
    )
    :effect (and
      (meeting_at ?s)
      (not (not_scheduled))
    )
  )
)