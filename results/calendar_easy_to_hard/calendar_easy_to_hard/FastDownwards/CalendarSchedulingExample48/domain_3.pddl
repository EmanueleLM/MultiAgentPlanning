(define (domain calendar-scheduling)
  (:requirements :strips :typing)
  (:types participant timeslot)
  (:predicates
    (is-timeslot ?t - timeslot)
    (is-participant ?p - participant)
    (available ?p - participant ?t - timeslot)
    (meeting-duration-30)
    (scheduled ?t - timeslot)
  )
  (:action schedule-meeting
    :parameters (?t - timeslot)
    :precondition (and
      (is-timeslot ?t)
      (available noah ?t)
      (available teresa ?t)
      (available bradley ?t)
      (available philip ?t)
      (available joyce ?t)
      (available ryan ?t)
      (available aaron ?t)
      (meeting-duration-30)
    )
    :effect (and (scheduled ?t))
  )
)