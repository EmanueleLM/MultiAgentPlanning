(define (domain schedule-meeting)
  (:requirements :typing :strips)
  (:types person hour meeting)

  (:constants dylan kathryn hannah anna - person)

  (:predicates
    (free_hour ?p - person ?h - hour)
    (unscheduled ?m - meeting)
    (meeting-free)
    (meeting-scheduled-global ?m - meeting)
    (scheduled-at ?m - meeting ?h - hour)
    (attends ?p - person ?m - meeting)
    (within-window ?h - hour)
  )

  (:action schedule-by-dylan
    :parameters (?m - meeting ?h - hour)
    :precondition (and
      (unscheduled ?m)
      (meeting-free)
      (within-window ?h)
      (free_hour dylan ?h)
      (free_hour kathryn ?h)
      (free_hour hannah ?h)
      (free_hour anna ?h)
    )
    :effect (and
      (meeting-scheduled-global ?m)
      (scheduled-at ?m ?h)
      (attends dylan ?m) (attends kathryn ?m) (attends hannah ?m) (attends anna ?m)
      (not (unscheduled ?m))
      (not (meeting-free))
      (not (free_hour dylan ?h)) (not (free_hour kathryn ?h)) (not (free_hour hannah ?h)) (not (free_hour anna ?h))
    )
  )

  (:action schedule-by-kathryn
