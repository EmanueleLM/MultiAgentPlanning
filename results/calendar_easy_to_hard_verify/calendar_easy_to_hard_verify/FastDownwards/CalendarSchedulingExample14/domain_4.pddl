(define (domain meeting-scheduling)
  (:requirements :typing)
  (:types agent timeslot)
  (:constants karen brandon donald kelly orchestrator - agent)
  (:predicates
    (available ?a - agent ?t - timeslot)
    (attending ?a - agent ?t - timeslot)
    (meeting-scheduled ?t - timeslot)
  )
  (:action attend-karen
    :parameters (?t - timeslot)
    :precondition (available karen ?t)
    :effect (attending karen ?t)
  )
  (:action attend-brandon
    :parameters (?t - timeslot)
    :precondition (available brandon ?t)
    :effect (attending brandon ?t)
  )
  (:action attend-donald
    :parameters (?t - timeslot)
    :precondition (available donald ?t)
    :effect (attending donald ?t)
  )
  (:action attend-kelly
    :parameters (?t - timeslot)
    :precondition (available kelly ?t)
    :effect (attending kelly ?t)
  )
  (:action finalize-meeting
    :parameters (?t - timeslot)
    :precondition (and
      (attending karen ?t)
      (attending brandon ?t)
      (attending donald ?t)
      (attending kelly ?t)
    )
    :effect (meeting-scheduled ?t)
  )
)