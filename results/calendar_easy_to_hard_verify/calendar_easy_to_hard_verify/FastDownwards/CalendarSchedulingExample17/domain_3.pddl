(define (domain multi-agent-meeting)
  (:requirements :typing :negative-preconditions :action-costs)
  (:types person slot)
  (:constants peter judith keith evelyn - person)
  (:predicates
    (busy ?p - person ?t - slot)
    (scheduled ?t - slot)
    (meeting_scheduled)
  )
  (:functions (total-cost))
  (:action schedule
    :parameters (?t - slot)
    :precondition (and
      (not (meeting_scheduled))
      (not (busy peter ?t))
      (not (busy judith ?t))
      (not (busy keith ?t))
      (not (busy evelyn ?t))
    )
    :effect (and
      (meeting_scheduled)
      (scheduled ?t)
      (increase (total-cost) 1)
    )
  )
)