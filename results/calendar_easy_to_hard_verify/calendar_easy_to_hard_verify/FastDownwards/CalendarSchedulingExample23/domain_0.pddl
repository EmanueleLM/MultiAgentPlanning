(define (domain meeting-scheduling)
  (:requirements :strips :negative-preconditions :typing)
  (:types participant slot)
  (:predicates
    (busy ?p - participant ?s - slot)
    (meeting-scheduled)
    (meeting-at ?s - slot)
    (attends ?p - participant ?s - slot)
  )
  (:action schedule
    :parameters (?s - slot)
    :precondition (and
      (not (meeting-scheduled))
      (not (busy elijah ?s))
      (not (busy janet ?s))
      (not (busy brian ?s))
      (not (busy carl ?s))
      (not (busy timothy ?s))
    )
    :effect (and
      (meeting-scheduled)
      (meeting-at ?s)
      (attends elijah ?s)
      (attends janet ?s)
      (attends brian ?s)
      (attends carl ?s)
      (attends timothy ?s)
    )
  )
)