(define (domain meeting-scheduling)
  (:requirements :typing :negative-preconditions :action-costs)
  (:types agent timeslot)
  (:predicates
    (attendee ?a - agent)
    (free ?a - agent ?s - timeslot)
    (scheduled ?s - timeslot)
    (meeting-scheduled)
  )
  (:action schedule
    :parameters (?s - timeslot)
    :precondition (and
      (free jesse ?s)
      (free nancy ?s)
      (free isabella ?s)
      (free harold ?s)
      (free linda ?s)
    )
    :effect (and
      (scheduled ?s)
      (meeting-scheduled)
      (not (free jesse ?s))
      (not (free nancy ?s))
      (not (free isabella ?s))
      (not (free harold ?s))
      (not (free linda ?s))
    )
  )
)