(define (domain meeting-scheduling)
  (:requirements :typing :negative-preconditions :action-costs)
  (:types agent timeslot)
  (:predicates
    (attendee ?a - agent)
    (free ?a - agent ?s - timeslot)
    (scheduled ?s - timeslot)
    (meeting-scheduled)
  )
  (:action schedule
    :parameters (?s - timeslot ?j - agent ?n - agent ?i - agent ?h - agent ?l - agent)
    :precondition (and
      (free ?j ?s)
      (free ?n ?s)
      (free ?i ?s)
      (free ?h ?s)
      (free ?l ?s)
    )
    :effect (and
      (scheduled ?s)
      (meeting-scheduled)
      (not (free ?j ?s))
      (not (free ?n ?s))
      (not (free ?i ?s))
      (not (free ?h ?s))
      (not (free ?l ?s))
    )
  )
)