(define (domain calendar-scheduling)
  (:requirements :typing :negative-preconditions :action-costs)
  (:types person slot)
  (:constants alice daniel scott lori cynthia jeffrey hannah - person)
  (:predicates
    (free ?p - person ?s - slot)
    (meeting-scheduled)
    (attend ?p - person ?s - slot)
  )
  (:action schedule
    :parameters (?s - slot)
    :precondition (and
      (not (meeting-scheduled))
      (free alice ?s)
      (free daniel ?s)
      (free scott ?s)
      (free lori ?s)
      (free cynthia ?s)
      (free jeffrey ?s)
      (free hannah ?s)
    )
    :effect (and
      (meeting-scheduled)
      (attend alice ?s)
      (attend daniel ?s)
      (attend scott ?s)
      (attend lori ?s)
      (attend cynthia ?s)
      (attend jeffrey ?s)
      (attend hannah ?s)
    )
  )
)