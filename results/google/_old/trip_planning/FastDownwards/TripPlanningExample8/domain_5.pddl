(define (domain trip-multiagent)
  (:requirements :strips :typing)
  (:types city day)
  (:predicates
    (in-krakow ?d - day)
    (in-athens ?d - day)
    (in-zurich ?d - day)
    (unassigned ?d - day)
    (allowed-krakow ?d - day)
    (allowed-athens ?d - day)
    (allowed-zurich ?d - day)
    (allowed-relatives ?d - day)
    (direct-flight ?from - city ?to - city)
    (flew ?from - city ?to - city ?d - day)
    (visited-relatives ?d - day)
  )
  (:action krakow-agent-occupy-day
    :parameters (?d - day)
    :precondition (and (unassigned ?d) (allowed-krakow ?d))
    :effect (and (in-krakow ?d) (not (unassigned ?d)))
  )
  (:action athens-agent-occupy-day
    :parameters (?d - day)
    :precondition (and (unassigned ?d) (allowed-athens ?d))
    :effect (and (in-athens ?d) (not (unassigned ?d)))
  )
  (:action zurich-agent-occupy-day
    :parameters (?d - day)
    :precondition (and (unassigned ?d) (allowed-zurich ?d))
    :effect (and (in-zurich ?d) (not (unassigned ?d)))
  )
  (:action krakow-to-zurich-fly
    :parameters (?d - day)
    :precondition (and (in-krakow ?d) (direct-flight krakow zurich))
    :effect (flew krakow zurich ?d)
  )
  (:action zurich-to-krakow-fly
    :parameters (?d - day)
    :precondition (and (in-zurich ?d) (direct-flight zurich krakow))
    :effect (flew zurich krakow ?d)
  )
  (:action athens-to-zurich-fly
    :parameters (?d - day)
    :precondition (and (in-athens ?d) (direct-flight athens zurich))
    :effect (flew athens zurich ?d)
  )
  (:action zurich-to-athens-fly
    :parameters (?d - day)
    :precondition (and (in-zurich ?d) (direct-flight zurich athens))
    :effect (flew zurich athens ?d)
  )
  (:action athens-agent-visit-relatives
    :parameters (?d - day)
    :precondition (and (in-athens ?d) (allowed-relatives ?d))
    :effect (visited-relatives ?d)
  )
)