(define (domain calendar_scheduling)
  (:requirements :strips :negative-preconditions)
  (:constants olivia anna virginia paul)
  (:predicates
    (slot ?s)
    (next ?s ?s2)
    (free ?p ?s)
    (meeting_scheduled)
    (meeting_start ?s)
  )
  (:action schedule
    :parameters (?s ?s2)
    :precondition (and
      (slot ?s)
      (next ?s ?s2)
      (not (meeting_scheduled))
      (free olivia ?s) (free anna ?s) (free virginia ?s) (free paul ?s)
      (free olivia ?s2) (free anna ?s2) (free virginia ?s2) (free paul ?s2)
    )
    :effect (and (meeting_scheduled) (meeting_start ?s))
  )
)