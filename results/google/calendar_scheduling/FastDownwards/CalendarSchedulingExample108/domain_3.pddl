(define (domain calendar-scheduling)
  (:requirements :strips :typing)
  (:types person slot)
  (:constants lisa dorothy anthony ryan orchestrator - person)
  (:predicates
    (available ?p - person ?s - slot)
    (reserved ?p - person ?s - slot)
    (scheduled ?s - slot)
  )
  (:action attend-lisa
    :parameters (?s - slot)
    :precondition (and (available lisa ?s))
    :effect (and (reserved lisa ?s))
  )
  (:action attend-dorothy
    :parameters (?s - slot)
    :precondition (and (available dorothy ?s))
    :effect (and (reserved dorothy ?s))
  )
  (:action attend-anthony
    :parameters (?s - slot)
    :precondition (and (available anthony ?s))
    :effect (and (reserved anthony ?s))
  )
  (:action attend-ryan
    :parameters (?s - slot)
    :precondition (and (available ryan ?s))
    :effect (and (reserved ryan ?s))
  )
  (:action finalize-meeting
    :parameters (?s - slot)
    :precondition (and
      (reserved lisa ?s)
      (reserved dorothy ?s)
      (reserved anthony ?s)
      (reserved ryan ?s)
    )
    :effect (and (scheduled ?s))
  )
)