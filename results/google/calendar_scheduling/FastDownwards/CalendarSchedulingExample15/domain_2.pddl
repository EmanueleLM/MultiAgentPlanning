(define (domain meeting-scheduling)
  (:requirements :strips :typing :equality :negative-preconditions)
  (:types orchestrator participant slot)
  (:predicates
    (available ?p - participant ?s - slot)
    (meeting-scheduled ?s - slot)
    (is-orchestrator ?o - orchestrator)
  )
  (:action schedule-meeting
    :parameters (?o - orchestrator ?s - slot ?p1 - participant ?p2 - participant ?p3 - participant ?p4 - participant)
    :precondition (and
      (is-orchestrator ?o)
      (available ?p1 ?s)
      (available ?p2 ?s)
      (available ?p3 ?s)
      (available ?p4 ?s)
      (not (= ?p1 ?p2))
      (not (= ?p1 ?p3))
      (not (= ?p1 ?p4))
      (not (= ?p2 ?p3))
      (not (= ?p2 ?p4))
      (not (= ?p3 ?p4))
    )
    :effect (meeting-scheduled ?s)
  )
)