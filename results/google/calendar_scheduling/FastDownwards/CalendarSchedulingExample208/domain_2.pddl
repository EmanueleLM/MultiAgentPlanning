(define (domain meeting-scheduling)
  (:requirements :typing :strips)
  (:types person slot room)
  (:constants jesse nancy isabella harold linda - person)
  (:predicates
    (unscheduled)
    (scheduled ?s - slot)
    (available ?p - person ?s - slot)
    (room-free ?r - room)
    (room-occupied ?r - room ?s - slot)
  )
  (:action schedule
    :parameters (?r - room ?s - slot)
    :precondition (and
      (unscheduled)
      (room-free ?r)
      (available jesse ?s)
      (available nancy ?s)
      (available isabella ?s)
      (available harold ?s)
      (available linda ?s)
    )
    :effect (and
      (not (unscheduled))
      (not (room-free ?r))
      (scheduled ?s)
      (room-occupied ?r ?s)
    )
  )
)