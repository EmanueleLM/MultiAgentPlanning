(define (domain meeting-scheduling)
  (:requirements :strips :typing)
  (:types person slot)
  (:constants benjamin hannah brenda - person)
  (:predicates
    (participant ?p - person)
    (free ?p - person ?s - slot)
    (scheduled ?s - slot)
    (acceptable-for-benjamin ?s - slot)
  )
  (:action schedule-by-benjamin
    :parameters (?s - slot)
    :precondition (and
      (participant benjamin)
      (participant hannah)
      (participant brenda)
      (free benjamin ?s)
      (free hannah ?s)
      (free brenda ?s)
      (acceptable-for-benjamin ?s)
    )
    :effect (and
      (scheduled ?s)
      (not (free benjamin ?s))
      (not (free hannah ?s))
      (not (free brenda ?s))
    )
  )
  (:action schedule-by-hannah
    :parameters (?s - slot)
    :precondition (and
      (participant benjamin)
      (participant hannah)
      (participant brenda)
      (free benjamin ?s)
      (free hannah ?s)
      (free brenda ?s)
    )
    :effect (and
      (scheduled ?s)
      (not (free benjamin ?s))
      (not (free hannah ?s))
      (not (free brenda ?s))
    )
  )
  (:action schedule-by-brenda
    :parameters (?s - slot)
    :precondition (and
      (participant benjamin)
      (participant hannah)
      (participant brenda)
      (free benjamin ?s)
      (free hannah ?s)
      (free brenda ?s)
    )
    :effect (and
      (scheduled ?s)
      (not (free benjamin ?s))
      (not (free hannah ?s))
      (not (free brenda ?s))
    )
  )
)