(define (domain calendar-scheduling)
  (:requirements :strips :adl :action-costs)
  (:predicates
    (available ?p ?s)
    (before13 ?s)
    (after13 ?s)
    (free-scheduler)
    (scheduled)
    (meeting ?s)
  )
  (:action schedule-preferred
    :parameters (?s)
    :precondition (and (free-scheduler)
                       (before13 ?s)
                       (available christine ?s)
                       (available janice ?s)
                       (available bobby ?s)
                       (available elizabeth ?s)
                       (available tyler ?s)
                       (available edward ?s))
    :effect (and (not (free-scheduler))
                 (scheduled)
                 (meeting ?s))
    :cost 0
  )
  (:action schedule-nonpreferred
    :parameters (?s)
    :precondition (and (free-scheduler)
                       (after13 ?s)
                       (available christine ?s)
                       (available janice ?s)
                       (available bobby ?s)
                       (available elizabeth ?s)
                       (available tyler ?s)
                       (available edward ?s))
    :effect (and (not (free-scheduler))
                 (scheduled)
                 (meeting ?s))
    :cost 1
  )
)