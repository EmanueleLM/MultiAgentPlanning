(define (domain meeting-scheduling)
  (:requirements :typing :negative-preconditions)
  (:types participant slot)
  (:predicates
    (available ?p - participant ?s - slot)
    (meeting-scheduled)
    (scheduled-at ?s - slot)
    (attends ?p - participant)
    (room-available ?s - slot)
  )
  (:action schedule-meeting
    :parameters (?s - slot ?p1 - participant ?p2 - participant ?p3 - participant ?p4 - participant ?p5 - participant ?p6 - participant)
    :precondition (and
      (not (meeting-scheduled))
      (room-available ?s)
      (available ?p1 ?s)
      (available ?p2 ?s)
      (available ?p3 ?s)
      (available ?p4 ?s)
      (available ?p5 ?s)
      (available ?p6 ?s)
    )
    :effect (and
      (meeting-scheduled)
      (scheduled-at ?s)
      (attends ?p1)
      (attends ?p2)
      (attends ?p3)
      (attends ?p4)
      (attends ?p5)
      (attends ?p6)
      (not (room-available ?s))
      (not (available ?p1 ?s))
      (not (available ?p2 ?s))
      (not (available ?p3 ?s))
      (not (available ?p4 ?s))
      (not (available ?p5 ?s))
      (not (available ?p6 ?s))
    )
  )
)