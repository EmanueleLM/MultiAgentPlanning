(define (domain meeting_scheduling_multiagent)
  (:requirements :strips :typing)
  (:types person slot)
  (:predicates
    (available ?p - person ?s - slot)
    (meeting_scheduled)
    (scheduled_at ?s - slot)
    (preferred ?p - person ?s - slot)
    (unscheduled)
  )
  (:action heather_schedule
    :parameters (?s - slot)
    :precondition (and (unscheduled)
                       (available heather ?s)
                       (available nicholas ?s)
                       (available zachary ?s))
    :effect (and (meeting_scheduled)
                 (scheduled_at ?s)
                 (not (unscheduled)))
  )
  (:action nicholas_schedule
    :parameters (?s - slot)
    :precondition (and (unscheduled)
                       (available heather ?s)
                       (available nicholas ?s)
                       (available zachary ?s))
    :effect (and (meeting_scheduled)
                 (scheduled_at ?s)
                 (not (unscheduled)))
  )
  (:action zachary_schedule
    :parameters (?s - slot)
    :precondition (and (unscheduled)
                       (available heather ?s)
                       (available nicholas ?s)
                       (available zachary ?s))
    :effect (and (meeting_scheduled)
                 (scheduled_at ?s)
                 (not (unscheduled)))
  )
)