(define (domain meetup-classical)
  (:requirements :typing :action-costs :negative-preconditions :equality)
  (:types agent location time)
  (:predicates
    (at ?a - agent ?l - location)
    (current-time ?t - time)
    (next ?t1 - time ?t2 - time)
    (can-travel ?a - agent ?from - location ?to - location ?s - time ?e - time)
    (can-meet ?s - time ?e - time)
    (met ?a - agent ?b - agent)
  )
  (:functions (total-cost))
  (:action travel-you-marina-to-mission
    :parameters (?s - time ?e - time)
    :precondition (and (current-time ?s) (at you marina) (can-travel you marina mission ?s ?e))
    :effect (and (not (current-time ?s)) (current-time ?e) (not (at you marina)) (at you mission) (increase (total-cost) 20))
  )
  (:action travel-you-mission-to-marina
    :parameters (?s - time ?e - time)
    :precondition (and (current-time ?s) (at you mission) (can-travel you mission marina ?s ?e))
    :effect (and (not (current-time ?s)) (current-time ?e) (not (at you mission)) (at you marina) (increase (total-cost) 19))
  )
  (:action travel-stephanie-marina-to-mission
    :parameters (?s - time ?e - time)
    :precondition (and (current-time ?s) (at stephanie marina) (can-travel stephanie marina mission ?s ?e))
    :effect (and (not (current-time ?s)) (current-time ?e) (not (at stephanie marina)) (at stephanie mission) (increase (total-cost) 20))
  )
  (:action travel-stephanie-mission-to-marina
    :parameters (?s - time ?e - time)
    :precondition (and (current-time ?s) (at stephanie mission) (can-travel stephanie mission marina ?s ?e))
    :effect (and (not (current-time ?s)) (current-time ?e) (not (at stephanie mission)) (at stephanie marina) (increase (total-cost) 19))
  )
  (:action wait
    :parameters (?s - time ?e - time)
    :precondition (and (current-time ?s) (next ?s ?e))
    :effect (and (not (current-time ?s)) (current-time ?e))
  )
  (:action stephanie-arrive
    :parameters ()
    :precondition (current-time t90)
    :effect (at stephanie mission)
  )
  (:action stephanie-leave
    :parameters ()
    :precondition (current-time t270)
    :effect (not (at stephanie mission))
  )
  (:action meet
    :parameters (?s - time ?e - time ?loc - location ?a - agent ?b - agent)
    :precondition (and (current-time ?s) (at ?a ?loc) (at ?b ?loc) (can-meet ?s ?e))
    :effect (and (not (current-time ?s)) (current-time ?e) (met ?a ?b))
  )
)