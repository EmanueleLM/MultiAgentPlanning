(define (domain multiagent-meeting)
  (:requirements :typing :negative-preconditions :action-costs :adl :fluents)
  (:types agent location time)
  (:constants richmond-district financial-district - location)
  (:predicates
    (at ?ag - agent ?loc - location)
    (now ?t - time)
    (met ?a - agent ?b - agent)
    (can-travel-rf ?t1 - time ?t2 - time)
    (can-travel-fr ?t1 - time ?t2 - time)
    (can-wait ?t1 - time ?t2 - time)
    (can-meet ?t1 - time ?t2 - time)
  )
  (:functions (total-cost))
  (:action travel-richmond-to-financial
    :parameters (?ag - agent ?t1 - time ?t2 - time)
    :precondition (and (now ?t1) (at ?ag richmond-district) (can-travel-rf ?t1 ?t2))
    :effect (and
      (not (now ?t1))
      (now ?t2)
      (not (at ?ag richmond-district))
      (at ?ag financial-district)
      (increase (total-cost) 22)
    )
  )
  (:action travel-financial-to-richmond
    :parameters (?ag - agent ?t1 - time ?t2 - time)
    :precondition (and (now ?t1) (at ?ag financial-district) (can-travel-fr ?t1 ?t2))
    :effect (and
      (not (now ?t1))
      (now ?t2)
      (not (at ?ag financial-district))
      (at ?ag richmond-district)
      (increase (total-cost) 21)
    )
  )
  (:action wait
    :parameters (?t1 - time ?t2 - time)
    :precondition (and (now ?t1) (can-wait ?t1 ?t2))
    :effect (and
      (not (now ?t1))
      (now ?t2)
      (increase (total-cost) 1)
    )
  )
  (:action meet-at-financial
    :parameters (?a - agent ?b - agent ?t1 - time ?t2 - time)
    :precondition (and (now ?t1) (at ?a financial-district) (at ?b financial-district) (can-meet ?t1 ?t2))
    :effect (and
      (not (now ?t1))
      (now ?t2)
      (met ?a ?b)
      (met ?b ?a)
      (increase (total-cost) 60)
    )
  )
)