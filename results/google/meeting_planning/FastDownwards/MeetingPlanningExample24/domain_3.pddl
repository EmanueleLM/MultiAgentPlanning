(define (domain MeetingPlanningExample24)
  (:requirements :strips :typing :negative-preconditions :action-costs)
  (:types location person time count)
  (:predicates
    (at ?l - location)
    (current-time ?t - time)
    (met ?p - person)
    (meeting-spot ?p - person ?l - location)
    (start-ok ?p - person ?t - time)
    (travel17 ?from - location ?to - location)
    (travel18 ?from - location ?to - location)
    (plus1 ?t1 - time ?t2 - time)
    (dec1 ?c1 - count ?c2 - count)
    (busy)
    (enroute ?from - location ?to - location)
    (counter ?c - count)
    (meeting-with ?p - person ?l - location)
  )
  (:functions (total-cost))
  (:action wait-1
    :parameters (?t ?t2 - time ?l - location)
    :precondition (and (current-time ?t) (plus1 ?t ?t2) (at ?l) (not (busy)))
    :effect (and (not (current-time ?t)) (current-time ?t2) (increase (total-cost) 1))
  )
  (:action start-move-17
    :parameters (?from ?to - location)
    :precondition (and (at ?from) (travel17 ?from ?to) (not (busy)))
    :effect (and (not (at ?from)) (enroute ?from ?to) (busy) (counter c17))
  )
  (:action start-move-18
    :parameters (?from ?to - location)
    :precondition (and (at ?from) (travel18 ?from ?to) (not (busy)))
    :effect (and (not (at ?from)) (enroute ?from ?to) (busy) (counter c18))
  )
  (:action tick-busy-1
    :parameters (?t ?t2 - time ?c1 ?c2 - count)
    :precondition (and (busy) (current-time ?t) (plus1 ?t ?t2) (counter ?c1) (dec1 ?c1 ?c2))
    :effect (and (not (current-time ?t)) (current-time ?t2) (not (counter ?c1)) (counter ?c2) (increase (total-cost) 1))
  )
  (:action finish-move
    :parameters (?from ?to - location)
    :precondition (and (busy) (enroute ?from ?to) (counter c00))
    :effect (and (not (enroute ?from ?to)) (not (busy)) (at ?to))
  )
  (:action start-meet-30
    :parameters (?p - person ?l - location ?t - time)
    :precondition (and (at ?l) (meeting-spot ?p ?l) (not (met ?p)) (not (busy)) (current-time ?t) (start-ok ?p ?t))
    :effect (and (busy) (meeting-with ?p ?l) (counter c30))
  )
  (:action finish-meet
    :parameters (?p - person ?l - location)
    :precondition (and (busy) (meeting-with ?p ?l) (counter c00))
    :effect (and (met ?p) (not (busy)) (not (meeting-with ?p ?l)))
  )
)