(define (domain meeting-domain)
  (:requirements :strips :typing :negative-preconditions :adl :action-costs :fluents)
  (:types person location time)
  (:constants traveler barbara - person bayview goldengatepark - location)
  (:predicates
    (at ?p - person ?l - location ?t - time)
    (met ?p - person ?q - person)
    (travel_bayview_ggp ?t1 - time ?t2 - time)
    (travel_ggp_bayview ?t1 - time ?t2 - time)
    (span ?t1 - time ?t2 - time)
    (span90 ?t1 - time ?t2 - time)
    (available ?p - person ?t1 - time ?t2 - time)
  )
  (:functions (total-cost))
  (:action travel-traveler-bayview-to-ggp
    :parameters (?t1 - time ?t2 - time)
    :precondition (and (at traveler bayview ?t1) (travel_bayview_ggp ?t1 ?t2) (available traveler ?t1 ?t2))
    :effect (and (not (at traveler bayview ?t1)) (at traveler goldengatepark ?t2) (increase (total-cost) 22))
  )
  (:action travel-traveler-ggp-to-bayview
    :parameters (?t1 - time ?t2 - time)
    :precondition (and (at traveler goldengatepark ?t1) (travel_ggp_bayview ?t1 ?t2) (available traveler ?t1 ?t2))
    :effect (and (not (at traveler goldengatepark ?t1)) (at traveler bayview ?t2) (increase (total-cost) 23))
  )
  (:action stay-traveler
    :parameters (?l - location ?t1 - time ?t2 - time)
    :precondition (and (at traveler ?l ?t1) (span ?t1 ?t2) (available traveler ?t1 ?t2))
    :effect (and (not (at traveler ?l ?t1)) (at traveler ?l ?t2))
  )
  (:action stay-barbara
    :parameters (?l - location ?t1 - time ?t2 - time)
    :precondition (and (at barbara ?l ?t1) (span ?t1 ?t2) (available barbara ?t1 ?t2))
    :effect (and (not (at barbara ?l ?t1)) (at barbara ?l ?t2))
  )
  (:action meet-traveler-barbara
    :parameters (?l - location ?t1 - time ?t2 - time)
    :precondition (and
      (at traveler ?l ?t1)
      (at barbara ?l ?t1)
      (span90 ?t1 ?t2)
      (available traveler ?t1 ?t2)
      (available barbara ?t1 ?t2)
      (not (met traveler barbara))
    )
    :effect (and
      (not (at traveler ?l ?t1))
      (at traveler ?l ?t2)
      (not (at barbara ?l ?t1))
      (at barbara ?l ?t2)
      (met traveler barbara)
    )
  )
)