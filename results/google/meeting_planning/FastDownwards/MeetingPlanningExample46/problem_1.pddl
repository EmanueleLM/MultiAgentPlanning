(define (problem meeting-problem)
  (:domain meeting-planning)
  (:objects traveler robert - person haight_ashbury north_beach - location)
  (:init
    (at traveler haight_ashbury)
    (= (num-meetings) 0)
    (at 450 (robert-available))
    (at 750 (not (robert-available)))
  )
  (:goal (>= (num-meetings) 1))
  (:metric maximize (num-meetings))
)