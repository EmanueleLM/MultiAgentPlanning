(define (problem meeting-problem-adapted)
  (:domain meeting-planning-adapted)
  (:objects
    traveler robert - person
    haight_ashbury north_beach - location
    t540 t990 t1080 t1290 t1320 - time
  )
  (:init
    (at traveler haight_ashbury t540)
    ;; free intervals for traveler between coarse timepoints
    (free-interval traveler t540 t990)
    (free-interval traveler t540 t1080)
    (free-interval traveler t540 t1290)
    (free-interval traveler t540 t1320)
    (free-interval traveler t990 t1080)
    (free-interval traveler t990 t1290)
    (free-interval traveler t990 t1320)
    (free-interval traveler t1080 t1290)
    (free-interval traveler t1080 t1320)
    (free-interval traveler t1290 t1320)
    ;; after19 holds for pairs where end - start >= 19 minutes (coarse points)
    (after19 t540 t990)
    (after19 t540 t1080)
    (after19 t540 t1290)
    (after19 t540 t1320)
    (after19 t990 t1080)
    (after19 t990 t1290)
    (after19 t990 t1320)
    (after19 t1080 t1290)
    (after19 t1080 t1320)
    (after19 t1290 t1320)
    ;; after18 holds similarly (all coarse pairs meet >=18)
    (after18 t540 t990)
    (after18 t540 t1080)
    (after18 t540 t1290)
    (after18 t540 t1320)
    (after18 t990 t1080)
    (after18 t990 t1290)
    (after18 t990 t1320)
    (after18 t1080 t1290)
    (after18 t1080 t1320)
    (after18 t1290 t1320)
    ;; after90 holds for coarse pairs where end - start >= 90
    (after90 t540 t990)
    (after90 t540 t1080)
    (after90 t540 t1290)
    (after90 t540 t1320)
    (after90 t990 t1080)
    (after90 t990 t1290)
    (after90 t990 t1320)
    (after90 t1080 t1290)
    (after90 t1080 t1320)
    ;; note: t1290->t1320 (30 minutes) does not satisfy after90, so omitted
    ;; robert availability intervals (within 16:30 (t990) .. 21:30 (t1290))
    (robert-available-interval t990 t1080)
    (robert-available-interval t990 t1290)
    (robert-available-interval t1080 t1290)
    (= (total-cost) 0)
  )
  (:goal (met-robert))
  (:metric minimize (total-cost))
)