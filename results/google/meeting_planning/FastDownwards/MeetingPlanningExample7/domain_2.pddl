(define (domain meeting-sf)
  (:requirements :strips :typing :negative-preconditions :action-costs)
  (:types location time count)
  (:constants richmond northbeach - location)
  (:predicates
    (at ?l - location)
    (at-time ?t - time)
    (succ ?t1 - time ?t2 - time)
    (reach17 ?t1 - time ?t2 - time)
    (reach18 ?t1 - time ?t2 - time)
    (in-window ?t - time)
    (need ?c - count)
    (count-succ ?c1 - count ?c2 - count)
  )
  (:functions (total-cost))
  (:action wait
    :parameters (?t1 - time ?t2 - time)
    :precondition (and
      (at-time ?t1)
      (succ ?t1 ?t2)
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
    )
  )
  (:action travel-rd-nb
    :parameters (?t1 - time ?t2 - time)
    :precondition (and
      (at richmond)
      (at-time ?t1)
      (reach17 ?t1 ?t2)
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
      (not (at richmond))
      (at northbeach)
      (increase (total-cost) 17)
    )
  )
  (:action travel-nb-rd
    :parameters (?t1 - time ?t2 - time)
    :precondition (and
      (at northbeach)
      (at-time ?t1)
      (reach18 ?t1 ?t2)
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
      (not (at northbeach))
      (at richmond)
      (increase (total-cost) 18)
    )
  )
  (:action meet-minute
    :parameters (?t1 - time ?t2 - time ?c1 - count ?c2 - count)
    :precondition (and
      (at northbeach)
      (at-time ?t1)
      (succ ?t1 ?t2)
      (in-window ?t1)
      (need ?c1)
      (count-succ ?c1 ?c2)
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
      (not (need ?c1))
      (need ?c2)
    )
  )
)