(stay-decrement dubrovnik t0 t1 l3 l2)
(fly dubrovnik istanbul)
(stay-decrement istanbul t1 t2 l5 l4)
(stay-decrement istanbul t2 t3 l4 l3)
(stay-decrement istanbul t3 t4 l3 l2)
(stay-decrement istanbul t4 t5 l2 l1)
(stay-decrement istanbul t5 t6 l1 l0)
(fly istanbul budapest)
(stay-decrement budapest t6 t7 l6 l5)
(stay-decrement budapest t7 t8 l5 l4)
(stay-decrement budapest t8 t9 l4 l3)
(stay-decrement budapest t9 t10 l3 l2)
(stay-decrement budapest t10 t11 l2 l1)
(stay-decrement budapest t11 t12 l1 l0)
(pay-shortfall dubrovnik l2 l1 t12)
(pay-shortfall dubrovnik l1 l0 t12)
(finalize-city dubrovnik l0 t12)
(finalize-city istanbul l0 t12)
(finalize-city budapest l0 t12)
; cost = 2 (general cost)
