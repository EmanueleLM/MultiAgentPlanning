(define (domain trip-planning-example22)
  (:requirements :strips :typing :negative-preconditions)
  (:types day city)
  (:predicates
    (next ?d1 - day ?d2 - day)
    (direct ?c1 - city ?c2 - city)
    (in ?d - day ?c - city)
    (current ?d - day)
    (last-city ?c - city)
    (assigned ?d - day)
  )
  (:action assign-stay
    :parameters (?d - day ?c - city)
    :precondition (and (current ?d) (not (assigned ?d)) (last-city ?c))
    :effect (and (in ?d ?c) (assigned ?d))
  )
  (:action assign-fly
    :parameters (?d - day ?from - city ?to - city)
    :precondition (and (current ?d) (not (assigned ?d)) (last-city ?from) (direct ?from ?to))
    :effect (and (in ?d ?to) (assigned ?d) (not (last-city ?from)) (last-city ?to))
  )
  (:action advance
    :parameters (?d - day ?d2 - day)
    :precondition (and (current ?d) (assigned ?d) (next ?d ?d2) (not (assigned ?d2)))
    :effect (and (not (current ?d)) (current ?d2))
  )
)