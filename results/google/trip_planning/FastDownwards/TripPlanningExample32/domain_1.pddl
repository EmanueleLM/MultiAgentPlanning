(define (domain trip-planning)
  (:requirements :typing :negative-preconditions :action-costs)
  (:types day city agent)
  (:predicates
    (assigned ?d - day)
    (in ?d - day ?c - city)
    (flight ?c1 - city ?c2 - city)
    (prev ?d - day ?prev - day)
    (start ?d - day)
    (proposal-origin ?c - city ?a - agent)
    (pref-duration-3 ?c - city)
    (pref-duration-4 ?c - city)
    (pref-contiguous ?c - city)
  )
  (:functions (total-cost))
  (:action assign-start
    :parameters (?d - day ?c - city)
    :precondition (and (start ?d) (not (assigned ?d)))
    :effect (and (assigned ?d) (in ?d ?c) (increase (total-cost) 0))
  )
  (:action assign-stay
    :parameters (?d - day ?prev - day ?c - city)
    :precondition (and (prev ?d ?prev) (not (assigned ?d)) (in ?prev ?c))
    :effect (and (assigned ?d) (in ?d ?c) (increase (total-cost) 0))
  )
  (:action assign-fly
    :parameters (?d - day ?prev - day ?cprev - city ?c - city)
    :precondition (and (prev ?d ?prev) (not (assigned ?d)) (in ?prev ?cprev) (flight ?cprev ?c))
    :effect (and (assigned ?d) (in ?d ?c) (increase (total-cost) 0))
  )
)