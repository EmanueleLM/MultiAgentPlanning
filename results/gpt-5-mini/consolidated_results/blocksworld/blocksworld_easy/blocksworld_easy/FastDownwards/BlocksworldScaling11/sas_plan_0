(stacker-pick-from-loc stacker box2 loc1)
(stacker-pick-from-loc unstacker box3 loc1)
(stacker-place-on-loc unstacker box3 loc2)
(stacker-place-on-item stacker box2 box3)
(stacker-pick-from-item stacker box2 box3)
(stacker-pick-from-loc unstacker box1 loc1)
(stacker-place-on-item unstacker box1 box2)
(stacker-advance-to-unstacking unstacker unstacker)
(unstacker-pick-from-item unstacker box1 box2)
(unstacker-place-on-destination unstacker box1 loc3)
(unstacker-pick-from-loc unstacker box3 loc2)
(unstacker-place-on-destination unstacker box3 loc3)
(unstacker-place-on-destination stacker box2 loc3)
; cost = 13 (unit cost)
