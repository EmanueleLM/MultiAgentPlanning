(unstack orange red s0 s1)
(stack orange yellow s1 s2)
(unstack red blue s2 s3)
(put-down red s3 s4)
(unstack orange yellow s4 s5)
(put-down orange s5 s6)
(pick-up yellow s6 s7)
(stack yellow blue s7 s8)
(pick-up orange s8 s9)
(stack orange yellow s9 s10)
(pick-up red s10 s11)
(stack red orange s11 s12)
; cost = 12 (unit cost)
