(unstack red blue s0 s1)
(putdown red s1 s2)
(unstack blue orange s2 s3)
(putdown blue s3 s4)
(unstack orange yellow s4 s5)
(putdown orange s5 s6)
(pickup yellow s6 s7)
(stack yellow red s7 s8)
(pickup orange s8 s9)
(stack orange yellow s9 s10)
(pickup blue s10 s11)
(stack blue orange s11 s12)
; cost = 12 (unit cost)
