(unstack orange red s0 s1)
(putdown orange s1 s2)
(unstack red yellow s2 s3)
(putdown red s3 s4)
(unstack yellow blue s4 s5)
(putdown yellow s5 s6)
(pickup orange s6 s7)
(stack orange yellow s7 s8)
(pickup red s8 s9)
(stack red orange s9 s10)
(pickup blue s10 s11)
(stack blue red s11 s12)
; cost = 12 (unit cost)
