(unstack red yellow s0 s1)
(putdown red s1 s2)
(unstack yellow blue s2 s3)
(putdown yellow s3 s4)
(unstack blue orange s4 s5)
(putdown blue s5 s6)
(pickup orange s6 s7)
(stack orange blue s7 s8)
(pickup red s8 s9)
(stack red orange s9 s10)
(pickup yellow s10 s11)
(stack yellow red s11 s12)
; cost = 12 (unit cost)
