(unstack red yellow s0 s1)
(putdown red s1 s2)
(pickup yellow s2 s3)
(putdown yellow s3 s4)
(unstack orange blue s4 s5)
(putdown orange s5 s6)
(pickup blue s6 s7)
(stack blue red s7 s8)
(pickup orange s8 s9)
(stack orange blue s9 s10)
(pickup yellow s10 s11)
(stack yellow orange s11 s12)
; cost = 12 (unit cost)
