(define (domain blocksworld_stages)
  (:requirements :strips :typing :negative-preconditions)
  (:types obj stage)
  (:predicates
    (block ?x - obj)
    (handempty)
    (holding ?x - obj)
    (on ?x - obj ?y - obj)
    (clear ?x - obj)
    (succ ?s - stage ?s2 - stage)
    (at-stage ?s - stage)
  )

  (:action unstack
    :parameters (?b - obj ?b2 - obj ?s - stage ?s2 - stage)
    :precondition (and
      (at-stage ?s)
      (succ ?s ?s2)
      (handempty)
      (on ?b ?b2)
      (clear ?b)
      (block ?b)
      (block ?b2)
    )
    :effect (and
      (not (handempty))
      (holding ?b)
      (not (on ?b ?b2))
      (clear ?b2)
      (not (clear ?b))
      (not (at-stage ?s))
      (at-stage ?s2)
    )
  )

  (:action pick-up
    :parameters (?b - obj ?s - stage ?s2 - stage)
    :precondition (and
      (at-stage ?s)
      (succ ?s ?s2)
      (handempty)
      (on ?b table)
      (clear ?b)
      (block ?b)
    )
    :effect (and
      (not (handempty))
      (holding ?b)
      (not (on ?b table))
      (not (clear ?b))
      (not (at-stage ?s))
      (at-stage ?s2)
    )
  )

  (:action put-down
    :parameters (?b - obj ?s - stage ?s2 - stage)
    :precondition (and
      (at-stage ?s)
      (succ ?s ?s2)
      (holding ?b)
      (block ?b)
    )
    :effect (and
      (handempty)
      (not (holding ?b))
      (on ?b table)
      (clear ?b)
      (not (at-stage ?s))
      (at-stage ?s2)
    )
  )

  (:action stack
    :parameters (?b - obj ?b2 - obj ?s - stage ?s2 - stage)
    :precondition (and
      (at-stage ?s)
      (succ ?s ?s2)
      (holding ?b)
      (clear ?b2)
      (block ?b)
      (block ?b2)
    )
    :effect (and
      (handempty)
      (not (holding ?b))
      (on ?b ?b2)
      (not (clear ?b2))
      (clear ?b)
      (not (at-stage ?s))
      (at-stage ?s2)
    )
  )
)