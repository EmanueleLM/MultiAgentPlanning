(define (domain meeting-itinerary)
  (:requirements :strips :typing :negative-preconditions)
  (:types location friend timeslot)

  (:predicates
    (at ?l - location)
    (time-at ?t - timeslot)
    (succ ?t1 ?t2 - timeslot)
    (busy ?t - timeslot)
    (met ?f - friend)
    (available ?f - friend ?t - timeslot)
    (satisfied-betty)
  )

  ;; Meeting Alice 09:00 (s0) -> 09:30 (s6), duration 6 slots (6*5=30min)
  (:action meet-alice-s0
    :parameters ()
    :precondition (and
      (time-at s0)
      (at presidio)
      (available alice s0) (available alice s1) (available alice s2)
      (available alice s3) (available alice s4) (available alice s5)
      (succ s0 s1) (succ s1 s2) (succ s2 s3) (succ s3 s4) (succ s4 s5) (succ s5 s6)
      (not (busy s0)) (not (busy s1)) (not (busy s2))
      (not (busy s3)) (not (busy s4)) (not (busy s5))
    )
    :effect (and
      (met alice)
      (busy s0) (busy s1) (busy s2) (busy s3) (busy s4) (busy s5)
      (not (time-at s0)) (time-at s6)
    )
  )

