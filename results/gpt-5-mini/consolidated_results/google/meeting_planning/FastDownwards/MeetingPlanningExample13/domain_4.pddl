(define (domain meeting-itinerary)
  (:requirements :strips :typing :negative-preconditions)
  (:types location friend timeslot)

  (:predicates
    (at ?l - location)
    (time-at ?t - timeslot)
    (succ ?t1 ?t2 - timeslot)
    (busy ?t - timeslot)
    (met ?f - friend)
    (available ?f - friend ?t - timeslot)
