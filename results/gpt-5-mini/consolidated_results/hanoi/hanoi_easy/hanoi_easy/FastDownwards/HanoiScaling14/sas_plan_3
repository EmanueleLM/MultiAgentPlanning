(move-disk-to-peg a b right s0 s1)
(move-disk-to-peg b c left s1 s2)
(move-peg-to-disk a right b s2 s3)
(move-disk-to-peg c d right s3 s4)
(move-disk-to-disk a b d s4 s5)
(move-peg-to-disk b left c s5 s6)
(move-disk-to-disk a d b s6 s7)
(move-peg-to-peg d middle left s7 s8)
(move-disk-to-peg a b middle s8 s9)
(move-peg-to-disk a middle d s9 s10)
(move-disk-to-peg b c middle s10 s11)
(move-disk-to-disk a d b s11 s12)
(move-peg-to-disk c right d s12 s13)
(move-disk-to-peg a b right s13 s14)
(move-peg-to-disk b middle c s14 s15)
(move-peg-to-disk a right b s15 s16)
; cost = 16 (unit cost)
