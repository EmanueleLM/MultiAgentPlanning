(define (domain hanoi)
  (:requirements :strips :typing :negative-preconditions)
  (:types disk peg stage)

  (:predicates
    (on-disk ?d - disk ?support - disk)
    (on-peg  ?d - disk ?p - peg)
    (clear-disk ?d - disk)
    (clear-peg  ?p - peg)
    (smaller ?d1 - disk ?d2 - disk)
    (current ?s - stage)
    (next ?s1 - stage ?s2 - stage)
  )

  (:action move-from-disk-to-disk
    :parameters (?d - disk ?from - disk ?to - disk ?s - stage ?s2 - stage)
    :precondition (and
      (on-disk ?d ?from)
      (clear-disk ?d)
      (clear-disk ?to)
      (smaller ?d ?to)
      (current ?s)
      (next ?s ?s2)
    )
    :effect (and
      (not (on-disk ?d ?from))
      (on-disk ?d ?to)
      (not (clear-disk ?to))
      (clear-disk ?from)
      (clear-disk ?d)
      (not (current ?s))
      (current ?s2)
    )
  )

  (:action move-from-disk-to-peg
    :parameters (?d - disk ?from - disk ?topeg - peg ?s - stage ?s2 - stage)
    :precondition (and
      (on-disk ?d ?from)
      (clear-disk ?d)
      (clear-peg ?topeg)
      (current ?s)
      (next ?s ?s2)
    )
    :effect (and
      (not (on-disk ?d ?from))
      (on-peg ?d ?topeg)
      (not (clear-peg ?topeg))
      (clear-disk ?from)
      (clear-disk ?d)
      (not (current ?s))
      (current ?s2)
    )
  )

  (:action move-from-peg-to-disk
    :parameters (?d - disk ?frompeg - peg ?to - disk ?s - stage ?s2 - stage)
    :precondition (and
      (on-peg ?d ?frompeg)
      (clear-disk ?d)
      (clear-disk ?to)
      (smaller ?d ?to)
      (current ?s)
      (next ?s ?s2)
    )
    :effect (and
      (not (on-peg ?d ?frompeg))
      (on-disk ?d ?to)
      (not (clear-disk ?to))
      (clear-peg ?frompeg)
      (clear-disk ?d)
      (not (current ?s))
      (current ?s2)
    )
  )

  (:action move-from-peg-to-peg
    :parameters (?d - disk ?frompeg - peg ?topeg - peg ?s - stage ?s2 - stage)
    :precondition (and
      (on-peg ?d ?frompeg)
      (clear-disk ?d)
      (clear-peg ?topeg)
      (current ?s)
      (next ?s ?s2)
    )
    :effect (and
      (not (on-peg ?d ?frompeg))
      (on-peg ?d ?topeg)
      (clear-peg ?frompeg)
      (not (clear-peg ?topeg))
      (clear-disk ?d)
      (not (current ?s))
      (current ?s2)
    )
  )
)