(define (domain tower-of-hanoi-7)
  (:requirements :strips :typing :negative-preconditions)
  (:types disk peg - place stage)

  (:predicates
    (support ?d - disk ?s - place)
    (on ?d - disk ?p - peg)
    (clear ?d - disk)
    (empty ?p - peg)
    (smaller ?x - disk ?y - disk)
    (current ?t - stage)
    (next ?t1 - stage ?t2 - stage)
  )

  (:action move-disk-from-disk-to-empty
    :parameters (?d - disk ?below - disk ?src - peg ?dst - peg ?st - stage ?st2 - stage)
    :precondition (and
      (support ?d ?below)
      (on ?below ?src)
      (clear ?d)
      (empty ?dst)
      (current ?st)
      (next ?st ?st2)
    )
    :effect (and
      (not (support ?d ?below))
      (not (on ?d ?src))
      (support ?d ?dst)
      (on ?d ?dst)
      (not (empty ?dst))
      (clear ?below)
      (not (current ?st))
      (current ?st2)
    )
  )

  (:action move-disk-from-disk-to-disk
    :parameters (?d - disk ?below - disk ?t - disk ?src - peg ?dst - peg ?st - stage ?st2 - stage)
    :precondition (and
      (support ?d ?below)
      (on ?below ?src)
      (on ?t ?dst)
      (clear ?d)
      (clear ?t)
      (smaller ?d ?t)
      (current ?st)
      (next ?st ?st2)
    )
    :effect (and
      (not (support ?d ?below))
      (not (on ?d ?src))
      (support ?d ?t)
      (on ?d ?dst)
      (not (clear ?t))
      (clear ?below)
      (not (current ?st))
      (current ?st2)
    )
  )

  (:action move-disk-from-peg-to-empty
    :parameters (?d - disk ?src - peg ?dst - peg ?st - stage ?st2 - stage)
    :precondition (and
      (support ?d ?src)
      (on ?d ?src)
      (clear ?d)
      (empty ?dst)
      (current ?st)
      (next ?st ?st2)
    )
    :effect (and
      (not (support ?d ?src))
      (not (on ?d ?src))
      (empty ?src)
      (support ?d ?dst)
      (on ?d ?dst)
      (not (empty ?dst))
      (not (current ?st))
      (current ?st2)
    )
  )

  (:action move-disk-from-peg-to-disk
    :parameters (?d - disk ?t - disk ?src - peg ?dst - peg ?st - stage ?st2 - stage)
    :precondition (and
      (support ?d ?src)
      (on ?d ?src)
      (clear ?d)
      (on ?t ?dst)
      (clear ?t)
      (smaller ?d ?t)
      (current ?st)
      (next ?st ?st2)
    )
    :effect (and
      (not (support ?d ?src))
      (not (on ?d ?src))
      (support ?d ?t)
      (on ?d ?dst)
      (not (clear ?t))
      (empty ?src)
      (not (current ?st))
      (current ?st2)
    )
  )
)