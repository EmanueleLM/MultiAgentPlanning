(define (domain hanoi-6)
  (:requirements :strips :typing :negative-preconditions)
  (:types disk peg step)

  (:predicates
    (on-peg ?d - disk ?p - peg)
    (on-disk ?d - disk ?below - disk)
    (top ?p - peg ?d - disk)
    (empty ?p - peg)
    (smaller ?x - disk ?y - disk)
    (succ ?s - step ?s2 - step)
    (current ?s - step)
  )

  (:action move-peg-to-empty
    :parameters (?d - disk ?pfrom - peg ?pto - peg ?s - step ?s2 - step)
    :precondition (and
      (on-peg ?d ?pfrom)
      (top ?pfrom ?d)
      (empty ?pto)
      (current ?s)
      (succ ?s ?s2)
    )
    :effect (and
      (not (on-peg ?d ?pfrom))
      (on-peg ?d ?pto)
      (not (top ?pfrom ?d))
      (top ?pto ?d)
      (empty ?pfrom)
      (not (empty ?pto))
      (not (current ?s))
      (current ?s2)
    )
  )

  (:action move-peg-to-disk
    :parameters (?d - disk ?pfrom - peg ?pto - peg ?dto - disk ?s - step ?s2 - step)
    :precondition (and
      (on-peg ?d ?pfrom)
      (top ?pfrom ?d)
      (top ?pto ?dto)
      (smaller ?d ?dto)
      (current ?s)
      (succ ?s ?s2)
    )
    :effect (and
      (not (on-peg ?d ?pfrom))
      (on-disk ?d ?dto)
      (not (top ?pto ?dto))
      (top ?pto ?d)
      (empty ?pfrom)
      (not (current ?s))
      (current ?s2)
    )
  )

  (:action move-disk-to-empty
    :parameters (?d - disk ?dbelow - disk ?pfrom - peg ?pto - peg ?s - step ?s2 - step)
    :precondition (and
      (on-disk ?d ?dbelow)
      (top ?pfrom ?d)
      (empty ?pto)
      (current ?s)
      (succ ?s ?s2)
    )
    :effect (and
      (not (on-disk ?d ?dbelow))
      (on-peg ?d ?pto)
      (not (top ?pfrom ?d))
      (top ?pfrom ?dbelow)
      (top ?pto ?d)
      (not (empty ?pto))
      (not (current ?s))
      (current ?s2)
    )
  )

  (:action move-disk-to-disk
    :parameters (?d - disk ?dbelow - disk ?pfrom - peg ?pto - peg ?dto - disk ?s - step ?s2 - step)
    :precondition (and
      (on-disk ?d ?dbelow)
      (top ?pfrom ?d)
      (top ?pto ?dto)
      (smaller ?d ?dto)
      (current ?s)
      (succ ?s ?s2)
    )
    :effect (and
      (not (on-disk ?d ?dbelow))
      (on-disk ?d ?dto)
      (not (top ?pto ?dto))
      (top ?pto ?d)
      (not (top ?pfrom ?d))
      (top ?pfrom ?dbelow)
      (not (current ?s))
      (current ?s2)
    )
  )
)