(define (domain hanoi-six-staged)
  (:requirements :strips :typing :negative-preconditions)
  (:types disk peg stage)

  (:predicates
    (on-peg ?d - disk ?p - peg)
    (on-disk ?d - disk ?d2 - disk)
    (clear ?d - disk)
    (empty ?p - peg)
    (loc ?d - disk ?p - peg)
    (smaller ?d1 - disk ?d2 - disk)
    (now ?s - stage)
    (next ?s - stage ?s2 - stage)
    (diff ?p1 - peg ?p2 - peg)
  )

  ;; Move a top disk that is directly on a peg to an empty peg
  (:action move-from-peg-to-empty
    :parameters (?d - disk ?from - peg ?to - peg ?s - stage ?s2 - stage)
    :precondition (and
      (on-peg ?d ?from)
      (clear ?d)
      (loc ?d ?from)
      (not (empty ?from))
      (empty ?to)
      (diff ?from ?to)
      (now ?s)
      (next ?s ?s2)
    )
    :effect (and
      (not (on-peg ?d ?from))
      (not (loc ?d ?from))
      (empty ?from)
      (not (empty ?to))
      (on-peg ?d ?to)
      (loc ?d ?to)
      (clear ?d)
      (not (now ?s))
      (now ?s2)
    )
  )

  ;; Move a top disk that is directly on a peg to sit on top of another disk
  (:action move-from-peg-to-disk
    :parameters (?d - disk ?from - peg ?to - peg ?top - disk ?s - stage ?s2 - stage)
    :precondition (and
      (on-peg ?d ?from)
      (clear ?d)
      (loc ?d ?from)
      (not (empty ?from))
      (loc ?top ?to)
      (clear ?top)
      (smaller ?d ?top)
      (diff ?from ?to)
      (now ?s)
      (next ?s ?s2)
    )
    :effect (and
      (not (on-peg ?d ?from))
      (not (loc ?d ?from))
      (empty ?from)
      (not (clear ?top))
      (on-disk ?d ?top)
      (loc ?d ?to)
      (not (empty ?to))
      (clear ?d)
      (not (now ?s))
      (now ?s2)
    )
  )

  ;; Move a top disk that sits on another disk to an empty peg
  (:action move-from-disk-to-empty
    :parameters (?d - disk ?below - disk ?from - peg ?to - peg ?s - stage ?s2 - stage)
    :precondition (and
      (on-disk ?d ?below)
      (clear ?d)
      (loc ?d ?from)
      (loc ?below ?from)
      (not (empty ?from))
      (empty ?to)
      (diff ?from ?to)
      (now ?s)
      (next ?s ?s2)
    )
    :effect (and
      (not (on-disk ?d ?below))
      (clear ?below)
      (not (loc ?d ?from))
      (not (empty ?to))
      (on-peg ?d ?to)
      (loc ?d ?to)
      (clear ?d)
      (not (now ?s))
      (now ?s2)
    )
  )

  ;; Move a top disk that sits on another disk to sit on top of another disk
  (:action move-from-disk-to-disk
    :parameters (?d - disk ?below - disk ?from - peg ?to - peg ?top - disk ?s - stage ?s2 - stage)
    :precondition (and
      (on-disk ?d ?below)
      (clear ?d)
      (loc ?d ?from)
      (loc ?below ?from)
      (loc ?top ?to)
      (clear ?top)
      (smaller ?d ?top)
      (diff ?from ?to)
      (now ?s)
      (next ?s ?s2)
    )
    :effect (and
      (not (on-disk ?d ?below))
      (clear ?below)
      (not (clear ?top))
      (on-disk ?d ?top)
      (not (loc ?d ?from))
      (loc ?d ?to)
      (not (empty ?to))
      (clear ?d)
      (not (now ?s))
      (now ?s2)
    )
  )
)