(driver-board driver1 truck1 loca)
(hoist-load-into-vehicle hoist1 truck1 crate1 loca)
(driver-drive driver1 truck1 loca locb)
(hoist-load-into-vehicle hoist1 truck1 crate2 locb)
(hoist-place-on-pallet hoist1 truck1 crate1 pallet1 locb)
(driver-drive driver1 truck1 locb locc)
(hoist-place-on-pallet hoist1 truck1 crate2 pallet3 locc)
; cost = 7 (unit cost)
