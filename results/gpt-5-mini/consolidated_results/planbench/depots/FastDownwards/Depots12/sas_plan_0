(hoist-safety-check hoist1 load)
(hoist-attach-to-ground hoist1 container1 truck1 load)
(hoist-place-on-truck hoist1 container1 truck1 load)
(truck-secure-load truck1 container1 load)
(truck-lock truck1 container1 load)
(hoist-attach-from-truck hoist1 container1 truck1 load)
(truck-drive truck1 load unload container1 hoist1)
(hoist-engage hoist1 container1)
(hoist-move-with-load hoist1 load unload container1)
(hoist-place-on-ground hoist1 container1 unload)
(hoist-disengage hoist1 container1)
(truck-unlock truck1 unload)
; cost = 12 (unit cost)
