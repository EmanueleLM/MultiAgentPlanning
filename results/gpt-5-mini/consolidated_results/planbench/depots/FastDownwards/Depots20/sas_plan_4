(drive-truck truck1 depot2 depot1)
(hoist-lift hoist1 crate0 depot1 pallet1)
(hoist-load-into-truck hoist1 crate0 truck1 depot1)
(hoist-lift hoist2 crate1 depot2 pallet2)
(drive-truck truck1 depot1 depot2)
(hoist-load-into-truck hoist2 crate1 truck1 depot2)
(hoist-unload-from-truck hoist2 crate0 truck1 depot2)
(drive-truck truck1 depot2 depot1)
(hoist-unload-from-truck hoist1 crate1 truck1 depot1)
(hoist-drop-to-surface hoist1 crate1 depot1 pallet1)
(hoist-drop-to-surface hoist2 crate0 depot2 pallet2)
; cost = 11 (unit cost)
