(assign-driver alice truck1 depot-platform)
(hoist-pick hoist-depot cratea depot-platform)
(hoist-place-on-vehicle hoist-depot truck1 cratea depot-platform)
(drive alice truck1 depot-platform sitea-platform)
(hoist-unload-from-vehicle hoist-site truck1 cratea sitea-platform)
(hoist-place-to-ground hoist-site cratea sitea-platform)
; cost = 6 (unit cost)
