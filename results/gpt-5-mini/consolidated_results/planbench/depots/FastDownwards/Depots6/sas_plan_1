(drive truck0 distributor0 depot0 s0 s1)
(hoist-pick-from-crate hoist0 crate2 crate1 depot0 s1 s2)
(hoist-pick-from-pallet hoist2 crate0 pallet2 depot2 s2 s3)
(hoist-load-into-truck hoist0 crate2 truck0 depot0 s3 s4)
(hoist-pick-from-pallet hoist0 crate1 pallet0 depot0 s4 s5)
(hoist-load-into-truck hoist0 crate1 truck0 depot0 s5 s6)
(drive truck0 depot0 depot2 s6 s7)
(hoist-load-into-truck hoist2 crate0 truck0 depot2 s7 s8)
(hoist-unload-from-truck hoist2 crate1 truck0 depot2 s8 s9)
(drive truck0 depot2 depot1 s9 s10)
(hoist-unload-from-truck hoist1 crate2 truck0 depot1 s10 s11)
(hoist-place-on-pallet hoist1 crate2 pallet1 depot1 s11 s12)
(hoist-unload-from-truck hoist1 crate0 truck0 depot1 s12 s13)
(hoist-place-on-crate hoist1 crate0 crate2 depot1 s13 s14)
(hoist-place-on-pallet hoist2 crate1 pallet2 depot2 s14 s15)
; cost = 15 (unit cost)
