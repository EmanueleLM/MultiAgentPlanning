(define (domain orchestrator)
  (:requirements :strips :typing :negative-preconditions)
  (:types obj step)

  (:predicates
    (cats ?o - obj)
    (hand ?o - obj)
    (texture ?o - obj)
    (sneeze ?o - obj)
    (spring ?o - obj)
    (stupendous ?o - obj)
    (collect ?a ?b - obj)
    (next ?a ?b - obj)
    (vase ?x ?y - obj)
    (now ?t - step)
    (succ ?t1 ?t2 - step)
  )

  ;; sip: requires hand, cats, texture, next(x,z) and next(y,z); produces vase(x,y), removes next(x,z)
  (:action sip
    :parameters (?x ?y ?z - obj ?t ?t2 - step)
    :precondition (and
      (now ?t)
      (succ ?t ?t2)
      (hand ?x)
      (cats ?y)
      (texture ?z)
      (next ?x ?z)
      (next ?y ?z)
    )
    :effect (and
      (vase ?x ?y)
      (not (next ?x ?z))
      (not (now ?t))
      (now ?t2)
    )
  )

  ;; clip: requires hand, sneeze, texture, next(y,z) and next(x,z); produces vase(x,y), removes next(x,z)
  (:action clip
    :parameters (?x ?y ?z - obj ?t ?t2 - step)
    :precondition (and
      (now ?t)
      (succ ?t ?t2)
      (hand ?x)
      (sneeze ?y)
      (texture ?z)
      (next ?y ?z)
      (next ?x ?z)
    )
    :effect (and
      (vase ?x ?y)
      (not (next ?x ?z))
      (not (now ?t))
      (now ?t2)
    )
  )

  ;; paltry: requires hand, cats, texture, vase(x,y), next(y,z); produces next(x,z), removes vase(x,y)
  (:action paltry
    :parameters (?x ?y ?z - obj ?t ?t2 - step)
    :precondition (and
      (now ?t)
      (succ ?t ?t2)
      (hand ?x)
      (cats ?y)
      (texture ?z)
      (vase ?x ?y)
      (next ?y ?z)
    )
    :effect (and
      (next ?x ?z)
      (not (vase ?x ?y))
      (not (now ?t))
      (now ?t2)
    )
  )

  ;; tightfisted: requires hand, sneeze, texture, next(y,z), vase(x,y); produces next(x,z), removes vase(x,y)
  (:action tightfisted
    :parameters (?x ?y ?z - obj ?t ?t2 - step)
    :precondition (and
      (now ?t)
      (succ ?t ?t2)
      (hand ?x)
      (sneeze ?y)
      (texture ?z)
      (next ?y ?z)
      (vase ?x ?y)
    )
    :effect (and
      (next ?x ?z)
      (not (vase ?x ?y))
      (not (now ?t))
      (now ?t2)
    )
  )

  ;; memory: requires cats(x), spring(y), spring(z), next(x,y); produces next(x,z), removes next(x,y)
  (:action memory
    :parameters (?x ?y ?z - obj ?t ?t2 - step)
    :precondition (and
      (now ?t)
      (succ ?t ?t2)
      (cats ?x)
      (spring ?y)
      (spring ?z)
      (next ?x ?y)
    )
    :effect (and
      (next ?x ?z)
      (not (next ?x ?y))
      (not (now ?t))
      (now ?t2)
    )
  )

  ;; wretched: requires sneeze(x), texture(y), texture(z), stupendous(w), next(x,y), collect(y,w), collect(z,w);
  ;; produces next(x,z), removes next(x,y)
  (:action wretched
    :parameters (?x ?y ?z ?w - obj ?t ?t2 - step)
    :precondition (and
      (now ?t)
      (succ ?t ?t2)
      (sneeze ?x)
      (texture ?y)
      (texture ?z)
      (stupendous ?w)
      (next ?x ?y)
      (collect ?y ?w)
      (collect ?z ?w)
    )
    :effect (and
      (next ?x ?z)
      (not (next ?x ?y))
      (not (now ?t))
      (now ?t2)
    )
  )
)