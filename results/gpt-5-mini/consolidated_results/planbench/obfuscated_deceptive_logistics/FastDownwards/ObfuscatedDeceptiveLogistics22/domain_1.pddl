(define (domain orchestrated-domain)
  (:requirements :strips :typing :negative-preconditions)
  (:types object stage)
  (:predicates
    (cats ?o - object)
    (collect ?o1 - object ?o2 - object)
    (hand ?o - object)
    (next ?o1 - object ?o2 - object)
    (sneeze ?o - object)
    (spring ?o - object)
    (stupendous ?o - object)
    (texture ?o - object)
    (vase ?o1 - object ?o2 - object)
    (at-stage ?s - stage)
    (stage-next ?s1 - stage ?s2 - stage)
  )

  ;; paltry: requires vase(h,c) and next(c,t) -> creates next(h,t), removes vase(h,c)
  (:action paltry
    :parameters (?h - object ?c - object ?t - object ?s_from - stage ?s_to - stage)
    :precondition (and
      (hand ?h)
      (cats ?c)
      (texture ?t)
      (vase ?h ?c)
      (next ?c ?t)
      (at-stage ?s_from)
      (stage-next ?s_from ?s_to)
    )
    :effect (and
      (not (vase ?h ?c))
      (next ?h ?t)
      (not (at-stage ?s_from))
      (at-stage ?s_to)
    )
  )

  ;; sip: requires next(h,t) and next(c,t) -> creates vase(h,c), removes next(h,t)
  (:action sip
    :parameters (?h - object ?c - object ?t - object ?s_from - stage ?s_to - stage)
    :precondition (and
      (hand ?h)
      (cats ?c)
      (texture ?t)
      (next ?h ?t)
      (next ?c ?t)
      (at-stage ?s_from)
      (stage-next ?s_from ?s_to)
    )
    :effect (and
      (vase ?h ?c)
      (not (next ?h ?t))
      (not (at-stage ?s_from))
      (at-stage ?s_to)
    )
  )

  ;; clip: requires next(sneezeObj,t) and next(h,t) -> creates vase(h,sneezeObj), removes next(h,t)
  (:action clip
    :parameters (?h - object ?sneezeObj - object ?t - object ?s_from - stage ?s_to - stage)
    :precondition (and
      (hand ?h)
      (sneeze ?sneezeObj)
      (texture ?t)
      (next ?sneezeObj ?t)
      (next ?h ?t)
      (at-stage ?s_from)
      (stage-next ?s_from ?s_to)
    )
    :effect (and
      (vase ?h ?sneezeObj)
      (not (next ?h ?t))
      (not (at-stage ?s_from))
      (at-stage ?s_to)
    )
  )

  ;; wretched: requires next(x,y) and collects -> creates next(x,z), removes next(x,y)
  (:action wretched
    :parameters (?x - object ?y - object ?z - object ?stup - object ?s_from - stage ?s_to - stage)
    :precondition (and
      (sneeze ?x)
      (texture ?y)
      (texture ?z)
      (stupendous ?stup)
      (next ?x ?y)
      (collect ?y ?stup)
      (collect ?z ?stup)
      (at-stage ?s_from)
      (stage-next ?s_from ?s_to)
    )
    :effect (and
      (not (next ?x ?y))
      (next ?x ?z)
      (not (at-stage ?s_from))
      (at-stage ?s_to)
    )
  )

  ;; memory: requires cats(a), spring(b), spring(c), next(a,b) -> creates next(a,c), removes next(a,b)
  (:action memory
    :parameters (?a - object ?b - object ?c - object ?s_from - stage ?s_to - stage)
    :precondition (and
      (cats ?a)
      (spring ?b)
      (spring ?c)
      (next ?a ?b)
      (at-stage ?s_from)
      (stage-next ?s_from ?s_to)
    )
    :effect (and
      (not (next ?a ?b))
      (next ?a ?c)
      (not (at-stage ?s_from))
      (at-stage ?s_to)
    )
  )

  ;; tightfisted: requires vase(h,sneezeObj) and next(sneezeObj,t) -> creates next(h,t), removes vase(h,sneezeObj)
  (:action tightfisted
    :parameters (?h - object ?sneezeObj - object ?t - object ?s_from - stage ?s_to - stage)
    :precondition (and
      (hand ?h)
      (sneeze ?sneezeObj)
      (texture ?t)
      (next ?sneezeObj ?t)
      (vase ?h ?sneezeObj)
      (at-stage ?s_from)
      (stage-next ?s_from ?s_to)
    )
    :effect (and
      (next ?h ?t)
      (not (vase ?h ?sneezeObj))
      (not (at-stage ?s_from))
      (at-stage ?s_to)
    )
  )
)