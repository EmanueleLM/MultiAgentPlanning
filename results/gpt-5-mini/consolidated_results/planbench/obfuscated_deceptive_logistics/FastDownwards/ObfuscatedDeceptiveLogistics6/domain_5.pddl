(define (domain orchestrated-domain)
  (:requirements :strips :typing :negative-preconditions)
  (:types object stage)
  (:predicates
    (hand ?o - object)
    (cats ?o - object)
    (sneeze ?o - object)
    (texture ?o - object)
    (spring ?o - object)
    (stupendous ?o - object)
    (collect ?a - object ?b - object)
    (vase ?a - object ?b - object)
    (next ?a - object ?b - object)
    (stage-now ?s - stage)
    (succ ?s1 - stage ?s2 - stage)
  )

  (:action paltry
    :parameters (?x - object ?y - object ?z - object ?t - stage ?tn - stage)
    :precondition (and
      (stage-now ?t)
      (succ ?t ?tn)
      (hand ?x)
      (cats ?y)
      (texture ?z)
      (vase ?x ?y)
      (next ?y ?z)
    )
    :effect (and
      (not (stage-now ?t))
      (stage-now ?tn)
      (next ?x ?z)
      (not (vase ?x ?y))
    )
  )

  (:action sip
    :parameters (?x - object ?y - object ?z - object ?t - stage ?tn - stage)
    :precondition (and
      (stage-now ?t)
      (succ ?t ?tn)
      (hand ?x)
      (cats ?y)
      (texture ?z)
      (next ?x ?z)
      (next ?y ?z)
    )
    :effect (and
      (not (stage-now ?t))
      (stage-now ?tn)
      (vase ?x ?y)
      (not (next ?x ?z))
    )
  )

  (:action clip
    :parameters (?x - object ?y - object ?z - object ?t - stage ?tn - stage)
    :precondition (and
      (stage-now ?t)
      (succ ?t ?tn)
      (hand ?x)
      (sneeze ?y)
      (texture ?z)
      (next ?y ?z)
      (next ?x ?z)
    )
    :effect (and
      (not (stage-now ?t))
      (stage-now ?tn)
      (vase ?x ?y)
      (not (next ?x ?z))
    )
  )

  (:action wretched
    :parameters (?x - object ?y - object ?z - object ?w - object ?t - stage ?tn - stage)
    :precondition (and
      (stage-now ?t)
      (succ ?t ?tn)
      (sneeze ?x)
      (texture ?y)
      (texture ?z)
      (stupendous ?w)
      (next ?x ?y)
      (collect ?y ?w)
      (collect ?z ?w)
    )
    :effect (and
      (not (stage-now ?t))
      (stage-now ?tn)
      (next ?x ?z)
      (not (next ?x ?y))
    )
  )

  (:action memory
    :parameters (?x - object ?y - object ?z - object ?t - stage ?tn - stage)
    :precondition (and
      (stage-now ?t)
      (succ ?t ?tn)
      (cats ?x)
      (spring ?y)
      (spring ?z)
      (next ?x ?y)
    )
    :effect (and
      (not (stage-now ?t))
      (stage-now ?tn)
      (next ?x ?z)
      (not (next ?x ?y))
    )
  )

  (:action tightfisted
    :parameters (?x - object ?y - object ?z - object ?t - stage ?tn - stage)
    :precondition (and
      (stage-now ?t)
      (succ ?t ?tn)
      (hand ?x)
      (sneeze ?y)
      (texture ?z)
      (next ?y ?z)
      (vase ?x ?y)
    )
    :effect (and
      (not (stage-now ?t))
      (stage-now ?tn)
      (next ?x ?z)
      (not (vase ?x ?y))
    )
  )
)