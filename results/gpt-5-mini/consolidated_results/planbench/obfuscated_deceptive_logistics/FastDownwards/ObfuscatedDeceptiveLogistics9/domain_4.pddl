(define (domain obfuscated-deceptive-logistics-9)
  (:requirements :strips :typing :negative-preconditions)
  (:types obj stage)

  (:predicates
    (hand ?x - obj)
    (cats ?x - obj)
    (texture ?x - obj)
    (vase ?x ?y - obj)
    (next ?x ?y - obj)
    (sneeze ?x - obj)
    (collect ?x ?y - obj)
    (spring ?x - obj)
    (stupendous ?x - obj)
    ;; explicit discrete stage progression token and successor relation
    (current ?s - stage)
    (succ ?s1 ?s2 - stage)
  )

  (:action paltry
    :parameters (?h - obj ?c - obj ?t - obj ?from - stage ?to - stage)
    :precondition (and
                    (hand ?h)
                    (cats ?c)
                    (texture ?t)
                    (vase ?h ?c)
                    (next ?c ?t)
                    (current ?from)
                    (succ ?from ?to)
                  )
    :effect (and
              (next ?h ?t)
              (not (vase ?h ?c))
              ;; enforce stage progression (irreversible)
              (not (current ?from))
              (current ?to)
            )
  )

  (:action sip
    :parameters (?h - obj ?c - obj ?t - obj ?from - stage ?to - stage)
    :precondition (and
                    (hand ?h)
                    (cats ?c)
                    (texture ?t)
                    (next ?h ?t)
                    (next ?c ?t)
                    (current ?from)
                    (succ ?from ?to)
                  )
    :effect (and
              (vase ?h ?c)
              (not (next ?h ?t))
              ;; enforce stage progression (irreversible)
              (not (current ?from))
              (current ?to)
            )
  )

  (:action clip
    :parameters (?h - obj ?s - obj ?t - obj ?from - stage ?to - stage)
    :precondition (and
                    (hand ?h)
                    (sneeze ?s)
                    (texture ?t)
                    (next ?s ?t)
                    (next ?h ?t)
                    (current ?from)
                    (succ ?from ?to)
                  )
    :effect (and
              (vase ?h ?s)
              (not (next ?h ?t))
              ;; enforce stage progression (irreversible)
              (not (current ?from))
              (current ?to)
            )
  )

  (:action wretched
    :parameters (?s0 - obj ?t1 - obj ?t2 - obj ?st - obj ?from - stage ?to - stage)
    :precondition (and
                    (sneeze ?s0)
                    (texture ?t1)
                    (texture ?t2)
                    (stupendous ?st)
                    (next ?s0 ?t1)
                    (collect ?t1 ?st)
                    (collect ?t2 ?st)
                    (current ?from)
                    (succ ?from ?to)
                  )
    :effect (and
              (next ?s0 ?t2)
              (not (next ?s0 ?t1))
              ;; enforce stage progression (irreversible)
              (not (current ?from))
              (current ?to)
            )
  )

  (:action memory
    :parameters (?c - obj ?s1 - obj ?s2 - obj ?from - stage ?to - stage)
    :precondition (and
                    (cats ?c)
                    (spring ?s1)
                    (spring ?s2)
                    (next ?c ?s1)
                    (current ?from)
                    (succ ?from ?to)
                  )
    :effect (and
              (next ?c ?s2)
              (not (next ?c ?s1))
              ;; enforce stage progression (irreversible)
              (not (current ?from))
              (current ?to)
            )
  )

  (:action tightfisted
    :parameters (?h - obj ?s - obj ?t - obj ?from - stage ?to - stage)
    :precondition (and
                    (hand ?h)
                    (sneeze ?s)
                    (texture ?t)
                    (next ?s ?t)
                    (vase ?h ?s)
                    (current ?from)
                    (succ ?from ?to)
                  )
    :effect (and
              (next ?h ?t)
              (not (vase ?h ?s))
              ;; enforce stage progression (irreversible)
              (not (current ?from))
              (current ?to)
            )
  )
)