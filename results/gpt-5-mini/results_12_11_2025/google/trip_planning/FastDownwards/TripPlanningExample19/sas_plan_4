(assign-first d1 valencia)
(assign-next d1 d2 valencia valencia)
(assign-next d2 d3 valencia valencia)
(assign-next d3 d4 valencia brussels)
(assign-next d4 d5 brussels brussels)
(assign-next d5 d6 brussels nice)
(assign-next d6 d7 nice nice)
(assign-next d7 d8 nice nice)
(assign-next d8 d9 nice nice)
; cost = 9 (unit cost)
