(choose-start d1 valencia)
(stay-same d1 d2 valencia)
(stay-same d2 d3 valencia)
(stay-same d3 d4 valencia)
(stay-same d4 d5 valencia)
(move d5 d6 valencia lyon)
(stay-same d6 d7 lyon)
(stay-same d7 d8 lyon)
(stay-same d8 d9 lyon)
(move d9 d10 lyon split)
(stay-same d10 d11 split)
(stay-same d11 d12 split)
(stay-same d12 d13 split)
(stay-same d13 d14 split)
(stay-same d14 d15 split)
(stay-same d15 d16 split)
; cost = 16 (unit cost)
