(define (domain TripPlanningExample7)
  (:requirements :strips :typing :negative-preconditions)
  (:types city day)
  (:predicates
    (at ?c - city)
    (visited ?c - city)
    (day-available ?d - day)
    (day-belongs ?d - day ?c - city)
    (edge ?from ?to - city)
  )
  (:action fly
    :parameters (?from ?to - city)
    :precondition (and (at ?from) (edge ?from ?to))
    :effect (and (not (at ?from)) (at ?to))
  )
  (:action stay
    :parameters (?c - city ?d - day)
    :precondition (and (at ?c) (day-available ?d) (day-belongs ?d ?c))
    :effect (and (not (day-available ?d)) (visited ?c))
  )
)