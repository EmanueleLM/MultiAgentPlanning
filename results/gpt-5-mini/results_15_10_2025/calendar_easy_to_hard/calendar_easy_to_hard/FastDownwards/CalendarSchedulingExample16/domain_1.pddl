(define (domain calendar-scheduling)
  (:requirements :strips :typing)
  (:types person slot)
  (:constants olivia anna virginia paul - person)
  (:predicates
    (free ?p - person ?s - slot)
    (next ?s - slot ?t - slot)
    (scheduled)
    (meeting-start ?s - slot)
  )
  (:action schedule
    :parameters (?s - slot ?t - slot)
    :precondition (and
      (next ?s ?t)
      (free olivia ?s) (free olivia ?t)
      (free anna ?s) (free anna ?t)
      (free virginia ?s) (free virginia ?t)
      (free paul ?s) (free paul ?t)
    )
    :effect (and
      (scheduled)
      (meeting-start ?s)
    )
  )
)