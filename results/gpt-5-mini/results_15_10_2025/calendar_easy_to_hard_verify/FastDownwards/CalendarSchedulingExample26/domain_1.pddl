(define (domain meeting-scheduling)
  (:requirements :typing)
  (:types participant timeslot)
  (:predicates
    (workslot ?t - timeslot)
    (free ?p - participant ?t - timeslot)
    (occupied ?p - participant ?t - timeslot)
    (meeting-at ?t - timeslot)
    (next ?t1 - timeslot ?t2 - timeslot)
  )
  (:action schedule-meeting
    :parameters (?t - timeslot ?p1 - participant ?p2 - participant ?p3 - participant ?p4 - participant ?p5 - participant)
    :precondition (and
      (workslot ?t)
      (free ?p1 ?t)
      (free ?p2 ?t)
      (free ?p3 ?t)
      (free ?p4 ?t)
      (free ?p5 ?t)
    )
    :effect (and
      (meeting-at ?t)
      (not (free ?p1 ?t))
      (not (free ?p2 ?t))
      (not (free ?p3 ?t))
      (not (free ?p4 ?t))
      (not (free ?p5 ?t))
      (occupied ?p1 ?t)
      (occupied ?p2 ?t)
      (occupied ?p3 ?t)
      (occupied ?p4 ?t)
      (occupied ?p5 ?t)
    )
  )
)