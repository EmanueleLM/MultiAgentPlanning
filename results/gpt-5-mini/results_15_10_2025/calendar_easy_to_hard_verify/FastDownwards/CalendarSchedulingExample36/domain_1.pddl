(define (domain calendar-scheduling-example36)
  (:requirements :typing :negative-preconditions)
  (:types person time)
  (:predicates
    (available ?p - person ?t - time)
    (scheduled)
    (meeting-at ?t - time)
  )
  (:action schedule-meeting
    :parameters (?t - time)
    :precondition (and
      (not (scheduled))
      (available donald ?t)
      (available zachary ?t)
      (available kathryn ?t)
      (available deborah ?t)
      (available teresa ?t)
      (available james ?t)
    )
    :effect (and (scheduled) (meeting-at ?t))
  )
)