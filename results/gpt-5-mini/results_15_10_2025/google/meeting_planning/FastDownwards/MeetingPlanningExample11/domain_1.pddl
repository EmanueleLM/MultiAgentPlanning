(define (domain meet-visitors-classical)
  (:requirements :typing :negative-preconditions :action-costs :adl :universal-preconditions)
  (:types person location time)
  (:predicates
    (at-time ?p - person ?l - location ?t - time)
    (available-at ?p - person ?t - time)
    (met ?p - person)
    (accounted ?p - person)
    (le ?t1 - time ?t2 - time)
    (plus25 ?t - time ?t2 - time)
    (plus27 ?t - time ?t2 - time)
    (in_meeting ?t - time ?k - time)
  )
  (:functions (total-cost))

  (:action travel-nobhill-to-sunset
    :parameters (?p - person ?t - time ?t2 - time)
    :precondition (and (at-time ?p Nob_Hill ?t) (plus25 ?t ?t2))
    :effect (and
      (forall (?k - time)
        (when (le ?t ?k)
          (not (at-time ?p Nob_Hill ?k))
        )
      )
      (forall (?k - time)
        (when (le ?t2 ?k)
          (at-time ?p Sunset_District ?k)
        )
      )
    )
  )

  (:action travel-sunset-to-nobhill
    :parameters (?p - person ?t - time ?t2 - time)
    :precondition (and (at-time ?p Sunset_District ?t) (plus27 ?t ?t2))
    :effect (and
      (forall (?k - time)
        (when (le ?t ?k)
          (not (at-time ?p Sunset_District ?k))
        )
      )
      (forall (?k - time)
        (when (le ?t2 ?k)
          (at-time ?p Nob_Hill ?k)
        )
      )
    )
  )

  (:action meet-with-carol
    :parameters (?v - person ?c - person ?t - time)
    :precondition (and
      (not (met ?c))
      (forall (?k - time)
        (imply (in_meeting ?t ?k)
               (and (at-time ?v Sunset_District ?k)
                    (at-time ?c Sunset_District ?k)
                    (available-at ?c ?k)))
      )
    )
    :effect (and (met ?c) (accounted ?c))
  )

  (:action skip-carol
    :parameters (?c - person)
    :precondition (not (accounted ?c))
    :effect (and (accounted ?c) (increase (total-cost) 1))
  )
)