(define (domain meeting-planning)
  (:requirements :strips :typing :negative-preconditions :fluents)
  (:types location person time)
  (:constants chinatown russian-hill - location ronald - person)
  (:predicates
    (at-traveler ?l - location)
    (met ?p - person)
    (current-time ?t - time)
    (succ7 ?t1 - time ?t2 - time)
    (succ9 ?t1 - time ?t2 - time)
    (succ105 ?t1 - time ?t2 - time)
    (avail-start ?t - time)
  )
  (:functions (total-cost))
  (:action move-chinatown-to-russianhill
    :parameters (?t1 - time ?t2 - time)
    :precondition (and (at-traveler chinatown) (current-time ?t1) (succ7 ?t1 ?t2))
    :effect (and (not (at-traveler chinatown)) (at-traveler russian-hill) (not (current-time ?t1)) (current-time ?t2) (increase (total-cost) 0))
  )
  (:action move-russianhill-to-chinatown
    :parameters (?t1 - time ?t2 - time)
    :precondition (and (at-traveler russian-hill) (current-time ?t1) (succ9 ?t1 ?t2))
    :effect (and (not (at-traveler russian-hill)) (at-traveler chinatown) (not (current-time ?t1)) (current-time ?t2) (increase (total-cost) 0))
  )
  (:action meet-ronald
    :parameters (?t1 - time ?t2 - time)
    :precondition (and (at-traveler russian-hill) (current-time ?t1) (succ105 ?t1 ?t2) (avail-start ?t1) (not (met ronald)))
    :effect (and (met ronald) (not (current-time ?t1)) (current-time ?t2) (increase (total-cost) 1))
  )
)