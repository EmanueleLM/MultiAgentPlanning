(define (domain meeting-planning)
  (:requirements :strips :typing :negative-preconditions)
  (:types person location time)
  (:constants mission-district haight-ashbury - location)
  (:predicates
    (at ?l - location)
    (met ?p - person)
    (current-time ?t - time)
    (after12 ?t1 ?t2 - time)
    (after11 ?t1 ?t2 - time)
    (after30 ?t1 ?t2 - time)
    (meet-start-ok ?t - time)
  )
  (:action travel-md-to-ha
    :parameters (?t1 ?t2 - time)
    :precondition (and (at mission-district) (current-time ?t1) (after12 ?t1 ?t2))
    :effect (and (not (at mission-district)) (at haight-ashbury) (not (current-time ?t1)) (current-time ?t2)))
  (:action travel-ha-to-md
    :parameters (?t1 ?t2 - time)
    :precondition (and (at haight-ashbury) (current-time ?t1) (after11 ?t1 ?t2))
    :effect (and (not (at haight-ashbury)) (at mission-district) (not (current-time ?t1)) (current-time ?t2)))
  (:action meet-margaret
    :parameters (?p - person ?t1 ?t2 - time)
    :precondition (and (at haight-ashbury) (current-time ?t1) (after30 ?t1 ?t2) (meet-start-ok ?t1))
    :effect (and (met ?p) (not (current-time ?t1)) (current-time ?t2)))
)