(define (domain meeting-planning)
  (:requirements :strips :typing :negative-preconditions :action-costs)
  (:types agent location time)
  (:predicates
    (at ?a - agent ?l - location)
    (now ?t - time)
    (travel-time ?t1 - time ?t2 - time)
    (travels ?from - location ?to - location)
    (meeting-span ?t1 - time ?t2 - time)
    (available ?a - agent ?t1 - time ?t2 - time)
    (met ?a - agent ?b - agent)
  )
  (:functions (total-cost))
  (:action traveler-travel
    :parameters (?from - location ?to - location ?t1 - time ?t2 - time)
    :precondition (and
      (now ?t1)
      (at traveler ?from)
      (travels ?from ?to)
      (travel-time ?t1 ?t2)
    )
    :effect (and
      (not (now ?t1))
      (now ?t2)
      (not (at traveler ?from))
      (at traveler ?to)
      (increase (total-cost) 22)
    )
  )
  (:action meet-traveler-barbara
    :parameters (?loc - location ?t1 - time ?t2 - time)
    :precondition (and
      (now ?t1)
      (at traveler ?loc)
      (at barbara ?loc)
      (meeting-span ?t1 ?t2)
      (available barbara ?t1 ?t2)
    )
    :effect (and
      (not (now ?t1))
      (now ?t2)
      (met traveler barbara)
    )
  )
)