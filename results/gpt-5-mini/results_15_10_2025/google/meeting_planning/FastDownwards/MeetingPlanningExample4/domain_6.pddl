(define (domain meeting-planning-classical)
  (:requirements :typing :adl :negative-preconditions :action-costs :equality)
  (:types agent location time)
  (:constants you jessica orchestrator - agent presidio marina - location)
  (:predicates
    (now ?t - time)
    (next1 ?t - time ?t1 - time)
    (next10 ?t - time ?t10 - time)
    (next60 ?t - time ?t60 - time)
    (at ?a - agent ?l - location)
    (jessica_present_marker ?t - time)
    (jessica_present)
    (in_meeting)
    (met60)
    (can_meet_60_at ?t - time)
  )
  (:functions (total-cost))
  (:action advance_idle
    :parameters (?t - time ?t1 - time)
    :precondition (and (now ?t) (next1 ?t ?t1) (not (in_meeting)))
    :effect (and
      (not (now ?t)) (now ?t1)
      (when (jessica_present_marker ?t1) (jessica_present))
      (when (not (jessica_present_marker ?t1)) (not (jessica_present)))
      (increase (total-cost) 1)
    )
  )
  (:action advance_meeting
    :parameters (?t - time ?t1 - time)
    :precondition (and (now ?t) (next1 ?t ?t1) (in_meeting) (at you marina) (jessica_present_marker ?t1))
    :effect (and
      (not (now ?t)) (now ?t1)
      (when (jessica_present_marker ?t1) (jessica_present))
      (increase (total-cost) 0)
    )
  )
  (:action you_move_presidio_to_marina
    :parameters (?t - time ?t10 - time)
    :precondition (and (now ?t) (at you presidio) (next10 ?t ?t10) (not (in_meeting)))
    :effect (and
      (not (now ?t)) (now ?t10)
      (not (at you presidio)) (at you marina)
      (when (jessica_present_marker ?t10) (jessica_present))
      (when (not (jessica_present_marker ?t10)) (not (jessica_present)))
      (increase (total-cost) 0)
    )
  )
  (:action start_meeting_60
    :parameters (?t - time ?t60 - time)
    :precondition (and (now ?t) (at you marina) (next60 ?t ?t60) (can_meet_60_at ?t) (jessica_present_marker ?t) (not (in_meeting)))
    :effect (and
      (not (now ?t)) (now ?t60)
      (in_meeting)
      (met60)
      (when (jessica_present_marker ?t60) (jessica_present))
      (when (not (jessica_present_marker ?t60)) (not (jessica_present)))
      (increase (total-cost) 0)
    )
  )
  (:action extend_meeting_1
    :parameters (?t - time ?t1 - time)
    :precondition (and (now ?t) (next1 ?t ?t1) (in_meeting) (at you marina) (jessica_present_marker ?t1))
    :effect (and
      (not (now ?t)) (now ?t1)
      (in_meeting)
      (when (jessica_present_marker ?t1) (jessica_present))
      (increase (total-cost) 0)
    )
  )
  (:action end_meeting
    :parameters (?t - time ?t1 - time)
    :precondition (and (now ?t) (next1 ?t ?t1) (in_meeting))
    :effect (and
      (not (now ?t)) (now ?t1)
      (not (in_meeting))
      (increase (total-cost) 0)
    )
  )
)