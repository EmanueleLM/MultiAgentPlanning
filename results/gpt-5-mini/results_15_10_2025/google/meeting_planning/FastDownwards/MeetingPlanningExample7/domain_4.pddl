(define (domain meeting-scheduling-classical)
  (:requirements :strips :typing :negative-preconditions :equality :adl :action-costs)
  (:types agent location time token)
  (:constants Richmond_District North_Beach - location traveler john orchestrator - agent)
  (:predicates
    (at ?a - agent ?l - location ?t - time)
    (succ ?t1 - time ?t2 - time)
    (can-travel ?from - location ?to - location ?t1 - time ?t2 - time)
    (john-present ?t - time)
    (met ?t - time)
    (token-free ?tk - token)
    (token-claimed ?tk - token)
  )
  (:functions (total-cost))
  (:action wait
    :parameters (?a - agent ?loc - location ?t - time ?t2 - time)
    :precondition (and (at ?a ?loc ?t) (succ ?t ?t2))
    :effect (and (not (at ?a ?loc ?t)) (at ?a ?loc ?t2) (increase (total-cost) 1))
  )
  (:action travel-rd-nb
    :parameters (?a - agent ?t - time ?t2 - time)
    :precondition (and (at ?a Richmond_District ?t) (can-travel Richmond_District North_Beach ?t ?t2))
    :effect (and (not (at ?a Richmond_District ?t)) (at ?a North_Beach ?t2) (increase (total-cost) 17))
  )
  (:action travel-nb-rd
    :parameters (?a - agent ?t - time ?t2 - time)
    :precondition (and (at ?a North_Beach ?t) (can-travel North_Beach Richmond_District ?t ?t2))
    :effect (and (not (at ?a North_Beach ?t)) (at ?a Richmond_District ?t2) (increase (total-cost) 18))
  )
  (:action meet-minute
    :parameters (?t - time ?t2 - time)
    :precondition (and (at traveler North_Beach ?t) (john-present ?t) (succ ?t ?t2))
    :effect (and (not (at traveler North_Beach ?t)) (at traveler North_Beach ?t2) (met ?t) (increase (total-cost) 0))
  )
  (:action claim-token
    :parameters (?tk - token ?t - time)
    :precondition (and (token-free ?tk) (met ?t))
    :effect (and (not (token-free ?tk)) (token-claimed ?tk) (not (met ?t)) (increase (total-cost) 0))
  )
)