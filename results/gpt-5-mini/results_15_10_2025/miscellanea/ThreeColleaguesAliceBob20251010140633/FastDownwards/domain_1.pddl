(define (domain meeting-scheduling)
  (:requirements :typing :negative-preconditions)
  (:types person slot date)
  (:predicates
    (on-date ?s - slot ?d - date)
    (free ?p - person ?s - slot)
    (preferred-slot ?s - slot)
    (scheduled ?s - slot)
  )
  (:action schedule
    :parameters (?s - slot)
    :precondition (and
      (free alice ?s)
      (free bob ?s)
      (free carol ?s)
      (preferred-slot ?s)
      (not (scheduled ?s))
    )
    :effect (and (scheduled ?s))
  )
)