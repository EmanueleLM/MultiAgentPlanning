(define (problem stacking-problem)
  (:domain multiagent-stacking)
  (:objects
    ;; vowels
    a e i o - vowel
    ;; consonants
    m j d n c g h f k l b - consonant
    ;; Note: vowel and consonant are subtypes of block by the domain typing
  )
  (:init
    ;; Stack1: M (on table) - J - D - N - C - O (top)
    (ontable m)
    (on j m)
    (on d j)
    (on n d)
    (on c n)
    (on o c)
    ;; Stack2: G (on table) - E - A - H - F - K - L - B - I (top)
    (ontable g)
    (on e g)
    (on a e)
    (on h a)
    (on f h)
    (on k f)
    (on l k)
    (on b l)
    (on i b)
    ;; Top-of-stack markers (clear)
    (clear o)
    (clear i)
    ;; Note: other blocks are not declared clear here (they have blocks above them)
  )
  (:goal (and
    (on a b)
    (on b c)
    (on c d)
    (on d e)
    (on e f)
    (on f g)
    (on g h)
    (on h i)
    (on i j)
    (on j k)
    (on k l)
    (on l m)
    (on m n)
    (on n o)
    (ontable o)
  ))
)