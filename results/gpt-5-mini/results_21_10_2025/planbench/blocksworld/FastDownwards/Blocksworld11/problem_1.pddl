(define (problem player1-problem)
  (:domain Blocksworld11)
  (:requirements :strips :typing)
  (:objects blue yellow red orange - block)
  ;; Initial state for player1 (blue on yellow; yellow on table)
  (:init
    (on blue yellow)
    (ontable yellow)
    (ontable red)
    (ontable orange)
    (clear blue)
    (clear red)
    (clear orange)
    (handempty)
  )
  ;; Goal: blue on orange AND yellow on red
  (:goal (and
    (on blue orange)
    (on yellow red)
  ))
)