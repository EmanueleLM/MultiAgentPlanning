(define (domain multi-agent-meeting)
  (:requirements :strips :typing)
  (:types slot)
  (:predicates
    (free-joyce ?s - slot)
    (free-christine ?s - slot)
    (free-alexander ?s - slot)
    (scheduled)
    (scheduled-at ?s - slot)
  )
  (:action schedule-meeting
    :parameters (?s - slot)
    :precondition (and (free-joyce ?s) (free-christine ?s) (free-alexander ?s))
    :effect (and (scheduled) (scheduled-at ?s))
  )
)