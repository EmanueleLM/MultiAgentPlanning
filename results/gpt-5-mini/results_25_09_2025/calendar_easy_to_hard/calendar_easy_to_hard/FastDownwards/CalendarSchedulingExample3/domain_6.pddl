(define (domain meeting-scheduling)
  (:requirements :typing :adl :negative-preconditions :action-costs :fluents)
  (:types person time)
  (:predicates
    (free ?p - person ?t - time)
    (succ ?t - time ?t2 - time)
    (scheduled ?t - time)
  )
  (:functions (total-cost))
  (:action schedule-meeting
    :parameters (?a - person ?b - person ?c - person ?t - time ?t2 - time)
    :precondition (and
      (succ ?t ?t2)
      (free ?a ?t) (free ?a ?t2)
      (free ?b ?t) (free ?b ?t2)
      (free ?c ?t) (free ?c ?t2)
      (not (scheduled ?t)) (not (scheduled ?t2))
      (not (= ?a ?b)) (not (= ?a ?c)) (not (= ?b ?c))
    )
    :effect (and
      (scheduled ?t) (scheduled ?t2)
      (not (free ?a ?t)) (not (free ?a ?t2))
      (not (free ?b ?t)) (not (free ?b ?t2))
      (not (free ?c ?t)) (not (free ?c ?t2))
      (increase (total-cost) 1)
    )
  )
)