(define (domain orchestrated-meeting)
  (:requirements :typing :negative-preconditions :adl)
  (:types person slot meeting)

  (:predicates
    (avail_a1 ?p - person ?s - slot)
    (busy_a2 ?p - person ?s - slot)
    (free_a2 ?s - slot)
    (attendee_a2 ?m - meeting ?p - person)
    (unscheduled_a2 ?m - meeting)
    (avail_a3 ?p - person ?s - slot)
    (confirmed_a1 ?s - slot ?p - person)
    (confirmed_a2 ?m - meeting ?s - slot)
    (confirmed_a3 ?s - slot ?p - person)
    (meeting-scheduled)
    (meeting-at ?s - slot)
    (meeting-with ?p - person)
    (scheduled_a2 ?m - meeting ?s - slot)
    (late ?s - slot)
  )

  (:action confirm-by-agent1
    :parameters (?s - slot ?p - person)
    :precondition (avail_a1 ?p ?s)
    :effect (confirmed_a1 ?s ?p)
  )

  (:action confirm-by-agent3
    :parameters (?s - slot ?p - person)
    :precondition (avail_a3 ?p ?s)
    :effect (confirmed_a3 ?s ?p)
  )

  (:action confirm-by-agent2-normal
    :parameters (?m - meeting ?s - slot ?p1 - person ?p2 - person ?p3 - person)
    :precondition (and
                    (unscheduled_a2 ?m)
                    (free_a2 ?s)
                    (attendee_a2 ?m ?p1)
                    (attendee_a2 ?m ?p2)
                    (attendee_a2 ?m ?p3)
                    (not (busy_a2 ?p1 ?s))
                    (not (busy_a2 ?p2 ?s))
                    (not (busy_a2 ?p3 ?s))
                    (not (late ?s))
                  )
    :effect (and
              (confirmed_a2 ?m ?s)
              (scheduled_a2 ?m ?s)
              (not (unscheduled_a2 ?m))
              (not (free_a2 ?s))
            )
  )

  (:action confirm-by-agent2-late
    :parameters (?m - meeting ?s - slot ?p1 - person ?p2 - person ?p3 - person)
    :precondition (and
                    (unscheduled_a2 ?m)
                    (free_a2 ?s)
                    (attendee_a2 ?m ?p1)
                    (attendee_a2 ?m ?p2)
                    (attendee_a2 ?m ?p3)
                    (not (busy_a2 ?p1 ?s))
                    (not (busy_a2 ?p2 ?s))
                    (not (busy_a2 ?p3 ?s))
                    (late ?s)
                  )
    :effect (and
              (confirmed_a2 ?m ?s)
              (scheduled_a2 ?m ?s)
              (not (unscheduled_a2 ?m))
              (not (free_a2 ?s))
            )
  )

  (:action finalize-meeting
    :parameters (?m - meeting ?s - slot ?org - person ?p1 - person ?p2 - person)
    :precondition (and
                    (confirmed_a1 ?s ?org)
                    (confirmed_a3 ?s ?p1)
                    (confirmed_a2 ?m ?s)
                    (attendee_a2 ?m ?org)
                    (attendee_a2 ?m ?p1)
                    (attendee_a2 ?m ?p2)
                    (not (meeting-scheduled))
                  )
    :effect (and
              (meeting-scheduled)
              (meeting-at ?s)
              (meeting-with ?org)
              (meeting-with ?p1)
              (meeting-with ?p2)
            )
  )
)