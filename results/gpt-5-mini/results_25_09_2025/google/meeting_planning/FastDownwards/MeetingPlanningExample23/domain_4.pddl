(define (domain meeting-schedule-classical)
  (:requirements :typing :fluents)
  (:types agent location)
  (:predicates
    (at ?a - agent ?l - location)
    (met)
    (john_available_for_75)
  )
  (:functions (total-cost))
  (:action travel
    :parameters (?a - agent ?from - location ?to - location)
    :precondition (and (at ?a ?from))
    :effect (and (not (at ?a ?from)) (at ?a ?to) (increase (total-cost) 23))
  )
  (:action meet
    :parameters (?a - agent ?b - agent ?l - location)
    :precondition (and (at ?a ?l) (at ?b ?l) (john_available_for_75))
    :effect (and (met) (increase (total-cost) 75))
  )
)