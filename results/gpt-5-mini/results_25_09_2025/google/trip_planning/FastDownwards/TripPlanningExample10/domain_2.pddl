(define (domain trip-domain)
  (:requirements :strips :typing :fluents)
  (:types city day)
  (:predicates
    (at ?c - city)
    (current ?d - day)
    (next ?d1 ?d2 - day)
    (in-relatives-window ?d - day)
    (relatives-visited)
  )
  (:functions
    (oslo-count)
    (dublin-count)
    (valencia-count)
  )
  (:action oslo_agent-stay-oslo
    :parameters (?d ?dnext - day)
    :precondition (and (current ?d) (next ?d ?dnext) (at oslo))
    :effect (and (not (current ?d)) (current ?dnext) (increase (oslo-count) 1))
  )
  (:action dublin_agent-stay-dublin
    :parameters (?d ?dnext - day)
    :precondition (and (current ?d) (next ?d ?dnext) (at dublin))
    :effect (and (not (current ?d)) (current ?dnext) (increase (dublin-count) 1))
  )
  (:action valencia_agent-stay-valencia
    :parameters (?d ?dnext - day)
    :precondition (and (current ?d) (next ?d ?dnext) (at valencia))
    :effect (and (not (current ?d)) (current ?dnext) (increase (valencia-count) 1))
  )
  (:action oslo_agent-fly-oslo-to-dublin
    :parameters ()
    :precondition (at oslo)
    :effect (and (not (at oslo)) (at dublin))
  )
  (:action dublin_agent-fly-dublin-to-oslo
    :parameters ()
    :precondition (at dublin)
    :effect (and (not (at dublin)) (at oslo))
  )
  (:action dublin_agent-fly-dublin-to-valencia
    :parameters ()
    :precondition (at dublin)
    :effect (and (not (at dublin)) (at valencia))
  )
  (:action valencia_agent-fly-valencia-to-dublin
    :parameters ()
    :precondition (at valencia)
    :effect (and (not (at valencia)) (at dublin))
  )
  (:action valencia_agent-visit-relatives
    :parameters (?d - day)
    :precondition (and (current ?d) (at valencia) (in-relatives-window ?d))
    :effect (relatives-visited)
  )
)