(define (domain meeting-scheduling)
  (:requirements :typing :strips :action-costs :fluents :conditional-effects :equality)
  (:types participant slot priority)
  (:predicates
    (available ?p - participant ?s - slot)
    (attended ?p - participant ?s - slot)
    (meeting-scheduled ?s - slot)
    (priority ?s - slot ?pr - priority)
  )
  (:functions (total-cost))
  (:action attend
    :parameters (?p - participant ?s - slot)
    :precondition (available ?p ?s)
    :effect (attended ?p ?s)
  )
  (:action schedule
    :parameters (?s - slot)
    :precondition ()
    :effect (and
      (meeting-scheduled ?s)
      (when (priority ?s pr1) (increase (total-cost) 1))
      (when (priority ?s pr2) (increase (total-cost) 2))
      (when (priority ?s pr3) (increase (total-cost) 3))
      (when (priority ?s pr4) (increase (total-cost) 4))
      (when (priority ?s pr5) (increase (total-cost) 5))
      (when (priority ?s pr6) (increase (total-cost) 6))
      (when (priority ?s pr7) (increase (total-cost) 7))
      (when (priority ?s pr8) (increase (total-cost) 8))
      (when (priority ?s pr9) (increase (total-cost) 9))
      (when (priority ?s pr10) (increase (total-cost) 10))
      (when (priority ?s pr11) (increase (total-cost) 11))
      (when (priority ?s pr12) (increase (total-cost) 12))
      (when (priority ?s pr13) (increase (total-cost) 13))
      (when (priority ?s pr14) (increase (total-cost) 14))
      (when (priority ?s pr15) (increase (total-cost) 15))
      (when (priority ?s pr16) (increase (total-cost) 16))
    )
  )
)