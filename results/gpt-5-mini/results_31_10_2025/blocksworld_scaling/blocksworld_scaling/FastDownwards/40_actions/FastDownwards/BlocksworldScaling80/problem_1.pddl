(define (problem two-agent-blocks-problem)
  (:domain two-agent-blocks)

  (:objects
    A B C D E F G H I J K L M N O - block
    table - table
  )

  (:init
    ;; Initial "on" relations (as provided)
    (on F table)
    (on A table)
    (on L A)
    (on B table)
    (on E L)
    (on H F)
    (on I H)
    (on G I)
    (on D table)
    (on J E)
    (on N D)
    (on M G)
    (on K B)
    (on C J)
    (on O C)

    ;; Initial clear blocks (top of each stack)
    (clear O)
    (clear K)
    (clear M)
    (clear N)

    ;; Vowel / Consonant classification
    (vowel A)
    (vowel E)
    (vowel I)
    (vowel O)

    (consonant F)
    (consonant L)
    (consonant B)
    (consonant H)
    (consonant G)
    (consonant D)
    (consonant J)
    (consonant N)
    (consonant M)
    (consonant K)
    (consonant C)
  )

  ;; Goal: A on B, B on C, ..., N on O, O on table (the specified single chain)
  (:goal (and
    (on A B)
    (on B C)
    (on C D)
    (on D E)
    (on E F)
    (on F G)
    (on G H)
    (on H I)
    (on I J)
    (on J K)
    (on K L)
    (on L M)
    (on M N)
    (on N O)
    (on O table)
  ))
)