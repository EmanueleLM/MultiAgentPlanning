(define (domain multiagent-hanoi-3agents)
  (:requirements :typing :negative-preconditions)
  (:types disk peg agent)

  (:predicates
    (onpeg ?d - disk ?p - peg)
    (below-disk ?d - disk ?d2 - disk)
    (below-peg ?d - disk ?p - peg)
    (topdisk ?d - disk)
    (empty ?p - peg)
    (canmove ?a - agent ?d - disk)
    (smaller ?d1 - disk ?d2 - disk)
  )

  ;; agent 1 actions
  (:action agent1-move-from-disk-to-disk
    :parameters (?a - agent ?d - disk ?b - disk ?frompeg - peg ?t - disk ?topeg - peg)
    :precondition (and
      (canmove ?a ?d)
      (topdisk ?d)
      (below-disk ?d ?b)
      (onpeg ?d ?frompeg)
      (topdisk ?t)
      (onpeg ?t ?topeg)
      (smaller ?d ?t)
    )
    :effect (and
      (not (topdisk ?d))
      (not (below-disk ?d ?b))
      (below-disk ?d ?t)
      (not (topdisk ?t))
      (topdisk ?b)
      (not (onpeg ?d ?frompeg))
      (onpeg ?d ?topeg)
      (not (empty ?topeg))
    )
  )

  (:action agent1-move-from-disk-to-empty
    :parameters (?a - agent ?d - disk ?b - disk ?frompeg - peg ?topeg - peg)
    :precondition (and
      (canmove ?a ?d)
      (topdisk ?d)
      (below-disk ?d ?b)
      (onpeg ?d ?frompeg)
      (empty ?topeg)
    )
    :effect (and
      (not (topdisk ?d))
      (not (below-disk ?d ?b))
      (below-peg ?d ?topeg)
      (topdisk ?b)
      (not (onpeg ?d ?frompeg))
      (onpeg ?d ?topeg)
      (not (empty ?topeg))
    )
  )

  (:action agent1-move-from-peg-to-disk
    :parameters (?a - agent ?d - disk ?frompeg - peg ?t - disk ?topeg - peg)
    :precondition (and
      (canmove ?a ?d)
      (topdisk ?d)
      (below-peg ?d ?frompeg)
      (onpeg ?d ?frompeg)
      (topdisk ?t)
      (onpeg ?t ?topeg)
      (smaller ?d ?t)
    )
    :effect (and
      (not (topdisk ?d))
      (not (below-peg ?d ?frompeg))
      (below-disk ?d ?t)
      (not (topdisk ?t))
      (not (onpeg ?d ?frompeg))
      (onpeg ?d ?topeg)
      (empty ?frompeg)
    )
  )

  (:action agent1-move-from-peg-to-empty
    :parameters (?a - agent ?d - disk ?frompeg - peg ?topeg - peg)
    :precondition (and
      (canmove ?a ?d)
      (topdisk ?d)
      (below-peg ?d ?frompeg)
      (onpeg ?d ?frompeg)
      (empty ?topeg)
    )
    :effect (and
      (not (topdisk ?d))
      (not (below-peg ?d ?frompeg))
      (below-peg ?d ?topeg)
      (not (onpeg ?d ?frompeg))
      (onpeg ?d ?topeg)
      (not (empty ?topeg))
      (empty ?frompeg)
    )
  )

