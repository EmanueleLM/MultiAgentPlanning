(define (domain calendar_scheduling_example29)
  (:requirements :strips :typing :negative-preconditions)
  (:types person timeslot)
  (:predicates
    (next ?s1 - timeslot ?s2 - timeslot)
    (free ?p - person ?s - timeslot)
    (role1 ?p - person)
    (role2 ?p - person)
    (role3 ?p - person)
    (scheduled ?s1 - timeslot ?s2 - timeslot)
    (meeting-scheduled)
  )

  (:action schedule_meeting
    :parameters (?p1 - person ?p2 - person ?p3 - person ?s1 - timeslot ?s2 - timeslot)
    :precondition (and
      (role1 ?p1)
      (role2 ?p2)
      (role3 ?p3)
      (next ?s1 ?s2)
      (free ?p1 ?s1) (free ?p1 ?s2)
      (free ?p2 ?s1) (free ?p2 ?s2)
      (free ?p3 ?s1) (free ?p3 ?s2)
      (not (meeting-scheduled))
    )
    :effect (and
      (meeting-scheduled)
      (scheduled ?s1 ?s2)
    )
  )
)