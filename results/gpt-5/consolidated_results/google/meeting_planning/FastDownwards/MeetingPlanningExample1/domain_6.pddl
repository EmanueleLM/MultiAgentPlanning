(define (domain sf_meet_betty_symbolic)
  (:requirements :strips :typing :negative-preconditions)
  (:types slot offset mlevel)
  (:predicates
    (at-richmond)
    (at-financial)
    (cur-slot ?s - slot)
    (cur-off ?o - offset)
    (met ?m - mlevel)
    (next-slot ?s1 - slot ?s2 - slot)
    (next-3slot ?s1 - slot ?s2 - slot)
    (next-4slot ?s1 - slot ?s2 - slot)
    (next-12slot ?s1 - slot ?s2 - slot)
    (next-off1 ?o1 - offset ?o2 - offset)
    (next-off2 ?o1 - offset ?o2 - offset)
    (next-m15 ?m1 - mlevel ?m2 - mlevel)
    (next-m60 ?m1 - mlevel ?m2 - mlevel)
    (allowed-15 ?s - slot ?o - offset)
    (allowed-60 ?s - slot ?o - offset)
    (deadline-ok ?s - slot ?o - offset)
  )

  (:action travel-richmond-financial
    :parameters (?s1 - slot ?s2 - slot ?o1 - offset ?o2 - offset)
    :precondition (and
      (at-richmond)
      (not (at-financial))
      (cur-slot ?s1)
      (cur-off ?o1)
      (next-4slot ?s1 ?s2)
      (next-off2 ?o1 ?o2)
      (deadline-ok ?s2 ?o2)
    )
    :effect (and
      (not (at-richmond))
      (at-financial)
      (not (cur-slot ?s1))
      (cur-slot ?s2)
      (not (cur-off ?o1))
      (cur-off ?o2)
    )
  )

  (:action travel-financial-richmond
    :parameters (?s1 - slot ?s2 - slot ?o1 - offset ?o2 - offset)
    :precondition (and
      (at-financial)
      (not (at-richmond))
      (cur-slot ?s1)
      (cur-off ?o1)
      (next-4slot ?s1 ?s2)
      (next-off1 ?o1 ?o2)
      (deadline-ok ?s2 ?o2)
    )
    :effect (and
      (not (at-financial))
      (at-richmond)
      (not (cur-slot ?s1))
      (cur-slot ?s2)
      (not (cur-off ?o1))
      (cur-off ?o2)
    )
  )

  (:action wait-5
    :parameters (?s1 - slot ?s2 - slot ?o - offset)
    :precondition (and
      (cur-slot ?s1)
      (cur-off ?o)
      (next-slot ?s1 ?s2)
      (deadline-ok ?s2 ?o)
    )
    :effect (and
      (not (cur-slot ?s1))
      (cur-slot ?s2)
    )
  )

  (:action meet-betty-15
    :parameters (?s1 - slot ?s2 - slot ?o - offset ?m1 - mlevel ?m2 - mlevel)
    :precondition (and
      (at-financial)
      (cur-slot ?s1)
      (cur-off ?o)
      (allowed-15 ?s1 ?o)
      (next-3slot ?s1 ?s2)
      (deadline-ok ?s2 ?o)
      (met ?m1)
      (next-m15 ?m1 ?m2)
    )
    :effect (and
      (not (cur-slot ?s1))
      (cur-slot ?s2)
      (not (met ?m1))
      (met ?m2)
    )
  )

  (:action meet-betty-60
    :parameters (?s1 - slot ?s2 - slot ?o - offset ?m1 - mlevel ?m2 - mlevel)
    :precondition (and
      (at-financial)
      (cur-slot ?s1)
      (cur-off ?o)
      (allowed-60 ?s1 ?o)
      (next-12slot ?s1 ?s2)
      (deadline-ok ?s2 ?o)
      (met ?m1)
      (next-m60 ?m1 ?m2)
    )
    :effect (and
      (not (cur-slot ?s1))
      (cur-slot ?s2)
      (not (met ?m1))
      (met ?m2)
    )
  )
)