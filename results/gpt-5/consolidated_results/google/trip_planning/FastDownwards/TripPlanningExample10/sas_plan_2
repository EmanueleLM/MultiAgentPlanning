(set-first-day d1 oslo)
(set-next-day d1 d2 oslo oslo)
(set-next-day d2 d3 oslo oslo)
(set-next-day d3 d4 oslo dublin)
(set-next-day d4 d5 dublin valencia)
(set-next-day d5 d6 valencia valencia)
(set-next-day d6 d7 valencia valencia)
(set-next-day d7 d8 valencia valencia)
(set-next-day d8 d9 valencia valencia)
; cost = 9 (unit cost)
