(define (domain two-agent-hanoi)
  (:requirements :strips :typing :negative-preconditions)
  (:types disk peg stage)
  (:predicates
    (on-peg ?d - disk ?p - peg)
    (on-disk ?d - disk ?u - disk)
    (clear-disk ?d - disk)
    (clear-peg ?p - peg)
    (smaller ?d1 - disk ?d2 - disk)
    (a1disk ?d - disk)
    (a2disk ?d - disk)
    (frozen ?d - disk)
    (at-stage ?s - stage)
    (next ?s - stage ?t - stage)
    (diffdisk ?x - disk ?y - disk)
    (diffpeg ?p - peg ?q - peg)
  )

  (:action move-a1-from-peg-to-peg
    :parameters (?d - disk ?p1 - peg ?p2 - peg ?s - stage ?t - stage)
    :precondition (and
      (a1disk ?d)
      (on-peg ?d ?p1)
      (clear-disk ?d)
      (clear-peg ?p2)
      (diffpeg ?p1 ?p2)
      (at-stage ?s) (next ?s ?t)
    )
    :effect (and
      (not (on-peg ?d ?p1))
      (on-peg ?d ?p2)
      (clear-peg ?p1)
      (not (clear-peg ?p2))
      (not (at-stage ?s))
      (at-stage ?t)
    )
  )

  (:action move-a1-from-peg-to-disk
    :parameters (?d - disk ?p1 - peg ?u - disk ?s - stage ?t - stage)
    :precondition (and
      (a1disk ?d)
      (on-peg ?d ?p1)
      (clear-disk ?d)
      (clear-disk ?u)
      (smaller ?d ?u)
      (at-stage ?s) (next ?s ?t)
    )
    :effect (and
      (not (on-peg ?d ?p1))
      (on-disk ?d ?u)
      (clear-peg ?p1)
      (not (clear-disk ?u))
      (not (at-stage ?s))
      (at-stage ?t)
    )
  )

  (:action move-a1-from-disk-to-peg
    :parameters (?d - disk ?u - disk ?p2 - peg ?s - stage ?t - stage)
    :precondition (and
      (a1disk ?d)
      (on-disk ?d ?u)
      (clear-disk ?d)
      (clear-peg ?p2)
      (at-stage ?s) (next ?s ?t)
    )
    :effect (and
      (not (on-disk ?d ?u))
      (on-peg ?d ?p2)
      (clear-disk ?u)
      (not (clear-peg ?p2))
      (not (at-stage ?s))
      (at-stage ?t)
    )
  )

  (:action move-a1-from-disk-to-disk
    :parameters (?d - disk ?u - disk ?v - disk ?s - stage ?t - stage)
    :precondition (and
      (a1disk ?d)
      (on-disk ?d ?u)
      (clear-disk ?d)
      (clear-disk ?v)
      (smaller ?d ?v)
      (diffdisk ?u ?v)
      (at-stage ?s) (next ?s ?t)
    )
    :effect (and
      (not (on-disk ?d ?u))
      (on-disk ?d ?v)
      (clear-disk ?u)
      (not (clear-disk ?v))
      (not (at-stage ?s))
      (at-stage ?t)
    )
  )

  (:action move-a2-from-peg-to-peg
    :parameters (?d - disk ?p1 - peg ?p2 - peg ?s - stage ?t - stage)
    :precondition (and
      (a2disk ?d)
      (not (frozen ?d))
      (on-peg ?d ?p1)
      (clear-disk ?d)
      (clear-peg ?p2)
      (diffpeg ?p1 ?p2)
      (at-stage ?s) (next ?s ?t)
    )
    :effect (and
      (not (on-peg ?d ?p1))
      (on-peg ?d ?p2)
      (clear-peg ?p1)
      (not (clear-peg ?p2))
      (not (at-stage ?s))
      (at-stage ?t)
    )
  )

  (:action move-a2-from-peg-to-disk
    :parameters (?d - disk ?p1 - peg ?u - disk ?s - stage ?t - stage)
    :precondition (and
      (a2disk ?d)
      (not (frozen ?d))
      (on-peg ?d ?p1)
      (clear-disk ?d)
      (clear-disk ?u)
      (smaller ?d ?u)
      (at-stage ?s) (next ?s ?t)
    )
    :effect (and
      (not (on-peg ?d ?p1))
      (on-disk ?d ?u)
      (clear-peg ?p1)
      (not (clear-disk ?u))
      (not (at-stage ?s))
      (at-stage ?t)
    )
  )

  (:action move-a2-from-disk-to-peg
    :parameters (?d - disk ?u - disk ?p2 - peg ?s - stage ?t - stage)
    :precondition (and
      (a2disk ?d)
      (not (frozen ?d))
      (on-disk ?d ?u)
      (clear-disk ?d)
      (clear-peg ?p2)
      (at-stage ?s) (next ?s ?t)
    )
    :effect (and
      (not (on-disk ?d ?u))
      (on-peg ?d ?p2)
      (clear-disk ?u)
      (not (clear-peg ?p2))
      (not (at-stage ?s))
      (at-stage ?t)
    )
  )

  (:action move-a2-from-disk-to-disk
    :parameters (?d - disk ?u - disk ?v - disk ?s - stage ?t - stage)
    :precondition (and
      (a2disk ?d)
      (not (frozen ?d))
      (on-disk ?d ?u)
      (clear-disk ?d)
      (clear-disk ?v)
      (smaller ?d ?v)
      (diffdisk ?u ?v)
      (at-stage ?s) (next ?s ?t)
    )
    :effect (and
      (not (on-disk ?d ?u))
      (on-disk ?d ?v)
      (clear-disk ?u)
      (not (clear-disk ?v))
      (not (at-stage ?s))
      (at-stage ?t)
    )
  )
)