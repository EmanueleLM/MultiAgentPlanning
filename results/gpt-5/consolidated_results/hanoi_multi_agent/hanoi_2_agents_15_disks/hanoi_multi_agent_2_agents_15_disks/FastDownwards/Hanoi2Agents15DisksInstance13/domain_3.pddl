(define (domain hanoi-multiagent)
  (:requirements :strips :typing :negative-preconditions)
  (:types
    agent
    place
    disk peg - place
    stage
  )
  (:constants
    agent_1 agent_2 - agent
  )
  (:predicates
    (on ?d - disk ?s - place)
    (clear ?s - place)
    (smaller ?x - disk ?y - disk)
    (can-move ?a - agent ?d - disk)
    (base ?d - disk ?p - peg)
    (at-stage ?t - stage)
    (next ?t - stage ?t2 - stage)
    (allowed-step ?t - stage ?d - disk ?from - peg ?to - peg)
  )

  (:action move-agent_1-to-peg
    :parameters (?t - stage ?t2 - stage ?d - disk ?from - place ?to - peg ?frompeg - peg)
    :precondition (and
      (can-move agent_1 ?d)
      (at-stage ?t)
      (next ?t ?t2)
      (allowed-step ?t ?d ?frompeg ?to)
      (on ?d ?from)
      (clear ?d)
      (clear ?to)
      (base ?d ?frompeg)
    )
    :effect (and
      (not (on ?d ?from))
      (on ?d ?to)
      (clear ?from)
      (not (clear ?to))
      (not (base ?d left))
      (not (base ?d middle))
      (not (base ?d right))
      (base ?d ?to)
      (not (at-stage ?t))
      (at-stage ?t2)
    )
  )

  (:action move-agent_1-to-disk
    :parameters (?t - stage ?t2 - stage ?d - disk ?from - place ?to - disk ?frompeg - peg ?topeg - peg)
    :precondition (and
      (can-move agent_1 ?d)
      (at-stage ?t)
      (next ?t ?t2)
      (allowed-step ?t ?d ?frompeg ?topeg)
      (on ?d ?from)
      (clear ?d)
      (clear ?to)
      (smaller ?d ?to)
      (base ?d ?frompeg)
      (base ?to ?topeg)
    )
    :effect (and
      (not (on ?d ?from))
      (on ?d ?to)
      (clear ?from)
      (not (clear ?to))
      (not (base ?d left))
      (not (base ?d middle))
      (not (base ?d right))
      (base ?d ?topeg)
      (not (at-stage ?t))
      (at-stage ?t2)
    )
  )

  (:action move-agent_2-to-peg
    :parameters (?t - stage ?t2 - stage ?d - disk ?from - place ?to - peg ?frompeg - peg)
    :precondition (and
      (can-move agent_2 ?d)
      (at-stage ?t)
      (next ?t ?t2)
      (allowed-step ?t ?d ?frompeg ?to)
      (on ?d ?from)
      (clear ?d)
      (clear ?to)
      (base ?d ?frompeg)
    )
    :effect (and
      (not (on ?d ?from))
      (on ?d ?to)
      (clear ?from)
      (not (clear ?to))
      (not (base ?d left))
      (not (base ?d middle))
      (not (base ?d right))
      (base ?d ?to)
      (not (at-stage ?t))
      (at-stage ?t2)
    )
  )

  (:action move-agent_2-to-disk
    :parameters (?t - stage ?t2 - stage ?d - disk ?from - place ?to - disk ?frompeg - peg ?topeg - peg)
    :precondition (and
      (can-move agent_2 ?d)
      (at-stage ?t)
      (next ?t ?t2)
      (allowed-step ?t ?d ?frompeg ?topeg)
      (on ?d ?from)
      (clear ?d)
      (clear ?to)
      (smaller ?d ?to)
      (base ?d ?frompeg)
      (base ?to ?topeg)
    )
    :effect (and
      (not (on ?d ?from))
      (on ?d ?to)
      (clear ?from)
      (not (clear ?to))
      (not (base ?d left))
      (not (base ?d middle))
      (not (base ?d right))
      (base ?d ?topeg)
      (not (at-stage ?t))
      (at-stage ?t2)
    )
  )
)