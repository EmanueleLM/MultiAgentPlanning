      (clear ?d)
      (on ?d ?below)
      (in-peg ?d ?from)
      (in-peg ?below ?from)
      (not (clear-peg ?to))
      (clear ?top)
      (in-peg ?top ?to)
      (smaller ?d ?top)
    )
    :effect (and
      (not (current-step ?s))
      (current-step ?t)
      (not (on ?d ?below))
