(define (problem Hanoi3Agents10DisksInstance3)
  (:domain hanoi3agents10disks)
  (:objects
    a b c d e f g h i j - disk
    left middle right - peg
    agent_1 agent_2 agent_3 - agent
  )
  (:init
    ; initial stack A..J on left (A smallest, J largest)
    (on a b)
    (on b c)
    (on c d)
    (on d e)
    (on e f)
    (on f g)
    (on g h)
    (on h i)
    (on i j)
    (on j left)

    ; clear tops and empty pegs
    (clear a)
    (clear middle)
    (clear right)

    ; full smaller relations (first is smaller than second)
    (smaller a b)
    (smaller a c)
    (smaller a d)
    (smaller a e)
    (smaller a f)
