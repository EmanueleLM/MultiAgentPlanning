  (:goal
    (and
      ; left: D E F G H I J on left
      (on d e)
      (on e f)
      (on f g)
      (on g h)
      (on h i)
      (on i j)
      (on j left)

      ; middle: A on middle
      (on a middle)

      ; right: B on C on right
      (on b c)
      (on c right)

      ; tops clear
      (clear a)
      (clear b)
    )
  )
