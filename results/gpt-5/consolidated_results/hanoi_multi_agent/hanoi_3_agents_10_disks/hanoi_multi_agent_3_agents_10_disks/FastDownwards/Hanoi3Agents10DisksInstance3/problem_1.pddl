    (ok-target i left)
    (ok-target i middle)
    (ok-target i right)
    (ok-target j left)
    (ok-target j middle)
    (ok-target j right)

    ; ok-target: a disk may be placed on any strictly larger disk
    (ok-target a b)
    (ok-target a c)
    (ok-target a d)
    (ok-target a e)
    (ok-target a f)
    (ok-target a g)
    (ok-target a h)
