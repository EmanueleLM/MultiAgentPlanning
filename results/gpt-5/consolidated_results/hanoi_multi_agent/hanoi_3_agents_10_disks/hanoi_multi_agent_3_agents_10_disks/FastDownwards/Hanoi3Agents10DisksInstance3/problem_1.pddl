    (smaller f j)
    (smaller g h)
    (smaller g i)
    (smaller g j)
    (smaller h i)
    (smaller h j)
    (smaller i j)

    ; agent permissions
    (allowed agent_1 a)
    (allowed agent_1 b)
    (allowed agent_1 c)
    (allowed agent_1 d)
    (allowed agent_2 e)
    (allowed agent_2 f)
    (allowed agent_2 g)
    (allowed agent_3 h)
    (allowed agent_3 i)
    (allowed agent_3 j)

    ; ok-target: any peg is a valid target for any disk
    (ok-target a left)
    (ok-target a middle)
    (ok-target a right)
    (ok-target b left)
    (ok-target b middle)
    (ok-target b right)
    (ok-target c left)
    (ok-target c middle)
    (ok-target c right)
    (ok-target d left)
