(define (problem Hanoi3Agents10DisksInstance4)
  (:domain multi-agent-hanoi)
  (:objects
    a b c d e f g h i j - disk
    left middle right - peg
  )
  (:init
    ; initial stacking (top to bottom on left)
    (on a b)
    (on b c)
    (on c d)
    (on d e)
    (on e f)
    (on f g)
    (on g h)
    (on h i)
    (on i j)
    (on j left)

    ; clear places at start
    (clear a)
    (clear middle)
    (clear right)

    ; agent permissions
    (permitted-a1 a)
    (permitted-a1 b)
    (permitted-a1 c)
    (permitted-a1 d)

    (permitted-a2 e)
    (permitted-a2 f)
    (permitted-a2 g)

    (permitted-a3 h)
    (permitted-a3 i)
    (permitted-a3 j)

    ; size ordering: disks over larger disks
    (smaller a b) (smaller a c) (smaller a d) (smaller a e) (smaller a f) (smaller a g) (smaller a h) (smaller a i) (smaller a j)
    (smaller b c) (smaller b d) (smaller b e) (smaller b f) (smaller b g) (smaller b h) (smaller b i) (smaller b j)
    (smaller c d) (smaller c e) (smaller c f) (smaller c g) (smaller c h) (smaller c i) (smaller c j)
    (smaller d e) (smaller d f) (smaller d g) (smaller d h) (smaller d i) (smaller d j)
    (smaller e f) (smaller e g) (smaller e h) (smaller e i) (smaller e j)
    (smaller f g) (smaller f h) (smaller f i) (smaller f j)
    (smaller g h) (smaller g i) (smaller g j)
    (smaller h i) (smaller h j)
    (smaller i j)

    ; size ordering: every disk is smaller than any peg (placing on empty peg is always OK)
    (smaller a left) (smaller a middle) (smaller a right)
    (smaller b left) (smaller b middle) (smaller b right)
    (smaller c left) (smaller c middle) (smaller c right)
    (smaller d left) (smaller d middle) (smaller d right)
    (smaller e left) (smaller e middle) (smaller e right)
    (smaller f left) (smaller f middle) (smaller f right)
    (smaller g left) (smaller g middle) (smaller g right)
    (smaller h left) (smaller h middle) (smaller h right)
    (smaller i left) (smaller i middle) (smaller i right)
    (smaller j left) (smaller j middle) (smaller j right)
  )
  (:goal (and
    (on a b)
    (on b d)
    (on d e)
    (on e f)
    (on f g)
    (on g h)
    (on h i)
    (on i j)
    (on j left)
    (on c middle)
    (clear a)
    (clear c)
    (clear right)
  ))
)