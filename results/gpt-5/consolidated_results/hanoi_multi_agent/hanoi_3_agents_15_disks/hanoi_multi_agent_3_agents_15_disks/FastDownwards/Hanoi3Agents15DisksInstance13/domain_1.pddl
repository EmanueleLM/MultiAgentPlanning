(define (domain hanoi3agents15disks)
  (:requirements :strips :typing :negative-preconditions)
  (:types agent disk peg)
  (:predicates
    (can-move ?a - agent ?d - disk)
    (smaller ?x - disk ?y - disk)
    (on ?x - disk ?y - disk)
    (on-peg ?x - disk ?p - peg)
    (clear ?x - disk)
    (top ?p - peg ?x - disk)
    (peg-clear ?p - peg)
  )

  (:action move_from_peg_to_empty
    :parameters (?a - agent ?x - disk ?p1 - peg ?p2 - peg)
    :precondition (and
      (can-move ?a ?x)
      (on-peg ?x ?p1)
      (top ?p1 ?x)
      (clear ?x)
      (peg-clear ?p2)
    )
    :effect (and
      (not (on-peg ?x ?p1))
      (peg-clear ?p1)
      (on-peg ?x ?p2)
      (not (peg-clear ?p2))
      (not (top ?p1 ?x))
      (top ?p2 ?x)
    )
  )

  (:action move_from_disk_to_empty
    :parameters (?a - agent ?x - disk ?y - disk ?p1 - peg ?p2 - peg)
    :precondition (and
      (can-move ?a ?x)
      (on ?x ?y)
      (clear ?x)
      (top ?p1 ?x)
      (peg-clear ?p2)
    )
    :effect (and
      (not (on ?x ?y))
      (clear ?y)
      (on-peg ?x ?p2)
      (not (peg-clear ?p2))
      (not (top ?p1 ?x))
      (top ?p2 ?x)
      (top ?p1 ?y)
    )
  )

  (:action move_from_peg_to_disk
    :parameters (?a - agent ?x - disk ?p1 - peg ?p2 - peg ?t - disk)
    :precondition (and
      (can-move ?a ?x)
      (on-peg ?x ?p1)
      (top ?p1 ?x)
      (clear ?x)
      (top ?p2 ?t)
      (clear ?t)
      (smaller ?x ?t)
    )
    :effect (and
      (not (on-peg ?x ?p1))
      (peg-clear ?p1)
      (on ?x ?t)
      (not (clear ?t))
      (not (top ?p1 ?x))
      (not (top ?p2 ?t))
      (top ?p2 ?x)
    )
  )

  (:action move_from_disk_to_disk
    :parameters (?a - agent ?x - disk ?y - disk ?p1 - peg ?p2 - peg ?t - disk)
    :precondition (and
      (can-move ?a ?x)
      (on ?x ?y)
      (clear ?x)
      (top ?p1 ?x)
      (top ?p2 ?t)
      (clear ?t)
      (smaller ?x ?t)
    )
    :effect (and
      (not (on ?x ?y))
      (clear ?y)
      (on ?x ?t)
      (not (clear ?t))
      (not (top ?p1 ?x))
      (top ?p2 ?x)
      (top ?p1 ?y)
      (not (top ?p2 ?t))
    )
  )
)