(define (domain hanoi-3agents-15-ordered)
  (:requirements :strips :typing)
  (:types disk peg step)
  (:predicates
    (smaller ?x - disk ?y - disk)
    (on ?x - disk ?y - disk)
    (onpeg ?x - disk ?p - peg)
    (clear ?x - disk)
    (clearpeg ?p - peg)
    (top ?p - peg ?x - disk)
    (current-step ?s - step)
    (succ ?s1 - step ?s2 - step)
    (allowed ?d - disk ?from - peg ?to - peg ?s - step)
  )

  ; Move top disk from a disk to an empty peg
  (:action move-from-disk-to-empty
    :parameters (?d - disk ?below - disk ?from - peg ?to - peg ?s - step ?s2 - step)
    :precondition (and
      (current-step ?s)
      (succ ?s ?s2)
      (allowed ?d ?from ?to ?s)
      (top ?from ?d)
      (on ?d ?below)
      (clear ?d)
      (clearpeg ?to)
    )
    :effect (and
      (not (on ?d ?below))
      (clear ?below)
      (not (top ?from ?d))
      (top ?from ?below)
      (not (clearpeg ?to))
      (onpeg ?d ?to)
      (top ?to ?d)
      (not (current-step ?s))
      (current-step ?s2)
    )
  )

  ; Move top disk from bottom of peg to an empty peg (only when peg has single disk)
  (:action move-from-peg-to-empty
    :parameters (?d - disk ?from - peg ?to - peg ?s - step ?s2 - step)
    :precondition (and
      (current-step ?s)
      (succ ?s ?s2)
      (allowed ?d ?from ?to ?s)
      (top ?from ?d)
      (onpeg ?d ?from)
      (clear ?d)
      (clearpeg ?to)
    )
    :effect (and
      (not (onpeg ?d ?from))
      (clearpeg ?from)
      (not (top ?from ?d))
      (not (clearpeg ?to))
      (onpeg ?d ?to)
      (top ?to ?d)
      (not (current-step ?s))
      (current-step ?s2)
    )
  )

  ; Move top disk from a disk to the top of a non-empty peg
  (:action move-from-disk-to-disk
    :parameters (?d - disk ?below - disk ?from - peg ?to - peg ?dsttop - disk ?s - step ?s2 - step)
    :precondition (and
      (current-step ?s)
      (succ ?s ?s2)
      (allowed ?d ?from ?to ?s)
      (top ?from ?d)
      (on ?d ?below)
      (clear ?d)
      (top ?to ?dsttop)
      (clear ?dsttop)
      (smaller ?d ?dsttop)
    )
    :effect (and
      (not (on ?d ?below))
      (clear ?below)
      (not (top ?from ?d))
      (top ?from ?below)
      (on ?d ?dsttop)
      (not (top ?to ?dsttop))
      (top ?to ?d)
      (not (clear ?dsttop))
      (not (current-step ?s))
      (current-step ?s2)
    )
  )

  ; Move top disk from bottom of peg to the top of a non-empty peg (only when peg has single disk)
  (:action move-from-peg-to-disk
    :parameters (?d - disk ?from - peg ?to - peg ?dsttop - disk ?s - step ?s2 - step)
    :precondition (and
      (current-step ?s)
      (succ ?s ?s2)
      (allowed ?d ?from ?to ?s)
      (top ?from ?d)
      (onpeg ?d ?from)
      (clear ?d)
      (top ?to ?dsttop)
      (clear ?dsttop)
      (smaller ?d ?dsttop)
    )
    :effect (and
      (not (onpeg ?d ?from))
      (clearpeg ?from)
      (not (top ?from ?d))
      (on ?d ?dsttop)
      (not (top ?to ?dsttop))
      (top ?to ?d)
      (not (clear ?dsttop))
      (not (current-step ?s))
      (current-step ?s2)
    )
  )
)