(define (domain hanoi-3agents-15disks)
  (:requirements :strips :typing :negative-preconditions)
  (:types
    agent disk peg
  )
  (:predicates
    (smaller ?x - disk ?y - disk)
    (succ ?x - disk ?y - disk)
    (on ?d - disk ?s - (either disk peg))
    (clear ?s - (either disk peg))
    (on-peg ?s - (either disk peg) ?p - peg)
    (top ?d - disk ?p - peg)
    (can-move ?a - agent ?d - disk)
  )

  (:action move-to-peg
    :parameters (?a - agent ?d - disk ?from - (either disk peg) ?to - peg ?pf - peg ?u - disk)
    :precondition (and
      (can-move ?a ?d)
      (on ?d ?from)
      (on-peg ?from ?pf)
      (on-peg ?d ?pf)
      (top ?d ?pf)
      (clear ?d)
      (clear ?to)
      (succ ?d ?u)
      (not (= ?pf ?to))
    )
    :effect (and
      (not (on ?d ?from))
      (on ?d ?to)
      (clear ?from)
      (not (clear ?to))
      (not (top ?d ?pf))
      (top ?d ?to)
      (not (on-peg ?d ?pf))
      (on-peg ?d ?to)
      (top ?u ?pf)
    )
  )

  (:action move-to-disk
    :parameters (?a - agent ?d - disk ?from - (either disk peg) ?to - disk ?pf - peg ?pt - peg ?u - disk)
    :precondition (and
      (can-move ?a ?d)
      (on ?d ?from)
      (on-peg ?from ?pf)
      (on-peg ?d ?pf)
      (top ?d ?pf)
      (clear ?d)
      (clear ?to)
      (on-peg ?to ?pt)
      (smaller ?d ?to)
      (succ ?d ?u)
      (not (= ?pf ?pt))
    )
    :effect (and
      (not (on ?d ?from))
      (on ?d ?to)
      (clear ?from)
      (not (clear ?to))
      (not (top ?d ?pf))
      (top ?d ?pt)
      (not (top ?to ?pt))
      (not (on-peg ?d ?pf))
      (on-peg ?d ?pt)
      (top ?u ?pf)
    )
  )
)