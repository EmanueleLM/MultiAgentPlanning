(define (domain hanoi-3agents-15disks)
  (:requirements :strips :typing :negative-preconditions)
  (:types agent disk peg)
  (:predicates
    (smaller ?x - disk ?y - disk)
    (on ?d - disk ?below - disk)
    (on-peg ?d - disk ?p - peg)
    (at-peg ?d - disk ?p - peg)
    (clear-disk ?d - disk)
    (clear-peg ?p - peg)
    (can-move ?a - agent ?d - disk)
    (different-peg ?p1 - peg ?p2 - peg)
  )

  (:action move-from-disk-to-peg
    :parameters (?a - agent ?d - disk ?below - disk ?from - peg ?to - peg)
    :precondition (and
      (can-move ?a ?d)
      (on ?d ?below)
      (at-peg ?d ?from)
      (at-peg ?below ?from)
      (clear-disk ?d)
      (clear-peg ?to)
      (different-peg ?from ?to)
    )
    :effect (and
      (not (on ?d ?below))
      (clear-disk ?below)
      (on-peg ?d ?to)
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (not (clear-peg ?to))
    )
  )

  (:action move-from-disk-to-disk
    :parameters (?a - agent ?d - disk ?below - disk ?target - disk ?from - peg ?to - peg)
    :precondition (and
      (can-move ?a ?d)
      (on ?d ?below)
      (at-peg ?d ?from)
      (at-peg ?below ?from)
      (clear-disk ?d)
      (clear-disk ?target)
      (at-peg ?target ?to)
      (smaller ?d ?target)
      (different-peg ?from ?to)
    )
    :effect (and
      (not (on ?d ?below))
      (clear-disk ?below)
      (on ?d ?target)
      (not (clear-disk ?target))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
    )
  )

  (:action move-from-peg-to-peg
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg)
    :precondition (and
      (can-move ?a ?d)
      (on-peg ?d ?from)
      (at-peg ?d ?from)
      (clear-disk ?d)
      (clear-peg ?to)
      (different-peg ?from ?to)
    )
    :effect (and
      (not (on-peg ?d ?from))
      (on-peg ?d ?to)
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (not (clear-peg ?to))
      (clear-peg ?from)
    )
  )

  (:action move-from-peg-to-disk
    :parameters (?a - agent ?d - disk ?target - disk ?from - peg ?to - peg)
    :precondition (and
      (can-move ?a ?d)
      (on-peg ?d ?from)
      (at-peg ?d ?from)
      (clear-disk ?d)
      (clear-disk ?target)
      (at-peg ?target ?to)
      (smaller ?d ?target)
      (different-peg ?from ?to)
    )
    :effect (and
      (not (on-peg ?d ?from))
      (on ?d ?target)
      (not (clear-disk ?target))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (clear-peg ?from)
    )
  )
)