(define (domain hanoi_multiagent)
  (:requirements :strips :typing :negative-preconditions)
  (:types agent disk peg stage)
  (:predicates
    (on-peg ?d - disk ?p - peg)
    (on-disk ?d - disk ?below - disk)
    (clear ?d - disk)
    (clear-peg ?p - peg)
    (can-move ?a - agent ?d - disk)
    (task-disk ?d - disk)
    (smaller ?x - disk ?y - disk)
    (at ?d - disk ?p - peg)
    (current ?s - stage)
    (next ?s - stage ?ns - stage)
    (scheduled ?s - stage ?a - agent ?d - disk ?from - peg ?to - peg)
  )

  (:action move_from_disk_to_peg
    :parameters (?a - agent ?d - disk ?from - peg ?b - disk ?to - peg ?s - stage ?ns - stage)
    :precondition (and
      (can-move ?a ?d)
      (task-disk ?d)
      (scheduled ?s ?a ?d ?from ?to)
      (current ?s)
      (next ?s ?ns)
      (at ?d ?from)
      (on-disk ?d ?b)
      (clear ?d)
      (clear-peg ?to)
    )
    :effect (and
      (not (on-disk ?d ?b))
      (on-peg ?d ?to)
      (not (at ?d ?from))
      (at ?d ?to)
      (clear ?b)
      (not (clear-peg ?to))
      (not (current ?s))
      (current ?ns)
    )
  )

  (:action move_from_disk_to_disk
    :parameters (?a - agent ?d - disk ?from - peg ?b - disk ?to - peg ?t - disk ?s - stage ?ns - stage)
    :precondition (and
      (can-move ?a ?d)
      (task-disk ?d)
      (scheduled ?s ?a ?d ?from ?to)
      (current ?s)
      (next ?s ?ns)
      (at ?d ?from)
      (on-disk ?d ?b)
      (clear ?d)
      (at ?t ?to)
      (clear ?t)
      (smaller ?d ?t)
    )
    :effect (and
      (not (on-disk ?d ?b))
      (on-disk ?d ?t)
      (not (at ?d ?from))
      (at ?d ?to)
      (clear ?b)
      (not (clear ?t))
      (not (current ?s))
      (current ?ns)
    )
  )

  (:action move_from_peg_to_disk
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?t - disk ?s - stage ?ns - stage)
    :precondition (and
      (can-move ?a ?d)
      (task-disk ?d)
      (scheduled ?s ?a ?d ?from ?to)
      (current ?s)
      (next ?s ?ns)
      (at ?d ?from)
      (on-peg ?d ?from)
      (clear ?d)
      (at ?t ?to)
      (clear ?t)
      (smaller ?d ?t)
    )
    :effect (and
      (not (on-peg ?d ?from))
      (on-disk ?d ?t)
      (not (at ?d ?from))
      (at ?d ?to)
      (clear-peg ?from)
      (not (clear ?t))
      (not (current ?s))
      (current ?ns)
    )
  )
)