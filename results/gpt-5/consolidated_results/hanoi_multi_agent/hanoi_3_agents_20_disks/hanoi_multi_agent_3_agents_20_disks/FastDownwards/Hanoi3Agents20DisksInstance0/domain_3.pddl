(define (domain hanoi-3peg-20-staged)
  (:requirements :strips :typing :negative-preconditions)
  (:types disk peg stage)
  (:predicates
    (on ?x - disk ?y - disk)
    (on-peg ?d - disk ?p - peg)
    (clear ?x - disk)
    (top ?p - peg ?d - disk)
    (empty ?p - peg)
    (smaller ?x - disk ?y - disk)
    (diff ?p - peg ?q - peg)
    (current ?s - stage)
    (next ?s - stage ?t - stage)
  )

  (:action move-only-to-empty
    :parameters (?d - disk ?p1 - peg ?p2 - peg ?s - stage ?t - stage)
    :precondition (and
      (diff ?p1 ?p2)
      (top ?p1 ?d)
      (on-peg ?d ?p1)
      (clear ?d)
      (empty ?p2)
      (current ?s)
      (next ?s ?t)
    )
    :effect (and
      (not (on-peg ?d ?p1))
      (not (top ?p1 ?d))
      (empty ?p1)
      (not (empty ?p2))
      (top ?p2 ?d)
      (on-peg ?d ?p2)
      (clear ?d)
      (not (current ?s))
      (current ?t)
    )
  )

  (:action move-only-to-disk
    :parameters (?d - disk ?p1 - peg ?p2 - peg ?d2 - disk ?s - stage ?t - stage)
    :precondition (and
      (diff ?p1 ?p2)
      (top ?p1 ?d)
      (on-peg ?d ?p1)
      (clear ?d)
      (top ?p2 ?d2)
      (clear ?d2)
      (smaller ?d ?d2)
      (current ?s)
      (next ?s ?t)
    )
    :effect (and
      (not (on-peg ?d ?p1))
      (not (top ?p1 ?d))
      (empty ?p1)
      (not (top ?p2 ?d2))
      (on ?d ?d2)
      (top ?p2 ?d)
      (not (clear ?d2))
      (clear ?d)
      (not (current ?s))
      (current ?t)
    )
  )

  (:action move-from-disk-to-empty
    :parameters (?d - disk ?p1 - peg ?p2 - peg ?d1 - disk ?s - stage ?t - stage)
    :precondition (and
      (diff ?p1 ?p2)
      (top ?p1 ?d)
      (on ?d ?d1)
      (clear ?d)
      (empty ?p2)
      (current ?s)
      (next ?s ?t)
    )
    :effect (and
      (not (on ?d ?d1))
      (clear ?d1)
      (not (top ?p1 ?d))
      (top ?p1 ?d1)
      (not (empty ?p2))
      (top ?p2 ?d)
      (on-peg ?d ?p2)
      (clear ?d)
      (not (current ?s))
      (current ?t)
    )
  )

  (:action move-from-disk-to-disk
    :parameters (?d - disk ?p1 - peg ?p2 - peg ?d1 - disk ?d2 - disk ?s - stage ?t - stage)
    :precondition (and
      (diff ?p1 ?p2)
      (top ?p1 ?d)
      (on ?d ?d1)
      (clear ?d)
      (top ?p2 ?d2)
      (clear ?d2)
      (smaller ?d ?d2)
      (current ?s)
      (next ?s ?t)
    )
    :effect (and
      (not (on ?d ?d1))
      (clear ?d1)
      (not (top ?p1 ?d))
      (top ?p1 ?d1)
      (not (top ?p2 ?d2))
      (on ?d ?d2)
      (top ?p2 ?d)
      (not (clear ?d2))
      (clear ?d)
      (not (current ?s))
      (current ?t)
    )
  )
)