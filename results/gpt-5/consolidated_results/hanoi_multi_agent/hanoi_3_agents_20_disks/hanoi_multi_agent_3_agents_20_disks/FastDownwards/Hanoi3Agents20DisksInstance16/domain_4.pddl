(define (domain hanoi-3agent-20-fixedsteps)
  (:requirements :strips :typing :negative-preconditions)
  (:types
    agent
    support
    peg - support
    disk - support
    step
  )
  (:constants
    agent_1 agent_2 agent_3 - agent
  )
  (:predicates
    (on ?d - disk ?s - support)
    (clear ?s - support)
    (smaller ?x - support ?y - support)
    (at-peg ?s - support ?p - peg)
    (can-move ?a - agent ?d - disk)
    (current-step ?s - step)
    (next ?s1 - step ?s2 - step)
    (step-disk ?s - step ?d - disk)
    (step-from ?s - step ?p - peg)
    (step-to ?s - step ?p - peg)
    (step-below ?s - step ?b - support)
  )

  (:action move_agent_1
    :parameters (?d - disk ?from - peg ?to - peg ?sfrom - support ?below - support ?si - step ?sj - step)
    :precondition (and
      (current-step ?si)
      (next ?si ?sj)
      (step-disk ?si ?d)
      (step-from ?si ?from)
      (step-to ?si ?to)
      (step-below ?si ?below)
      (can-move agent_1 ?d)
      (on ?d ?sfrom)
      (at-peg ?d ?from)
      (clear ?d)
      (clear ?below)
      (at-peg ?below ?to)
      (smaller ?d ?below)
    )
    :effect (and
      (not (current-step ?si))
      (current-step ?sj)
      (not (on ?d ?sfrom))
      (on ?d ?below)
      (clear ?sfrom)
      (not (clear ?below))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
    )
  )

  (:action move_agent_2
    :parameters (?d - disk ?from - peg ?to - peg ?sfrom - support ?below - support ?si - step ?sj - step)
    :precondition (and
      (current-step ?si)
      (next ?si ?sj)
      (step-disk ?si ?d)
      (step-from ?si ?from)
      (step-to ?si ?to)
      (step-below ?si ?below)
      (can-move agent_2 ?d)
      (on ?d ?sfrom)
      (at-peg ?d ?from)
      (clear ?d)
      (clear ?below)
      (at-peg ?below ?to)
      (smaller ?d ?below)
    )
    :effect (and
      (not (current-step ?si))
      (current-step ?sj)
      (not (on ?d ?sfrom))
      (on ?d ?below)
      (clear ?sfrom)
      (not (clear ?below))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
    )
  )

  (:action move_agent_3
    :parameters (?d - disk ?from - peg ?to - peg ?sfrom - support ?below - support ?si - step ?sj - step)
    :precondition (and
      (current-step ?si)
      (next ?si ?sj)
      (step-disk ?si ?d)
      (step-from ?si ?from)
      (step-to ?si ?to)
      (step-below ?si ?below)
      (can-move agent_3 ?d)
      (on ?d ?sfrom)
      (at-peg ?d ?from)
      (clear ?d)
      (clear ?below)
      (at-peg ?below ?to)
      (smaller ?d ?below)
    )
    :effect (and
      (not (current-step ?si))
      (current-step ?sj)
      (not (on ?d ?sfrom))
      (on ?d ?below)
      (clear ?sfrom)
      (not (clear ?below))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
    )
  )
)