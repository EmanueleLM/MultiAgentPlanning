(define (domain MeetingPlanningExample24)
  (:requirements :strips :typing :negative-preconditions :action-costs)
  (:types location person time count)
  (:predicates
    (at ?l - location)
    (current-time ?t - time)
    (met ?p - person)
    (meeting-spot ?p - person ?l - location)
    (start-ok ?p - person ?t - time)
    (travel17 ?from - location ?to - location)
    (travel18 ?from - location ?to - location)
    (plus1 ?t1 - time ?t2 - time)
    (dec1 ?c1 - count ?c2 - count)
    (busy)
    (traveling ?from - location ?to - location)
    (meeting-with ?p - person ?l - location)
    (tcounter ?c - count)
    (mcounter ?c - count)
    (is-zero ?c - count)
    (is-17 ?c - count)
    (is-18 ?c - count)
    (is-30 ?c - count)
  )
  (:functions (total-cost))
  (:action wait-1
    :parameters (?t ?t2 - time ?l - location)
    :precondition (and (current-time ?t) (plus1 ?t ?t2) (at ?l) (not (busy)))
    :effect (and (not (current-time ?t)) (current-time ?t2) (increase (total-cost) 1))
  )
  (:action start-move-17
    :parameters (?from ?to - location ?c - count)
    :precondition (and (at ?from) (travel17 ?from ?to) (not (busy)) (is-17 ?c))
    :effect (and (not (at ?from)) (traveling ?from ?to) (busy) (tcounter ?c))
  )
  (:action start-move-18
    :parameters (?from ?to - location ?c - count)
    :precondition (and (at ?from) (travel18 ?from ?to) (not (busy)) (is-18 ?c))
    :effect (and (not (at ?from)) (traveling ?from ?to) (busy) (tcounter ?c))
  )
  (:action tick-travel-1
    :parameters (?t ?t2 - time ?c1 ?c2 - count)
    :precondition (and (busy) (current-time ?t) (plus1 ?t ?t2) (tcounter ?c1) (dec1 ?c1 ?c2))
    :effect (and (not (current-time ?t)) (current-time ?t2) (not (tcounter ?c1)) (tcounter ?c2) (increase (total-cost) 1))
  )
  (:action finish-move
    :parameters (?from ?to - location ?c - count)
    :precondition (and (busy) (traveling ?from ?to) (tcounter ?c) (is-zero ?c))
    :effect (and (not (traveling ?from ?to)) (not (busy)) (not (tcounter ?c)) (at ?to))
  )
  (:action start-meet-30
    :parameters (?p - person ?l - location ?t - time ?c - count)
    :precondition (and (at ?l) (meeting-spot ?p ?l) (not (met ?p)) (not (busy)) (current-time ?t) (start-ok ?p ?t) (is-30 ?c))
    :effect (and (busy) (meeting-with ?p ?l) (mcounter ?c))
  )
  (:action tick-meet-1
    :parameters (?t ?t2 - time ?c1 ?c2 - count ?p - person ?l - location)
    :precondition (and (busy) (meeting-with ?p ?l) (current-time ?t) (plus1 ?t ?t2) (mcounter ?c1) (dec1 ?c1 ?c2))
    :effect (and (not (current-time ?t)) (current-time ?t2) (not (mcounter ?c1)) (mcounter ?c2) (increase (total-cost) 1))
  )
  (:action finish-meet
    :parameters (?p - person ?l - location ?c - count)
    :precondition (and (busy) (meeting-with ?p ?l) (mcounter ?c) (is-zero ?c))
    :effect (and (met ?p) (not (busy)) (not (meeting-with ?p ?l)) (not (mcounter ?c)))
  )
)