(define (domain meeting-planner-sf)
  (:requirements :strips :typing :negative-preconditions :action-costs)
  (:types
    location person timepoint needlevel
  )
  (:predicates
    (at ?l - location)
    (person_at ?p - person ?l - location)
    (at-time ?t - timepoint)
    (next-10 ?t1 - timepoint ?t2 - timepoint)
    (next-22 ?t1 - timepoint ?t2 - timepoint)
    (next-23 ?t1 - timepoint ?t2 - timepoint)
    (meet-start-ok ?p - person ?t - timepoint)
    (remaining ?p - person ?r - needlevel)
    (next-need ?r1 - needlevel ?r2 - needlevel)
    (conn-22 ?from - location ?to - location)
    (conn-23 ?from - location ?to - location)
  )
  (:functions (total-cost))
  (:action travel-22
    :parameters (?from - location ?to - location ?t1 - timepoint ?t2 - timepoint)
    :precondition (and
      (at ?from)
      (at-time ?t1)
      (conn-22 ?from ?to)
      (next-22 ?t1 ?t2)
    )
    :effect (and
      (not (at ?from))
      (at ?to)
      (not (at-time ?t1))
      (at-time ?t2)
      (increase (total-cost) 22)
    )
  )
  (:action travel-23
    :parameters (?from - location ?to - location ?t1 - timepoint ?t2 - timepoint)
    :precondition (and
      (at ?from)
      (at-time ?t1)
      (conn-23 ?from ?to)
      (next-23 ?t1 ?t2)
    )
    :effect (and
      (not (at ?from))
      (at ?to)
      (not (at-time ?t1))
      (at-time ?t2)
      (increase (total-cost) 23)
    )
  )
  (:action meet-10
    :parameters (?p - person ?l - location ?t1 - timepoint ?t2 - timepoint ?r1 - needlevel ?r2 - needlevel)
    :precondition (and
      (at ?l)
      (person_at ?p ?l)
      (at-time ?t1)
      (next-10 ?t1 ?t2)
      (meet-start-ok ?p ?t1)
      (remaining ?p ?r1)
      (next-need ?r1 ?r2)
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
      (not (remaining ?p ?r1))
      (remaining ?p ?r2)
      (increase (total-cost) 10)
    )
  )
)