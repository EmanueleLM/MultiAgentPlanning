(define (problem trip-itinerary-problem)
  (:domain trip-itinerary)
  (:objects
    reykjavik vienna venice - city
    d1 d2 d3 d4 d5 d6 d7 d8 d9 d10 d11 - day
  )
  (:init
    (next d1 d2)
    (next d2 d3)
    (next d3 d4)
    (next d4 d5)
    (next d5 d6)
    (next d6 d7)
    (next d7 d8)
    (next d8 d9)
    (next d9 d10)
    (next d10 d11)

    (connected reykjavik vienna)
    (connected vienna reykjavik)
    (connected vienna venice)
    (connected venice vienna)

    (at d1 reykjavik)
    (filled d1)
  )
  (:goal (and
    (at d1 reykjavik)
    (at d2 reykjavik)
    (at d3 vienna)
    (at d4 vienna)
    (at d5 vienna)
    (at d6 vienna)
    (at d7 vienna)
    (at d8 venice)
    (at d9 venice)
    (at d10 venice)
    (at d11 venice)
  ))
