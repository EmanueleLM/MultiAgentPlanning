(assign_first d1 valencia)
(advance_same d1 d2 valencia)
(advance_same d2 d3 valencia)
(advance_same d3 d4 valencia)
(advance_same d4 d5 valencia)
(advance_fly d5 d6 valencia lyon)
(advance_same d6 d7 lyon)
(advance_same d7 d8 lyon)
(advance_same d8 d9 lyon)
(advance_fly d9 d10 lyon split)
(advance_same d10 d11 split)
(advance_same d11 d12 split)
(advance_same d12 d13 split)
(advance_same d13 d14 split)
(advance_same d14 d15 split)
(advance_same d15 d16 split)
; cost = 16 (unit cost)
