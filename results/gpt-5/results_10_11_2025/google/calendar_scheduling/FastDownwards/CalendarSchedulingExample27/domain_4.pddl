(define (domain calendar_scheduling_example27)
  (:requirements :typing :negative-preconditions)
  (:types participant timeslot)
  (:predicates
    (free ?p - participant ?t - timeslot)
    (workhour ?t - timeslot)
    (next ?t1 - timeslot ?t2 - timeslot)
    (scheduled ?t - timeslot)
    (meeting-scheduled)
    (is-jesse ?p - participant)
    (is-kathryn ?p - participant)
    (is-megan ?p - participant)
  )
  (:action schedule-meeting
    :parameters (?t - timeslot ?j - participant ?k - participant ?m - participant)
    :precondition (and
      (workhour ?t)
      (is-jesse ?j)
      (is-kathryn ?k)
      (is-megan ?m)
      (free ?j ?t)
      (free ?k ?t)
      (free ?m ?t)
      (not (meeting-scheduled))
    )
    :effect (and
      (scheduled ?t)
      (meeting-scheduled)
    )
  )
)