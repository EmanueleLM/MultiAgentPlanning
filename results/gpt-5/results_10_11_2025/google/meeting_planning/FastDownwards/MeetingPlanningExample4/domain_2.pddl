(define (domain meetingplanningexample4-domain)
  (:requirements :strips :typing :negative-preconditions)
  (:types time location count)
  (:predicates
    (at ?l - location)
    (at-time ?t - time)
    (next ?t1 - time ?t2 - time)
    (next10 ?t1 - time ?t2 - time)
    (jessica-available ?t - time)
    (streak ?c - count)
    (nextCount ?c1 - count ?c2 - count)
    (isC12 ?c - count)
    (achieved60)
    (adjacent ?l1 - location ?l2 - location)
    (jessica-meet-location ?l - location)
    (isZero ?c - count)
  )

  (:action move
    :parameters (?from - location ?to - location ?t1 - time ?t2 - time ?c - count ?z - count)
    :precondition (and
      (at ?from)
      (adjacent ?from ?to)
      (at-time ?t1)
      (next10 ?t1 ?t2)
      (streak ?c)
      (isZero ?z)
    )
    :effect (and
      (not (at ?from))
      (at ?to)
      (not (at-time ?t1))
      (at-time ?t2)
      (not (streak ?c))
      (streak ?z)
    )
  )

  (:action wait-one
    :parameters (?t1 - time ?t2 - time ?c - count ?z - count)
    :precondition (and
      (at-time ?t1)
      (next ?t1 ?t2)
      (streak ?c)
      (isZero ?z)
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
      (not (streak ?c))
      (streak ?z)
    )
  )

  (:action meet-minute
    :parameters (?l - location ?t1 - time ?t2 - time ?c1 - count ?c2 - count)
    :precondition (and
      (at ?l)
      (jessica-meet-location ?l)
      (at-time ?t1)
      (next ?t1 ?t2)
      (jessica-available ?t1)
      (jessica-available ?t2)
      (streak ?c1)
      (nextCount ?c1 ?c2)
      (not (isC12 ?c2))
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
      (not (streak ?c1))
      (streak ?c2)
    )
  )

  (:action meet-minute-hit-60
    :parameters (?l - location ?t1 - time ?t2 - time ?c1 - count ?c2 - count)
    :precondition (and
      (at ?l)
      (jessica-meet-location ?l)
      (at-time ?t1)
      (next ?t1 ?t2)
      (jessica-available ?t1)
      (jessica-available ?t2)
      (streak ?c1)
      (nextCount ?c1 ?c2)
      (isC12 ?c2)
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
      (not (streak ?c1))
      (streak ?c2)
      (achieved60)
    )
  )
)