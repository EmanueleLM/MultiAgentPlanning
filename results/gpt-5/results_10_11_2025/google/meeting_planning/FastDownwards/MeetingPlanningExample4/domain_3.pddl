(define (domain meetingplanningexample4-domain-revised)
  (:requirements :strips :typing :negative-preconditions)
  (:types time location count)
  (:predicates
    (at ?l - location)
    (at-time ?t - time)
    (next5 ?t1 - time ?t2 - time)
    (next10 ?t1 - time ?t2 - time)
    (adjacent ?l1 - location ?l2 - location)
    (is-presidio ?l - location)
    (is-marina ?l - location)
    (jessica-meet-location ?l - location)
    (jessica-available ?t - time)
    (streak ?c - count)
    (nextCount ?c1 - count ?c2 - count)
    (isZero ?c - count)
    (isC12 ?c - count)
    (achieved60)
  )

  (:action move10-p2m
    :parameters (?p - location ?m - location ?t1 - time ?t2 - time ?c - count ?z - count)
    :precondition (and
      (is-presidio ?p)
      (is-marina ?m)
      (at ?p)
      (adjacent ?p ?m)
      (at-time ?t1)
      (next10 ?t1 ?t2)
      (streak ?c)
      (isZero ?z)
    )
    :effect (and
      (not (at ?p))
      (at ?m)
      (not (at-time ?t1))
      (at-time ?t2)
      (not (streak ?c))
      (streak ?z)
    )
  )

  (:action move10-m2p-after-60
    :parameters (?m - location ?p - location ?t1 - time ?t2 - time ?c - count ?z - count)
    :precondition (and
      (achieved60)
      (is-marina ?m)
      (is-presidio ?p)
      (at ?m)
      (adjacent ?m ?p)
      (at-time ?t1)
      (next10 ?t1 ?t2)
      (streak ?c)
      (isZero ?z)
    )
    :effect (and
      (not (at ?m))
      (at ?p)
      (not (at-time ?t1))
      (at-time ?t2)
      (not (streak ?c))
      (streak ?z)
    )
  )

  (:action wait5
    :parameters (?t1 - time ?t2 - time ?c - count ?z - count)
    :precondition (and
      (at-time ?t1)
      (next5 ?t1 ?t2)
      (streak ?c)
      (isZero ?z)
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
      (not (streak ?c))
      (streak ?z)
    )
  )

  (:action meet5
    :parameters (?l - location ?t1 - time ?t2 - time ?c1 - count ?c2 - count)
    :precondition (and
      (at ?l)
      (jessica-meet-location ?l)
      (at-time ?t1)
      (next5 ?t1 ?t2)
      (jessica-available ?t1)
      (jessica-available ?t2)
      (streak ?c1)
      (nextCount ?c1 ?c2)
      (not (isC12 ?c2))
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
      (not (streak ?c1))
      (streak ?c2)
    )
  )

  (:action meet5-hit-60
    :parameters (?l - location ?t1 - time ?t2 - time ?c1 - count ?c2 - count)
    :precondition (and
      (at ?l)
      (jessica-meet-location ?l)
      (at-time ?t1)
      (next5 ?t1 ?t2)
      (jessica-available ?t1)
      (jessica-available ?t2)
      (streak ?c1)
      (nextCount ?c1 ?c2)
      (isC12 ?c2)
    )
    :effect (and
      (not (at-time ?t1))
      (at-time ?t2)
      (not (streak ?c1))
      (streak ?c2)
      (achieved60)
    )
  )
)