(define (problem tripplanningexample1-problem)
  (:domain tripplanningexample1)
  (:objects
    riga manchester split - city
    d0 d1 d2 d3 d4 d5 d6 d7 d8 d9 d10 d11 d12 d13 d14 d15 - day
    ur1 ur2 ur3 ur4 ur5 um1 um2 um3 um4 us1 us2 us3 us4 us5 us6 - unit
  )
  (:init
    (at riga)

    (direct riga manchester)
    (direct manchester riga)
    (direct manchester split)

    (curr-day d15)
    (dec d15 d14)
    (dec d14 d13)
    (dec d13 d12)
    (dec d12 d11)
    (dec d11 d10)
    (dec d10 d9)
    (dec d9 d8)
    (dec d8 d7)
    (dec d7 d6)
    (dec d6 d5)
    (dec d5 d4)
    (dec d4 d3)
    (dec d3 d2)
    (dec d2 d1)
    (dec d1 d0)

    (for ur1 riga) (remaining ur1)
    (for ur2 riga) (remaining ur2)
    (for ur3 riga) (remaining ur3)
    (for ur4 riga) (remaining ur4)
    (for ur5 riga) (remaining ur5)

    (for um1 manchester) (remaining um1)
    (for um2 manchester) (remaining um2)
    (for um3 manchester) (remaining um3)
    (for um4 manchester) (remaining um4)

    (for us1 split) (remaining us1)
    (for us2 split) (remaining us2)
    (for us3 split) (remaining us3)
    (for us4 split) (remaining us4)
    (for us5 split) (remaining us5)
    (for us6 split) (remaining us6)
  )
  (:goal
    (and
      (curr-day d0)
      (spent ur1)
      (spent ur2)
      (spent ur3)
      (spent ur4)
      (spent ur5)
      (spent um1)
      (spent um2)
      (spent um3)
      (spent um4)
      (spent us1)
      (spent us2)
      (spent us3)
      (spent us4)
      (spent us5)
      (spent us6)
    )
  )
)