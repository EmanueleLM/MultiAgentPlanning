(define (problem trip10_eu-instance)
  (:domain trip10_eu)
  (:objects
    mykonos vienna venice - city
    d1 d2 d3 d4 d5 d6 d7 d8 d9 d10 - day
  )
  (:init
    (first d1)
    (next d1 d2)
    (next d2 d3)
    (next d3 d4)
    (next d4 d5)
    (next d5 d6)
    (next d6 d7)
    (next d7 d8)
    (next d8 d9)
    (next d9 d10)
    (direct mykonos vienna)
    (direct vienna mykonos)
    (direct vienna venice)
    (direct venice vienna)
  )
  (:goal (and
    (visited mykonos)
    (visited vienna)
    (visited venice)
    (at d5 venice)
    (at d6 venice)
    (at d7 venice)
    (at d8 venice)
    (at d9 venice)
    (at d10 venice)
  ))
  (:metric minimize (total-cost))
)