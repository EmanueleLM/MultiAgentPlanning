(define (domain hanoi-ordered)
  (:requirements :strips :typing :negative-preconditions)
  (:types
    step place - object
    peg disk - place
  )
  (:predicates
    (on ?d - disk ?x - place)
    (clear ?x - place)
    (smaller ?d1 - disk ?d2 - disk)
    (fits ?d - disk ?x - place)
    (topdisk ?p - peg ?d - disk)
    (current ?s - step)
    (next ?s1 - step ?s2 - step)
    (must ?s - step ?d - disk ?from - peg ?to - peg)
  )

  (:action move-required-fromdisk-topeg
    :parameters (?s - step ?s2 - step ?d - disk ?from - peg ?below - disk ?to - peg)
    :precondition (and
      (current ?s)
      (next ?s ?s2)
      (must ?s ?d ?from ?to)
      (topdisk ?from ?d)
      (on ?d ?below)
      (clear ?d)
      (clear ?to)
      (fits ?d ?to)
    )
    :effect (and
      (not (current ?s))
      (current ?s2)
      (not (on ?d ?below))
      (clear ?below)
      (on ?d ?to)
      (not (clear ?to))
      (not (topdisk ?from ?d))
      (topdisk ?from ?below)
      (topdisk ?to ?d)
    )
  )

  (:action move-required-frompeg-topeg
    :parameters (?s - step ?s2 - step ?d - disk ?from - peg ?to - peg)
    :precondition (and
      (current ?s)
      (next ?s ?s2)
      (must ?s ?d ?from ?to)
      (topdisk ?from ?d)
      (on ?d ?from)
      (clear ?d)
      (clear ?to)
      (fits ?d ?to)
    )
    :effect (and
      (not (current ?s))
      (current ?s2)
      (not (on ?d ?from))
      (clear ?from)
      (on ?d ?to)
      (not (clear ?to))
      (not (topdisk ?from ?d))
      (topdisk ?to ?d)
    )
  )

  (:action move-required-fromdisk-todisk
    :parameters (?s - step ?s2 - step ?d - disk ?from - peg ?below - disk ?to - peg ?t - disk)
    :precondition (and
      (current ?s)
      (next ?s ?s2)
      (must ?s ?d ?from ?to)
      (topdisk ?from ?d)
      (on ?d ?below)
      (clear ?d)
      (topdisk ?to ?t)
      (clear ?t)
      (fits ?d ?t)
    )
    :effect (and
      (not (current ?s))
      (current ?s2)
      (not (on ?d ?below))
      (clear ?below)
      (on ?d ?t)
      (not (clear ?t))
      (not (topdisk ?from ?d))
      (topdisk ?from ?below)
      (topdisk ?to ?d)
    )
  )

  (:action move-required-frompeg-todisk
    :parameters (?s - step ?s2 - step ?d - disk ?from - peg ?to - peg ?t - disk)
    :precondition (and
      (current ?s)
      (next ?s ?s2)
      (must ?s ?d ?from ?to)
      (topdisk ?from ?d)
      (on ?d ?from)
      (clear ?d)
      (topdisk ?to ?t)
      (clear ?t)
      (fits ?d ?t)
    )
    :effect (and
      (not (current ?s))
      (current ?s2)
      (not (on ?d ?from))
      (clear ?from)
      (on ?d ?t)
      (not (clear ?t))
      (not (topdisk ?from ?d))
      (topdisk ?to ?d)
    )
  )
)