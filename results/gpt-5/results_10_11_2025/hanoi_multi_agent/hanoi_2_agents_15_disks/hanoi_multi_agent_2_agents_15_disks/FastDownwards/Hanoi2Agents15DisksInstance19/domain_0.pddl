(define (domain hanoi-2agent-audited)
  (:requirements :strips :typing :negative-preconditions)
  (:types
    step
    support
    peg disk - support
  )
  (:predicates
    (on ?d - disk ?s - support)
    (clear ?s - support)
    (smaller ?x - disk ?y - disk)
    (owned-a1 ?d - disk)
    (owned-a2 ?d - disk)
    (at-peg ?d - disk ?p - peg)
    (at-step ?s - step)
    (next ?s1 - step ?s2 - step)
    (allowed-at-step ?d - disk ?from - peg ?to - peg ?s - step)
  )

  (:action move_agent_1_to_peg
    :parameters (?d - disk ?u - support ?from - peg ?to - peg ?s - step ?s2 - step)
    :precondition (and
      (owned-a1 ?d)
      (at-step ?s)
      (next ?s ?s2)
      (allowed-at-step ?d ?from ?to ?s)
      (at-peg ?d ?from)
      (on ?d ?u)
      (clear ?d)
      (clear ?to)
    )
    :effect (and
      (not (on ?d ?u))
      (clear ?u)
      (on ?d ?to)
      (not (clear ?to))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (not (at-step ?s))
      (at-step ?s2)
    )
  )

  (:action move_agent_1_to_disk
    :parameters (?d - disk ?u - support ?below - disk ?from - peg ?to - peg ?s - step ?s2 - step)
    :precondition (and
      (owned-a1 ?d)
      (at-step ?s)
      (next ?s ?s2)
      (allowed-at-step ?d ?from ?to ?s)
      (at-peg ?d ?from)
      (at-peg ?below ?to)
      (on ?d ?u)
      (clear ?d)
      (clear ?below)
      (smaller ?d ?below)
    )
    :effect (and
      (not (on ?d ?u))
      (clear ?u)
      (on ?d ?below)
      (not (clear ?below))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (not (at-step ?s))
      (at-step ?s2)
    )
  )

  (:action move_agent_2_to_peg
    :parameters (?d - disk ?u - support ?from - peg ?to - peg ?s - step ?s2 - step)
    :precondition (and
      (owned-a2 ?d)
      (at-step ?s)
      (next ?s ?s2)
      (allowed-at-step ?d ?from ?to ?s)
      (at-peg ?d ?from)
      (on ?d ?u)
      (clear ?d)
      (clear ?to)
    )
    :effect (and
      (not (on ?d ?u))
      (clear ?u)
      (on ?d ?to)
      (not (clear ?to))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (not (at-step ?s))
      (at-step ?s2)
    )
  )

  (:action move_agent_2_to_disk
    :parameters (?d - disk ?u - support ?below - disk ?from - peg ?to - peg ?s - step ?s2 - step)
    :precondition (and
      (owned-a2 ?d)
      (at-step ?s)
      (next ?s ?s2)
      (allowed-at-step ?d ?from ?to ?s)
      (at-peg ?d ?from)
      (at-peg ?below ?to)
      (on ?d ?u)
      (clear ?d)
      (clear ?below)
      (smaller ?d ?below)
    )
    :effect (and
      (not (on ?d ?u))
      (clear ?u)
      (on ?d ?below)
      (not (clear ?below))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (not (at-step ?s))
      (at-step ?s2)
    )
  )
)