(define (domain hanoi-two-agent)
  (:requirements :strips :typing :negative-preconditions)
  (:types
    disk peg agent
  )
  (:predicates
    (on ?d - disk ?s - (either disk peg))
    (clear ?x - (either disk peg))
    (smaller ?d1 - disk ?d2 - disk)
    (top ?d - disk ?p - peg)
    (can-move-disk ?a - agent ?d - disk)
    (can-use-peg ?a - agent ?p - peg)
  )

  ; Agent 1 actions
  (:action move_agent_1_from_disk_to_peg
    :parameters (?d - disk ?u - disk ?sp - peg ?dp - peg)
    :precondition (and
      (can-move-disk agent_1 ?d)
      (can-use-peg agent_1 ?sp)
      (can-use-peg agent_1 ?dp)
      (top ?d ?sp)
      (on ?d ?u)
      (clear ?d)
      (clear ?dp)
    )
    :effect (and
      (on ?d ?dp)
      (not (on ?d ?u))
      (clear ?u)
      (not (clear ?dp))
      (top ?d ?dp)
      (not (top ?d ?sp))
      (top ?u ?sp)
    )
  )

  (:action move_agent_1_from_peg_to_peg
    :parameters (?d - disk ?sp - peg ?dp - peg)
    :precondition (and
      (can-move-disk agent_1 ?d)
      (can-use-peg agent_1 ?sp)
      (can-use-peg agent_1 ?dp)
      (top ?d ?sp)
      (on ?d ?sp)
      (clear ?d)
      (clear ?dp)
    )
    :effect (and
      (on ?d ?dp)
      (not (on ?d ?sp))
      (clear ?sp)
      (not (clear ?dp))
      (top ?d ?dp)
      (not (top ?d ?sp))
    )
  )

  (:action move_agent_1_from_disk_to_disk
    :parameters (?d - disk ?u - disk ?sp - peg ?v - disk ?dp - peg)
    :precondition (and
      (can-move-disk agent_1 ?d)
      (can-use-peg agent_1 ?sp)
      (can-use-peg agent_1 ?dp)
      (top ?d ?sp)
      (on ?d ?u)
      (clear ?d)
      (top ?v ?dp)
      (clear ?v)
      (smaller ?d ?v)
    )
    :effect (and
      (on ?d ?v)
      (not (on ?d ?u))
      (clear ?u)
      (not (clear ?v))
      (not (top ?v ?dp))
      (top ?d ?dp)
      (not (top ?d ?sp))
      (top ?u ?sp)
    )
  )

  (:action move_agent_1_from_peg_to_disk
    :parameters (?d - disk ?sp - peg ?v - disk ?dp - peg)
    :precondition (and
      (can-move-disk agent_1 ?d)
      (can-use-peg agent_1 ?sp)
      (can-use-peg agent_1 ?dp)
      (top ?d ?sp)
      (on ?d ?sp)
      (clear ?d)
      (top ?v ?dp)
      (clear ?v)
      (smaller ?d ?v)
    )
    :effect (and
      (on ?d ?v)
      (not (on ?d ?sp))
      (clear ?sp)
      (not (clear ?v))
      (not (top ?v ?dp))
      (top ?d ?dp)
      (not (top ?d ?sp))
    )
  )

  ; Agent 2 actions
  (:action move_agent_2_from_disk_to_peg
    :parameters (?d - disk ?u - disk ?sp - peg ?dp - peg)
    :precondition (and
      (can-move-disk agent_2 ?d)
      (can-use-peg agent_2 ?sp)
      (can-use-peg agent_2 ?dp)
      (top ?d ?sp)
      (on ?d ?u)
      (clear ?d)
      (clear ?dp)
    )
    :effect (and
      (on ?d ?dp)
      (not (on ?d ?u))
      (clear ?u)
      (not (clear ?dp))
      (top ?d ?dp)
      (not (top ?d ?sp))
      (top ?u ?sp)
    )
  )

  (:action move_agent_2_from_peg_to_peg
    :parameters (?d - disk ?sp - peg ?dp - peg)
    :precondition (and
      (can-move-disk agent_2 ?d)
      (can-use-peg agent_2 ?sp)
      (can-use-peg agent_2 ?dp)
      (top ?d ?sp)
      (on ?d ?sp)
      (clear ?d)
      (clear ?dp)
    )
    :effect (and
      (on ?d ?dp)
      (not (on ?d ?sp))
      (clear ?sp)
      (not (clear ?dp))
      (top ?d ?dp)
      (not (top ?d ?sp))
    )
  )

  (:action move_agent_2_from_disk_to_disk
    :parameters (?d - disk ?u - disk ?sp - peg ?v - disk ?dp - peg)
    :precondition (and
      (can-move-disk agent_2 ?d)
      (can-use-peg agent_2 ?sp)
      (can-use-peg agent_2 ?dp)
      (top ?d ?sp)
      (on ?d ?u)
      (clear ?d)
      (top ?v ?dp)
      (clear ?v)
      (smaller ?d ?v)
    )
    :effect (and
      (on ?d ?v)
      (not (on ?d ?u))
      (clear ?u)
      (not (clear ?v))
      (not (top ?v ?dp))
      (top ?d ?dp)
      (not (top ?d ?sp))
      (top ?u ?sp)
    )
  )

  (:action move_agent_2_from_peg_to_disk
    :parameters (?d - disk ?sp - peg ?v - disk ?dp - peg)
    :precondition (and
      (can-move-disk agent_2 ?d)
      (can-use-peg agent_2 ?sp)
      (can-use-peg agent_2 ?dp)
      (top ?d ?sp)
      (on ?d ?sp)
      (clear ?d)
      (top ?v ?dp)
      (clear ?v)
      (smaller ?d ?v)
    )
    :effect (and
      (on ?d ?v)
      (not (on ?d ?sp))
      (clear ?sp)
      (not (clear ?v))
      (not (top ?v ?dp))
      (top ?d ?dp)
      (not (top ?d ?sp))
    )
  )
)