(define (domain hanoi_multiagent_constrained_v2)
  (:requirements :strips :typing :negative-preconditions)
  (:types agent peg disk step)
  (:constants agent_1 agent_2 - agent)
  (:predicates
    (on ?x - disk ?y - disk)
    (on-peg ?x - disk ?p - peg)
    (in-peg ?x - disk ?p - peg)
    (clear ?x - disk)
    (clear-peg ?p - peg)
    (smaller ?x - disk ?y - disk)
    (permits ?a - agent ?d - disk)
    (current-step ?s - step)
    (next ?s - step ?t - step)
    (is-last ?s - step)
    (step-agent ?s - step ?a - agent)
    (step-disk ?s - step ?d - disk)
    (step-from ?s - step ?p - peg)
    (step-to ?s - step ?p - peg)
  )

  (:action a1_move_fromdisk_toempty_progress
    :parameters (?d - disk ?below - disk ?from - peg ?to - peg ?s - step ?t - step)
    :precondition (and
      (current-step ?s) (next ?s ?t)
      (step-agent ?s agent_1)
      (step-disk ?s ?d) (step-from ?s ?from) (step-to ?s ?to)
      (permits agent_1 ?d)
      (clear ?d)
      (on ?d ?below)
      (in-peg ?d ?from) (in-peg ?below ?from)
      (clear-peg ?to)
    )
    :effect (and
      (not (current-step ?s)) (current-step ?t)
      (not (on ?d ?below)) (clear ?below)
      (on-peg ?d ?to) (not (clear-peg ?to))
      (not (in-peg ?d ?from)) (in-peg ?d ?to)
    )
  )

  (:action a1_move_frompeg_toempty_progress
    :parameters (?d - disk ?from - peg ?to - peg ?s - step ?t - step)
