(define (domain hanoi-2agents)
  (:requirements :strips :typing)
  (:types disk peg)
  (:predicates
    (on ?x - disk ?y - disk)
    (on-peg ?x - disk ?p - peg)
    (clear ?x - disk)
    (top ?p - peg ?x - disk)
    (smaller ?x - disk ?y - disk)
    (empty ?p - peg)
    (allowed1 ?x - disk)
    (allowed2 ?x - disk)
  )

  ;; Agent 1 actions
  (:action move_agent_1_from_peg_to_empty_peg
    :parameters (?d - disk ?from - peg ?to - peg)
    :precondition (and
      (allowed1 ?d)
      (clear ?d)
      (on-peg ?d ?from)
      (top ?from ?d)
      (empty ?to)
    )
    :effect (and
      (not (on-peg ?d ?from))
      (not (top ?from ?d))
      (empty ?from)
      (on-peg ?d ?to)
      (top ?to ?d)
      (not (empty ?to))
    )
  )

  (:action move_agent_1_from_peg_to_disk
    :parameters (?d - disk ?from - peg ?to - peg ?destTop - disk)
    :precondition (and
      (allowed1 ?d)
      (clear ?d)
      (on-peg ?d ?from)
      (top ?from ?d)
      (top ?to ?destTop)
      (smaller ?d ?destTop)
    )
    :effect (and
      (not (on-peg ?d ?from))
      (not (top ?from ?d))
      (empty ?from)
      (on ?d ?destTop)
      (not (clear ?destTop))
      (not (top ?to ?destTop))
      (top ?to ?d)
    )
  )

  (:action move_agent_1_from_disk_to_empty_peg
    :parameters (?d - disk ?below - disk ?from - peg ?to - peg)
    :precondition (and
      (allowed1 ?d)
      (clear ?d)
      (on ?d ?below)
      (top ?from ?d)
      (empty ?to)
    )
    :effect (and
      (not (on ?d ?below))
      (clear ?below)
      (not (top ?from ?d))
      (top ?from ?below)
      (on-peg ?d ?to)
      (top ?to ?d)
      (not (empty ?to))
    )
  )

  (:action move_agent_1_from_disk_to_disk
    :parameters (?d - disk ?below - disk ?from - peg ?to - peg ?destTop - disk)
    :precondition (and
      (allowed1 ?d)
      (clear ?d)
      (on ?d ?below)
      (top ?from ?d)
      (top ?to ?destTop)
      (smaller ?d ?destTop)
    )
    :effect (and
      (not (on ?d ?below))
      (clear ?below)
      (not (top ?from ?d))
      (top ?from ?below)
      (not (top ?to ?destTop))
      (on ?d ?destTop)
      (not (clear ?destTop))
      (top ?to ?d)
    )
  )

  ;; Agent 2 actions
  (:action move_agent_2_from_peg_to_empty_peg
    :parameters (?d - disk ?from - peg ?to - peg)
    :precondition (and
      (allowed2 ?d)
      (clear ?d)
      (on-peg ?d ?from)
      (top ?from ?d)
      (empty ?to)
    )
    :effect (and
      (not (on-peg ?d ?from))
      (not (top ?from ?d))
      (empty ?from)
      (on-peg ?d ?to)
      (top ?to ?d)
      (not (empty ?to))
    )
  )

  (:action move_agent_2_from_peg_to_disk
    :parameters (?d - disk ?from - peg ?to - peg ?destTop - disk)
    :precondition (and
      (allowed2 ?d)
      (clear ?d)
      (on-peg ?d ?from)
      (top ?from ?d)
      (top ?to ?destTop)
      (smaller ?d ?destTop)
    )
    :effect (and
      (not (on-peg ?d ?from))
      (not (top ?from ?d))
      (empty ?from)
      (on ?d ?destTop)
      (not (clear ?destTop))
      (not (top ?to ?destTop))
      (top ?to ?d)
    )
  )

  (:action move_agent_2_from_disk_to_empty_peg
    :parameters (?d - disk ?below - disk ?from - peg ?to - peg)
    :precondition (and
      (allowed2 ?d)
      (clear ?d)
      (on ?d ?below)
      (top ?from ?d)
      (empty ?to)
    )
    :effect (and
      (not (on ?d ?below))
      (clear ?below)
      (not (top ?from ?d))
      (top ?from ?below)
      (on-peg ?d ?to)
      (top ?to ?d)
      (not (empty ?to))
    )
  )

  (:action move_agent_2_from_disk_to_disk
    :parameters (?d - disk ?below - disk ?from - peg ?to - peg ?destTop - disk)
    :precondition (and
      (allowed2 ?d)
      (clear ?d)
      (on ?d ?below)
      (top ?from ?d)
      (top ?to ?destTop)
      (smaller ?d ?destTop)
    )
    :effect (and
      (not (on ?d ?below))
      (clear ?below)
      (not (top ?from ?d))
      (top ?from ?below)
      (not (top ?to ?destTop))
      (on ?d ?destTop)
      (not (clear ?destTop))
      (top ?to ?d)
    )
  )
)