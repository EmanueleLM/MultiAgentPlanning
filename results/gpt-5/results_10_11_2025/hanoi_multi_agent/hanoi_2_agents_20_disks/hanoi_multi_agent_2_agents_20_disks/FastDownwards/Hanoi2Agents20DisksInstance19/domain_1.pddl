(define (domain hanoi-20-multiagent-coop)
  (:requirements :strips :typing :negative-preconditions)
  (:types disk peg step)
  (:predicates
    (on ?x - disk ?y - disk)
    (on-peg ?x - disk ?p - peg)
    (clear ?x - disk)
    (peg-clear ?p - peg)
    (smaller ?x - disk ?y - disk)
    (at ?x - disk ?p - peg)
    (can-move-agent1 ?x - disk)
    (can-move-agent2 ?x - disk)
    (fixed-left ?x - disk)
    (allowed ?s - step ?x - disk ?from - peg ?to - peg)
    (next ?s - step ?s2 - step)
    (curstep ?s - step)
  )

  ; Agent 1 actions
  (:action move_agent_1_from_disk_to_peg
    :parameters (?s - step ?s2 - step ?x - disk ?y - disk ?from - peg ?to - peg)
    :precondition (and
      (curstep ?s) (next ?s ?s2) (allowed ?s ?x ?from ?to)
      (can-move-agent1 ?x)
      (not (fixed-left ?x))
      (clear ?x)
      (on ?x ?y)
      (at ?x ?from)
      (peg-clear ?to)
    )
    :effect (and
      (not (curstep ?s)) (curstep ?s2)
      (not (on ?x ?y))
      (clear ?y)
      (on-peg ?x ?to)
      (not (peg-clear ?to))
      (not (at ?x ?from))
      (at ?x ?to)
    )
  )

  (:action move_agent_1_from_peg_to_peg
    :parameters (?s - step ?s2 - step ?x - disk ?from - peg ?to - peg)
    :precondition (and
      (curstep ?s) (next ?s ?s2) (allowed ?s ?x ?from ?to)
      (can-move-agent1 ?x)
      (not (fixed-left ?x))
      (clear ?x)
      (on-peg ?x ?from)
      (peg-clear ?to)
      (at ?x ?from)
    )
    :effect (and
      (not (curstep ?s)) (curstep ?s2)
      (not (on-peg ?x ?from))
      (peg-clear ?from)
      (on-peg ?x ?to)
      (not (peg-clear ?to))
      (not (at ?x ?from))
      (at ?x ?to)
    )
  )

  (:action move_agent_1_from_peg_to_disk
    :parameters (?s - step ?s2 - step ?x - disk ?toDisk - disk ?from - peg ?toPeg - peg)
    :precondition (and
      (curstep ?s) (next ?s ?s2) (allowed ?s ?x ?from ?toPeg)
      (can-move-agent1 ?x)
      (not (fixed-left ?x))
      (clear ?x)
      (on-peg ?x ?from)
      (clear ?toDisk)
      (smaller ?x ?toDisk)
      (at ?x ?from)
      (at ?toDisk ?toPeg)
    )
    :effect (and
      (not (curstep ?s)) (curstep ?s2)
      (not (on-peg ?x ?from))
      (peg-clear ?from)
      (on ?x ?toDisk)
      (not (clear ?toDisk))
      (not (at ?x ?from))
      (at ?x ?toPeg)
    )
  )

  (:action move_agent_1_from_disk_to_disk
    :parameters (?s - step ?s2 - step ?x - disk ?fromBelow - disk ?toDisk - disk ?from - peg ?toPeg - peg)
    :precondition (and
      (curstep ?s) (next ?s ?s2) (allowed ?s ?x ?from ?toPeg)
      (can-move-agent1 ?x)
      (not (fixed-left ?x))
      (clear ?x)
      (on ?x ?fromBelow)
      (clear ?toDisk)
      (smaller ?x ?toDisk)
      (at ?x ?from)
      (at ?toDisk ?toPeg)
    )
    :effect (and
      (not (curstep ?s)) (curstep ?s2)
      (not (on ?x ?fromBelow))
      (clear ?fromBelow)
      (on ?x ?toDisk)
      (not (clear ?toDisk))
      (not (at ?x ?from))
      (at ?x ?toPeg)
    )
  )

  ; Agent 2 actions
  (:action move_agent_2_from_disk_to_peg
    :parameters (?s - step ?s2 - step ?x - disk ?y - disk ?from - peg ?to - peg)
    :precondition (and
      (curstep ?s) (next ?s ?s2) (allowed ?s ?x ?from ?to)
      (can-move-agent2 ?x)
      (not (fixed-left ?x))
      (clear ?x)
      (on ?x ?y)
      (at ?x ?from)
      (peg-clear ?to)
    )
    :effect (and
      (not (curstep ?s)) (curstep ?s2)
      (not (on ?x ?y))
      (clear ?y)
      (on-peg ?x ?to)
      (not (peg-clear ?to))
      (not (at ?x ?from))
      (at ?x ?to)
    )
  )

  (:action move_agent_2_from_peg_to_peg
    :parameters (?s - step ?s2 - step ?x - disk ?from - peg ?to - peg)
    :precondition (and
      (curstep ?s) (next ?s ?s2) (allowed ?s ?x ?from ?to)
      (can-move-agent2 ?x)
      (not (fixed-left ?x))
      (clear ?x)
      (on-peg ?x ?from)
      (peg-clear ?to)
      (at ?x ?from)
    )
    :effect (and
      (not (curstep ?s)) (curstep ?s2)
      (not (on-peg ?x ?from))
      (peg-clear ?from)
      (on-peg ?x ?to)
      (not (peg-clear ?to))
      (not (at ?x ?from))
      (at ?x ?to)
    )
  )

  (:action move_agent_2_from_peg_to_disk
    :parameters (?s - step ?s2 - step ?x - disk ?toDisk - disk ?from - peg ?toPeg - peg)
    :precondition (and
      (curstep ?s) (next ?s ?s2) (allowed ?s ?x ?from ?toPeg)
      (can-move-agent2 ?x)
      (not (fixed-left ?x))
      (clear ?x)
      (on-peg ?x ?from)
      (clear ?toDisk)
      (smaller ?x ?toDisk)
      (at ?x ?from)
      (at ?toDisk ?toPeg)
    )
    :effect (and
      (not (curstep ?s)) (curstep ?s2)
      (not (on-peg ?x ?from))
      (peg-clear ?from)
      (on ?x ?toDisk)
      (not (clear ?toDisk))
      (not (at ?x ?from))
      (at ?x ?toPeg)
    )
  )

  (:action move_agent_2_from_disk_to_disk
    :parameters (?s - step ?s2 - step ?x - disk ?fromBelow - disk ?toDisk - disk ?from - peg ?toPeg - peg)
    :precondition (and
      (curstep ?s) (next ?s ?s2) (allowed ?s ?x ?from ?toPeg)
      (can-move-agent2 ?x)
      (not (fixed-left ?x))
      (clear ?x)
      (on ?x ?fromBelow)
      (clear ?toDisk)
      (smaller ?x ?toDisk)
      (at ?x ?from)
      (at ?toDisk ?toPeg)
    )
    :effect (and
      (not (curstep ?s)) (curstep ?s2)
      (not (on ?x ?fromBelow))
      (clear ?fromBelow)
      (on ?x ?toDisk)
      (not (clear ?toDisk))
      (not (at ?x ?from))
      (at ?x ?toPeg)
    )
  )
)