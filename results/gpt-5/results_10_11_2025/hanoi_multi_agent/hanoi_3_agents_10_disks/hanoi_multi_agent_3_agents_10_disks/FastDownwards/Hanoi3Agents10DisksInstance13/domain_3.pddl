(define (domain hanoi3agents10disks-scheduled-legal)
  (:requirements :strips :typing :negative-preconditions)
  (:types
    step
    agent
    support
    peg disk - support
  )

  (:predicates
    (on ?d - disk ?s - support)
    (top ?p - peg ?s - support)
    (clear ?d - disk)
    (smaller ?d1 - disk ?d2 - disk)
    (permission ?a - agent ?d - disk)
    (current ?s - step)
    (next ?s1 - step ?s2 - step)
    (scheduled ?s - step ?a - agent ?d - disk ?from - peg ?to - peg)
  )

  (:action move-from-disk-to-empty
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?u - disk ?s - step ?s2 - step)
    :precondition (and
      (current ?s)
      (next ?s ?s2)
      (scheduled ?s ?a ?d ?from ?to)
      (permission ?a ?d)
      (top ?from ?d)
      (top ?to ?to)
      (on ?d ?u)
      (clear ?d)
    )
    :effect (and
      (not (on ?d ?u))
      (on ?d ?to)
      (not (top ?from ?d))
      (top ?from ?u)
      (not (top ?to ?to))
      (top ?to ?d)
      (clear ?u)
      (not (current ?s))
      (current ?s2)
    )
  )

  (:action move-from-peg-to-empty
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?s - step ?s2 - step)
    :precondition (and
      (current ?s)
      (next ?s ?s2)
      (scheduled ?s ?a ?d ?from ?to)
      (permission ?a ?d)
      (top ?from ?d)
      (top ?to ?to)
      (on ?d ?from)
      (clear ?d)
    )
    :effect (and
      (not (on ?d ?from))
      (on ?d ?to)
      (not (top ?from ?d))
      (top ?from ?from)
      (not (top ?to ?to))
      (top ?to ?d)
      (not (current ?s))
      (current ?s2)
    )
  )

  (:action move-from-disk-to-disk
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?u_from - disk ?u_to - disk ?s - step ?s2 - step)
    :precondition (and
      (current ?s)
      (next ?s ?s2)
      (scheduled ?s ?a ?d ?from ?to)
      (permission ?a ?d)
      (top ?from ?d)
      (top ?to ?u_to)
      (on ?d ?u_from)
      (clear ?d)
      (smaller ?d ?u_to)
    )
    :effect (and
      (not (on ?d ?u_from))
      (on ?d ?u_to)
      (not (top ?from ?d))
      (top ?from ?u_from)
      (not (top ?to ?u_to))
      (top ?to ?d)
      (clear ?u_from)
      (not (clear ?u_to))
      (not (current ?s))
      (current ?s2)
    )
  )

  (:action move-from-peg-to-disk
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?u_to - disk ?s - step ?s2 - step)
    :precondition (and
      (current ?s)
      (next ?s ?s2)
      (scheduled ?s ?a ?d ?from ?to)
      (permission ?a ?d)
      (top ?from ?d)
      (top ?to ?u_to)
      (on ?d ?from)
      (clear ?d)
      (smaller ?d ?u_to)
    )
    :effect (and
      (not (on ?d ?from))
      (on ?d ?u_to)
      (not (top ?from ?d))
      (top ?from ?from)
      (not (top ?to ?u_to))
      (top ?to ?d)
      (not (clear ?u_to))
      (not (current ?s))
      (current ?s2)
    )
  )
)