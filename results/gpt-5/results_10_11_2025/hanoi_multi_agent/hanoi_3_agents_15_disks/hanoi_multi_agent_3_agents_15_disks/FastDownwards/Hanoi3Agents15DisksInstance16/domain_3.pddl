(define (domain hanoi-3agents-15-ordered)
  (:requirements :strips :typing :negative-preconditions)
  (:types disk peg step agent)
  (:predicates
    (smaller ?x - disk ?y - disk)
    (on ?x - disk ?y - disk)
    (onpeg ?x - disk ?p - peg)
    (clear ?x - disk)
    (clearpeg ?p - peg)
    (top ?p - peg ?x - disk)
    (current-step ?s - step)
    (succ ?s1 - step ?s2 - step)
    (can-move ?a - agent ?d - disk)
    (allowed ?a - agent ?d - disk ?from - peg ?to - peg ?s - step)
  )

  (:action move-from-disk-to-empty
    :parameters (?a - agent ?d - disk ?below - disk ?from - peg ?to - peg ?s - step ?s2 - step)
    :precondition (and
      (current-step ?s)
      (succ ?s ?s2)
      (allowed ?a ?d ?from ?to ?s)
      (can-move ?a ?d)
      (top ?from ?d)
      (on ?d ?below)
      (clear ?d)
      (clearpeg ?to)
    )
    :effect (and
      (not (on ?d ?below))
      (clear ?below)
      (not (top ?from ?d))
      (top ?from ?below)
      (not (clearpeg ?to))
      (onpeg ?d ?to)
      (top ?to ?d)
      (not (current-step ?s))
      (current-step ?s2)
    )
  )

  (:action move-from-peg-to-empty
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?s - step ?s2 - step)
    :precondition (and
      (current-step ?s)
      (succ ?s ?s2)
      (allowed ?a ?d ?from ?to ?s)
      (can-move ?a ?d)
      (top ?from ?d)
      (onpeg ?d ?from)
      (clear ?d)
      (clearpeg ?to)
    )
    :effect (and
      (not (onpeg ?d ?from))
      (clearpeg ?from)
      (not (top ?from ?d))
      (not (clearpeg ?to))
      (onpeg ?d ?to)
      (top ?to ?d)
      (not (current-step ?s))
      (current-step ?s2)
    )
  )

  (:action move-from-disk-to-disk
    :parameters (?a - agent ?d - disk ?below - disk ?from - peg ?to - peg ?dsttop - disk ?s - step ?s2 - step)
    :precondition (and
      (current-step ?s)
      (succ ?s ?s2)
      (allowed ?a ?d ?from ?to ?s)
      (can-move ?a ?d)
      (top ?from ?d)
      (on ?d ?below)
      (clear ?d)
      (top ?to ?dsttop)
      (clear ?dsttop)
      (smaller ?d ?dsttop)
    )
    :effect (and
      (not (on ?d ?below))
      (clear ?below)
      (not (top ?from ?d))
      (top ?from ?below)
      (on ?d ?dsttop)
      (not (top ?to ?dsttop))
      (top ?to ?d)
      (not (clear ?dsttop))
      (not (current-step ?s))
      (current-step ?s2)
    )
  )

  (:action move-from-peg-to-disk
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?dsttop - disk ?s - step ?s2 - step)
    :precondition (and
      (current-step ?s)
      (succ ?s ?s2)
      (allowed ?a ?d ?from ?to ?s)
      (can-move ?a ?d)
      (top ?from ?d)
      (onpeg ?d ?from)
      (clear ?d)
      (top ?to ?dsttop)
      (clear ?dsttop)
      (smaller ?d ?dsttop)
    )
    :effect (and
      (not (onpeg ?d ?from))
      (clearpeg ?from)
      (not (top ?from ?d))
      (on ?d ?dsttop)
      (not (top ?to ?dsttop))
      (top ?to ?d)
      (not (clear ?dsttop))
      (not (current-step ?s))
      (current-step ?s2)
    )
  )
)