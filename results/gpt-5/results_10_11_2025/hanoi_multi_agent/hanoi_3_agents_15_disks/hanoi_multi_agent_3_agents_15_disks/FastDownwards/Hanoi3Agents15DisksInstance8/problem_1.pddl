(define (problem Hanoi3Agents15DisksInstance8)
  (:domain hanoi-multiagent-3agents-15disks)
  (:objects
    agent_1 agent_2 agent_3 auditor orchestrator - agent
    A B C D E F G H I J K L M N O - disk
    left middle right - peg
    s1 s2 s3 s4 s5 s6 s7 s8 - step
  )

  (:init
    ; initial tower: left = [A,B,C,D,E,F,G,H,I,J,K,L,M,N,O] (top->bottom)
    (on A B)
    (on B C)
    (on C D)
    (on D E)
    (on E F)
    (on F G)
    (on G H)
    (on H I)
    (on I J)
    (on J K)
    (on K L)
    (on L M)
    (on M N)
    (on N O)
    (on-peg O left)

    (clear A)
    (clear-peg middle)
    (clear-peg right)

    ; all disks initially belong to the left stack
    (in-stack A left)
    (in-stack B left)
    (in-stack C left)
    (in-stack D left)
    (in-stack E left)
    (in-stack F left)
    (in-stack G left)
    (in-stack H left)
    (in-stack I left)
    (in-stack J left)
    (in-stack K left)
    (in-stack L left)
    (in-stack M left)
    (in-stack N left)
    (in-stack O left)

    ; agent-disk permissions
    (can-move agent_1 A)
    (can-move agent_1 B)
    (can-move agent_1 C)
    (can-move agent_1 D)
    (can-move agent_1 E)

    (can-move agent_2 F)
    (can-move agent_2 G)
    (can-move agent_2 H)
    (can-move agent_2 I)
    (can-move agent_2 J)

    (can-move agent_3 K)
    (can-move agent_3 L)
    (can-move agent_3 M)
    (can-move agent_3 N)
    (can-move agent_3 O)

    ; strict size ordering A (smallest) ... O (largest)
    (smaller A B) (smaller A C) (smaller A D) (smaller A E) (smaller A F) (smaller A G) (smaller A H) (smaller A I) (smaller A J) (smaller A K) (smaller A L) (smaller A M) (smaller A N) (smaller A O)
    (smaller B C) (smaller B D) (smaller B E) (smaller B F) (smaller B G) (smaller B H) (smaller B I) (smaller B J) (smaller B K) (smaller B L) (smaller B M) (smaller B N) (smaller B O)
    (smaller C D) (smaller C E) (smaller C F) (smaller C G) (smaller C H) (smaller C I) (smaller C J) (smaller C K) (smaller C L) (smaller C M) (smaller C N) (smaller C O)
    (smaller D E) (smaller D F) (smaller D G) (smaller D H) (smaller D I) (smaller D J) (smaller D K) (smaller D L) (smaller D M) (smaller D N) (smaller D O)
    (smaller E F) (smaller E G) (smaller E H) (smaller E I) (smaller E J) (smaller E K) (smaller E L) (smaller E M) (smaller E N) (smaller E O)
    (smaller F G) (smaller F H) (smaller F I) (smaller F J) (smaller F K) (smaller F L) (smaller F M) (smaller F N) (smaller F O)
    (smaller G H) (smaller G I) (smaller G J) (smaller G K) (smaller G L) (smaller G M) (smaller G N) (smaller G O)
    (smaller H I) (smaller H J) (smaller H K) (smaller H L) (smaller H M) (smaller H N) (smaller H O)
    (smaller I J) (smaller I K) (smaller I L) (smaller I M) (smaller I N) (smaller I O)
    (smaller J K) (smaller J L) (smaller J M) (smaller J N) (smaller J O)
    (smaller K L) (smaller K M) (smaller K N) (smaller K O)
    (smaller L M) (smaller L N) (smaller L O)
    (smaller M N) (smaller M O)
    (smaller N O)

    ; enforce the exact 7-step ordered sequence
    (atstep s1)
    (next s1 s2)
    (next s2 s3)
    (next s3 s4)
    (next s4 s5)
    (next s5 s6)
    (next s6 s7)
    (next s7 s8)

    ; allowed moves per step (agent, disk, from-peg, to-peg)
    (allowed-step s1 agent_1 A left right)
    (allowed-step s2 agent_1 B left middle)
    (allowed-step s3 agent_1 A right middle)
    (allowed-step s4 agent_1 C left right)
    (allowed-step s5 agent_1 A middle left)
    (allowed-step s6 agent_1 B middle right)
    (allowed-step s7 agent_1 A left middle)
  )

  (:goal (and
    ; final stacks (top->bottom)
    ; left = [D,E,F,G,H,I,J,K,L,M,N,O]
    (on D E)
    (on E F)
    (on F G)
    (on G H)
    (on H I)
    (on I J)
    (on J K)
    (on K L)
    (on L M)
    (on M N)
    (on N O)
    (on-peg O left)
    (clear D)

    ; middle = [A]
    (on-peg A middle)
    (clear A)

    ; right = [B,C]
    (on B C)
    (on-peg C right)
    (clear B)

    ; sequence completion
    (atstep s8)

    ; peg membership consistency at goal
    (in-stack A middle)
    (in-stack B right)
    (in-stack C right)
    (in-stack D left)
    (in-stack E left)
    (in-stack F left)
    (in-stack G left)
    (in-stack H left)
    (in-stack I left)
    (in-stack J left)
    (in-stack K left)
    (in-stack L left)
    (in-stack M left)
    (in-stack N left)
    (in-stack O left)
  ))
)