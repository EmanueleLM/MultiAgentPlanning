(define (domain hanoi-3peg-20disks-audited)
  (:requirements :strips :typing :negative-preconditions)
  (:types
    peg disk agent step
  )
  (:predicates
    (on ?d - disk ?p - peg)
    (clear ?p - peg)
    (top ?d - disk ?p - peg)
    (smaller ?x - disk ?y - disk)
    (permitted ?a - agent ?d - disk)
    (current-step ?s - step)
    (next ?s1 - step ?s2 - step)
    (must-move ?s - step ?a - agent ?d - disk ?from - peg ?to - peg)
    (next-top ?p - peg ?dold - disk ?dnew - disk)
    (becomes-empty ?p - peg ?d - disk)
  )

  ; move a top disk from a peg to an empty peg; source peg will still have a top (next-top)
  (:action move-to-empty-with-next
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?s - step ?s2 - step ?d2 - disk)
    :precondition (and
      (permitted ?a ?d)
      (on ?d ?from)
      (top ?d ?from)
      (clear ?to)
      (current-step ?s)
      (next ?s ?s2)
      (must-move ?s ?a ?d ?from ?to)
      (next-top ?from ?d ?d2)
    )
    :effect (and
      (not (current-step ?s))
      (current-step ?s2)
      (not (top ?d ?from))
      (top ?d2 ?from)
      (not (clear ?to))
      (top ?d ?to)
      (on ?d ?to)
      (not (on ?d ?from))
    )
  )

  ; move a top disk from a peg to an empty peg; source peg becomes empty
  (:action move-to-empty-last
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?s - step ?s2 - step)
    :precondition (and
      (permitted ?a ?d)
      (on ?d ?from)
      (top ?d ?from)
      (clear ?to)
      (current-step ?s)
      (next ?s ?s2)
      (must-move ?s ?a ?d ?from ?to)
      (becomes-empty ?from ?d)
    )
    :effect (and
      (not (current-step ?s))
      (current-step ?s2)
      (not (top ?d ?from))
      (clear ?from)
      (not (clear ?to))
      (top ?d ?to)
      (on ?d ?to)
      (not (on ?d ?from))
    )
  )

  ; move a top disk from a peg onto a non-empty peg's top; source peg will still have a top (next-top)
  (:action move-onto-top-with-next
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?t - disk ?s - step ?s2 - step ?d2 - disk)
    :precondition (and
      (permitted ?a ?d)
      (on ?d ?from)
      (top ?d ?from)
      (top ?t ?to)
      (smaller ?d ?t)
      (current-step ?s)
      (next ?s ?s2)
      (must-move ?s ?a ?d ?from ?to)
      (next-top ?from ?d ?d2)
    )
    :effect (and
      (not (current-step ?s))
      (current-step ?s2)
      (not (top ?d ?from))
      (top ?d2 ?from)
      (not (top ?t ?to))
      (top ?d ?to)
      (on ?d ?to)
      (not (on ?d ?from))
    )
  )

  ; move a top disk from a peg onto a non-empty peg's top; source peg becomes empty
  (:action move-onto-top-last
    :parameters (?a - agent ?d - disk ?from - peg ?to - peg ?t - disk ?s - step ?s2 - step)
    :precondition (and
      (permitted ?a ?d)
      (on ?d ?from)
      (top ?d ?from)
      (top ?t ?to)
      (smaller ?d ?t)
      (current-step ?s)
      (next ?s ?s2)
      (must-move ?s ?a ?d ?from ?to)
      (becomes-empty ?from ?d)
    )
    :effect (and
      (not (current-step ?s))
      (current-step ?s2)
      (not (top ?d ?from))
      (clear ?from)
      (not (top ?t ?to))
      (top ?d ?to)
      (on ?d ?to)
      (not (on ?d ?from))
    )
  )
)