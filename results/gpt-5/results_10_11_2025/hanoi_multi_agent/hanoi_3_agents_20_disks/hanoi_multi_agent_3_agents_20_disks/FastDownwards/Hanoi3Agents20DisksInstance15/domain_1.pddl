(define (domain hanoi_3peg_multiagent_ordered)
  (:requirements :strips :typing :negative-preconditions)
  (:types
    agent step thing
    disk peg - thing
  )
  (:predicates
    (on ?d - disk ?x - thing)
    (clear ?d - disk)
    (empty ?p - peg)
    (top-of ?p - peg ?d - disk)
    (at-peg ?d - disk ?p - peg)
    (smaller ?d1 - disk ?d2 - disk)
    (authorized ?a - agent ?d - disk)
    (immobile ?d - disk)
    (allowed-move ?s - step ?a - agent ?d - disk ?from - peg ?to - peg)
    (current-step ?s - step)
    (next ?s1 - step ?s2 - step)
  )

  (:action move_from_disk_to_empty
    :parameters (?ag - agent ?d - disk ?from - peg ?to - peg ?s - disk ?st - step ?nst - step)
    :precondition (and
      (authorized ?ag ?d)
      (not (immobile ?d))
      (at-peg ?d ?from)
      (at-peg ?s ?from)
      (on ?d ?s)
      (clear ?d)
      (top-of ?from ?d)
      (empty ?to)
      (current-step ?st)
      (allowed-move ?st ?ag ?d ?from ?to)
      (next ?st ?nst)
    )
    :effect (and
      (not (on ?d ?s))
      (clear ?s)
      (not (empty ?to))
      (on ?d ?to)
      (top-of ?to ?d)
      (not (top-of ?from ?d))
      (top-of ?from ?s)
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (not (current-step ?st))
      (current-step ?nst)
    )
  )

  (:action move_from_disk_to_disk
    :parameters (?ag - agent ?d - disk ?from - peg ?to - peg ?s - disk ?t - disk ?st - step ?nst - step)
    :precondition (and
      (authorized ?ag ?d)
      (not (immobile ?d))
      (at-peg ?d ?from)
      (at-peg ?s ?from)
      (on ?d ?s)
      (clear ?d)
      (top-of ?from ?d)
      (at-peg ?t ?to)
      (top-of ?to ?t)
      (smaller ?d ?t)
      (current-step ?st)
      (allowed-move ?st ?ag ?d ?from ?to)
      (next ?st ?nst)
    )
    :effect (and
      (not (on ?d ?s))
      (clear ?s)
      (not (top-of ?from ?d))
      (top-of ?from ?s)
      (not (top-of ?to ?t))
      (not (clear ?t))
      (on ?d ?t)
      (top-of ?to ?d)
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (not (current-step ?st))
      (current-step ?nst)
    )
  )

  (:action move_from_peg_to_empty
    :parameters (?ag - agent ?d - disk ?from - peg ?to - peg ?st - step ?nst - step)
    :precondition (and
      (authorized ?ag ?d)
      (not (immobile ?d))
      (at-peg ?d ?from)
      (on ?d ?from)
      (clear ?d)
      (top-of ?from ?d)
      (empty ?to)
      (current-step ?st)
      (allowed-move ?st ?ag ?d ?from ?to)
      (next ?st ?nst)
    )
    :effect (and
      (not (on ?d ?from))
      (empty ?from)
      (not (empty ?to))
      (on ?d ?to)
      (top-of ?to ?d)
      (not (top-of ?from ?d))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (not (current-step ?st))
      (current-step ?nst)
    )
  )

  (:action move_from_peg_to_disk
    :parameters (?ag - agent ?d - disk ?from - peg ?to - peg ?t - disk ?st - step ?nst - step)
    :precondition (and
      (authorized ?ag ?d)
      (not (immobile ?d))
      (at-peg ?d ?from)
      (on ?d ?from)
      (clear ?d)
      (top-of ?from ?d)
      (at-peg ?t ?to)
      (top-of ?to ?t)
      (smaller ?d ?t)
      (current-step ?st)
      (allowed-move ?st ?ag ?d ?from ?to)
      (next ?st ?nst)
    )
    :effect (and
      (not (on ?d ?from))
      (empty ?from)
      (not (top-of ?to ?t))
      (not (clear ?t))
      (on ?d ?t)
      (top-of ?to ?d)
      (not (top-of ?from ?d))
      (not (at-peg ?d ?from))
      (at-peg ?d ?to)
      (not (current-step ?st))
      (current-step ?nst)
    )
  )
)