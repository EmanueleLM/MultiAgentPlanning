    (step-disk s3 c) (step-from s3 left)  (step-to s3 middle) (step-below s3 middle)
    (step-disk s4 a) (step-from s4 right) (step-to s4 middle) (step-below s4 c)
    (step-disk s5 b) (step-from s5 right) (step-to s5 left)  (step-below s5 d)
  )

  (:goal
    (and
      ; goal stacks
      (on a c)
      (on c middle)

      (on b d)
      (on d e)
      (on e f)
      (on f g)
      (on g h)
      (on h i)
      (on i j)
      (on j k)
      (on k l)
