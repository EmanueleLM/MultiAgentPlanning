(define (problem Blocksworld15-Scenario1)
  (:domain BlocksWorld-STRIPS)
  (:objects red blue yellow orange - block)
  (:init
    (ontable red)
    (ontable blue)
    (ontable orange)
    (on yellow orange)
    (clear red)
    (clear blue)
    (clear yellow)
    (handempty)
    (= (total-cost) 0)
  )
  (:goal (and
    (on orange blue)
    (on yellow red)
  ))
  (:metric minimize (total-cost))
)