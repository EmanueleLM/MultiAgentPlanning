(define (domain multiagent-tasks)
  (:requirements :typing :negative-preconditions)
  (:types agent task resource slot)
  (:constants paltry sip clip wretched memory tightfisted - task)
  (:predicates
    (is-agent ?a - agent)
    (is-task ?t - task)
    (is-resource ?r - resource)
    (is-slot ?s - slot)
    (earliest ?s - slot)
    (available ?r - resource)
    (slot-free ?s - slot)
    (requires ?t - task ?r - resource)
    (assigned ?t - task ?a - agent)
    (assigned-slot ?t - task ?s - slot)
    (completed ?t - task)
  )

  (:action paltry
    :parameters (?a - agent ?s - slot ?r - resource)
    :precondition (and
      (is-agent ?a)
      (is-task paltry)
      (is-slot ?s)
      (earliest ?s)
      (slot-free ?s)
      (is-resource ?r)
      (available ?r)
      (requires paltry ?r)
    )
    :effect (and
      (not (slot-free ?s))
      (not (available ?r))
      (assigned paltry ?a)
      (assigned-slot paltry ?s)
      (completed paltry)
    )
  )

  (:action sip
    :parameters (?a - agent ?s - slot ?r - resource)
    :precondition (and
      (is-agent ?a)
      (is-task sip)
      (is-slot ?s)
      (earliest ?s)
      (slot-free ?s)
      (is-resource ?r)
      (available ?r)
      (requires sip ?r)
    )
    :effect (and
      (not (slot-free ?s))
      (not (available ?r))
      (assigned sip ?a)
      (assigned-slot sip ?s)
      (completed sip)
    )
  )

  (:action clip
    :parameters (?a - agent ?s - slot ?r - resource)
    :precondition (and
      (is-agent ?a)
      (is-task clip)
      (is-slot ?s)
      (slot-free ?s)
      (is-resource ?r)
      (available ?r)
      (requires clip ?r)
    )
    :effect (and
      (not (slot-free ?s))
      (not (available ?r))
      (assigned clip ?a)
      (assigned-slot clip ?s)
      (completed clip)
    )
  )

  (:action wretched
    :parameters (?a - agent ?s - slot ?r - resource)
    :precondition (and
      (is-agent ?a)
      (is-task wretched)
      (is-slot ?s)
      (slot-free ?s)
      (is-resource ?r)
      (available ?r)
      (requires wretched ?r)
    )
    :effect (and
      (not (slot-free ?s))
      (not (available ?r))
      (assigned wretched ?a)
      (assigned-slot wretched ?s)
      (completed wretched)
    )
  )

  (:action memory
    :parameters (?a - agent ?s - slot ?r - resource)
    :precondition (and
      (is-agent ?a)
      (is-task memory)
      (is-slot ?s)
      (slot-free ?s)
      (is-resource ?r)
      (available ?r)
      (requires memory ?r)
    )
    :effect (and
      (not (slot-free ?s))
      (not (available ?r))
      (assigned memory ?a)
      (assigned-slot memory ?s)
      (completed memory)
    )
  )

  (:action tightfisted
    :parameters (?a - agent ?s - slot ?r - resource)
    :precondition (and
      (is-agent ?a)
      (is-task tightfisted)
      (is-slot ?s)
      (slot-free ?s)
      (is-resource ?r)
      (available ?r)
      (requires tightfisted ?r)
    )
    :effect (and
      (not (slot-free ?s))
      (not (available ?r))
      (assigned tightfisted ?a)
      (assigned-slot tightfisted ?s)
      (completed tightfisted)
    )
  )
)