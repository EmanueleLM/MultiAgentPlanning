(define (domain meeting-scheduling)
  (:requirements :typing :negative-preconditions)
  (:types person slot)
  (:predicates
    (available ?p - person ?s - slot)
    (feasible ?s - slot)
    (meeting-scheduled)
    (scheduled ?s - slot)
  )
  (:action schedule-by-alice-t1500
