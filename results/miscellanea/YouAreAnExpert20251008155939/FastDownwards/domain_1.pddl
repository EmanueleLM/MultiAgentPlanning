(define (domain meeting-scheduling)
  (:requirements :typing :negative-preconditions)
  (:types agent slot)
  (:predicates
    (next ?s1 - slot ?s2 - slot)
    (busy ?a - agent ?s - slot)
    (scheduled ?s1 - slot ?s2 - slot)
    (meeting-scheduled)
  )
  (:action schedule
    :parameters (?a1 - agent ?a2 - agent ?a3 - agent ?s1 - slot ?s2 - slot)
    :precondition (and
      (next ?s1 ?s2)
      (not (meeting-scheduled))
      (not (busy ?a1 ?s1)) (not (busy ?a1 ?s2))
      (not (busy ?a2 ?s1)) (not (busy ?a2 ?s2))
      (not (busy ?a3 ?s1)) (not (busy ?a3 ?s2))
    )
    :effect (and
      (meeting-scheduled)
      (scheduled ?s1 ?s2)
      (busy ?a1 ?s1) (busy ?a1 ?s2)
      (busy ?a2 ?s1) (busy ?a2 ?s2)
      (busy ?a3 ?s1) (busy ?a3 ?s2)
    )
  )
)