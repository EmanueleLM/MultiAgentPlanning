(define (domain scheduling)
  (:requirements :typing :negative-preconditions)
  (:types person time)
  (:constants michelle steven jerry - person)
  (:predicates
    (free ?p - person ?t - time)
    (next ?t1 - time ?t2 - time)
    (meeting-scheduled)
    (scheduled-at ?t - time)
  )
  (:action schedule
    :parameters (?t - time ?t2 - time)
    :precondition (and (not (meeting-scheduled)) (next ?t ?t2)
                       (free michelle ?t) (free michelle ?t2)
                       (free steven ?t) (free steven ?t2)
                       (free jerry ?t) (free jerry ?t2))
    :effect (and (meeting-scheduled) (scheduled-at ?t))
  )
)