(define (problem combined-blocks-problem)
  (:domain combined-blocks)
  (:objects
    A B C D E F G H I J K L M N O - block
  )

  (:init
    ;; Table bases (from the agent inputs)
    (ontable C)
    (ontable H)
    (ontable O)

    ;; Stacks / on relations (consistent between agents)
    (on G C)
    (on F G)
    (on A F)
    (on D A)
    (on M D)
    (on B M)
    (on K B)
    (on I K)
    (on E I)
    (on J H)
    (on N J)
    (on L O)

    ;; Clear (top) blocks in initial state
    (clear E)
    (clear N)
    (clear L)

    ;; Vowel / Consonant assignments (merged)
    (vowel A)
    (vowel E)
    (vowel I)
    (vowel O)

    (consonant B)
    (consonant C)
    (consonant D)
    (consonant F)
    (consonant G)
    (consonant H)
    (consonant J)
    (consonant K)
    (consonant L)
    (consonant M)
    (consonant N)
  )

  (:goal (and
    (on A B)
    (on B C)
    (on C D)
    (on D E)
    (on E F)
    (on F G)
    (on G H)
    (on H I)
    (on I J)
    (on J K)
    (on K L)
    (on L M)
    (on M N)
    (on N O)
    (ontable O)
  ))
)