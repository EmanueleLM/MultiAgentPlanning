    b c d f g h j k l - consonant
  )

  (:init
    (ontable a)
    (on i a)
    (on l i)
    (on k l)
    (on e k)
    (on f e)
    (ontable c)
    (on d c)
    (on h d)
    (on g h)
    (on j g)
    (on b j)
    (clear f)
    (clear b)
    (handempty-v)
    (handempty-c)
    (free a)
    (free b)
    (free c)
    (free d)
    (free e)
    (free f)
    (free g)
    (free h)
    (free i)
    (free j)
    (free k)
    (free l)
