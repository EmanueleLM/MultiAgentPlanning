(define (domain meeting-scheduling)
  (:requirements :strips :typing)
  (:types person time)
  (:predicates
    (free ?p - person ?t - time)
    (slot-free ?t - time)
    (earliest-slot ?t - time)
    (scheduled ?t - time)
    (meeting-scheduled)
  )
  (:action schedule
    :parameters (?t - time)
    :precondition (and (slot-free ?t) (earliest-slot ?t))
    :effect (and (scheduled ?t) (meeting-scheduled))
  )
)