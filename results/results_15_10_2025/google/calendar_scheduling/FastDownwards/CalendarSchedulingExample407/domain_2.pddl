(define (domain meeting-scheduling)
  (:requirements :typing :negative-preconditions :equality :action-costs)
  (:types participant time)
  (:predicates
    (slot ?t - time)
    (free ?p - participant ?t - time)
    (attending ?p - participant)
    (meeting-scheduled)
    (meeting-start ?t - time)
  )
  (:functions
    (time-index ?t - time)
    (total-cost)
  )
  (:action schedule-meeting
    :parameters (?t - time)
    :precondition (and
      (slot ?t)
      (not (meeting-scheduled))
      (free alice ?t)
      (free daniel ?t)
      (free scott ?t)
      (free lori ?t)
      (free cynthia ?t)
      (free jeffrey ?t)
      (free hannah ?t)
    )
    :effect (and
      (meeting-scheduled)
      (meeting-start ?t)
      (increase (total-cost) (time-index ?t))
      (attending alice) (not (free alice ?t))
      (attending daniel) (not (free daniel ?t))
      (attending scott) (not (free scott ?t))
      (attending lori) (not (free lori ?t))
      (attending cynthia) (not (free cynthia ?t))
      (attending jeffrey) (not (free jeffrey ?t))
      (attending hannah) (not (free hannah ?t))
    )
  )
)