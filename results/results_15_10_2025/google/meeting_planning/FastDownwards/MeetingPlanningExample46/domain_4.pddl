(define (domain meeting-planning-adapted)
  (:requirements :typing :strips :negative-preconditions :fluents :action-costs :equality)
  (:types person location time)
  (:constants haight_ashbury north_beach - location)
  (:predicates
    (at ?p - person ?l - location ?t - time)
    (free-interval ?p - person ?ts - time ?te - time)
    (after19 ?ts - time ?te - time)
    (after18 ?ts - time ?te - time)
    (after90 ?ts - time ?te - time)
    (robert-available-interval ?ts - time ?te - time)
    (met-robert)
  )
  (:functions (total-cost))
  (:action move-haight-to-north
    :parameters (?p - person ?ts - time ?te - time)
    :precondition (and (at ?p haight_ashbury ?ts) (free-interval ?p ?ts ?te) (after19 ?ts ?te))
    :effect (and (not (at ?p haight_ashbury ?ts)) (at ?p north_beach ?te) (not (free-interval ?p ?ts ?te)) (increase (total-cost) 19))
  )
  (:action move-north-to-haight
    :parameters (?p - person ?ts - time ?te - time)
    :precondition (and (at ?p north_beach ?ts) (free-interval ?p ?ts ?te) (after18 ?ts ?te))
    :effect (and (not (at ?p north_beach ?ts)) (at ?p haight_ashbury ?te) (not (free-interval ?p ?ts ?te)) (increase (total-cost) 18))
  )
  (:action meet-robert
    :parameters (?p - person ?ts - time ?te - time)
    :precondition (and (at ?p north_beach ?ts) (free-interval ?p ?ts ?te) (after90 ?ts ?te) (robert-available-interval ?ts ?te))
    :effect (and (at ?p north_beach ?te) (not (free-interval ?p ?ts ?te)) (met-robert) (increase (total-cost) 1))
  )
)