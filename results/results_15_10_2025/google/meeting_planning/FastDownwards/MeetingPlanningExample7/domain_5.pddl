(define (domain meeting-scheduling)
  (:requirements :strips :typing :negative-preconditions :equality :adl :action-costs)
  (:types agent location time)
  (:predicates
    (at ?a - agent ?l - location ?t - time)
    (succ ?t1 - time ?t2 - time)
    (can-travel ?from - location ?to - location ?t1 - time ?t2 - time)
    (john-present ?t - time)
    (allowed-meet ?s - time ?e - time)
    (met)
  )
  (:functions (total-cost))
  (:action wait
    :parameters (?a - agent ?loc - location ?t - time ?t2 - time)
    :precondition (and (at ?a ?loc ?t) (succ ?t ?t2))
    :effect (and (not (at ?a ?loc ?t)) (at ?a ?loc ?t2) (increase (total-cost) 0))
  )
  (:action travel-rd-nb
    :parameters (?t - time ?t2 - time)
    :precondition (and (at traveler Richmond_District ?t) (can-travel Richmond_District North_Beach ?t ?t2))
    :effect (and (not (at traveler Richmond_District ?t)) (at traveler North_Beach ?t2) (increase (total-cost) 0))
  )
  (:action travel-nb-rd
    :parameters (?t - time ?t2 - time)
    :precondition (and (at traveler North_Beach ?t) (can-travel North_Beach Richmond_District ?t ?t2))
    :effect (and (not (at traveler North_Beach ?t)) (at traveler Richmond_District ?t2) (increase (total-cost) 0))
  )
  (:action meet-john
    :parameters (?s - time ?e - time)
    :precondition (and (at traveler North_Beach ?s) (allowed-meet ?s ?e) (not (met)))
    :effect (and (not (at traveler North_Beach ?s)) (at traveler North_Beach ?e) (met) (increase (total-cost) 0))
  )
)