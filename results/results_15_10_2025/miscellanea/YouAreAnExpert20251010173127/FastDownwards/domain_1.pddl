(define (domain meeting-schedule)
  (:requirements :typing :negative-preconditions)
  (:types participant timeslot)
  (:predicates
    (busy ?p - participant ?t - timeslot)
    (occupied ?start - timeslot ?t - timeslot)
    (meeting-scheduled)
    (meeting-at ?s - timeslot)
  )
  (:action schedule-meeting
    :parameters (?s - timeslot ?t1 - timeslot ?t2 - timeslot ?p1 - participant ?p2 - participant ?p3 - participant)
    :precondition (and
      (occupied ?s ?t1)
      (occupied ?s ?t2)
      (not (busy ?p1 ?t1))
      (not (busy ?p1 ?t2))
      (not (busy ?p2 ?t1))
      (not (busy ?p2 ?t2))
      (not (busy ?p3 ?t1))
      (not (busy ?p3 ?t2))
    )
    :effect (and (meeting-scheduled) (meeting-at ?s))
  )
)