(define (domain schedule-meeting)
  (:requirements :strips :typing :negative-preconditions :action-costs)
  (:types person slot)
  (:constants gerald roy barbara - person)
  (:predicates
    (available ?p - person ?s - slot)
    (before13 ?s - slot)
    (after13 ?s - slot)
    (scheduled)
    (meeting-at ?s - slot)
  )
  (:functions (total-cost))
  (:action schedule-meeting-before13
    :parameters (?s - slot)
    :precondition (and
      (available gerald ?s)
      (available roy ?s)
      (available barbara ?s)
      (before13 ?s)
      (not (scheduled))
    )
    :effect (and
      (scheduled)
      (meeting-at ?s)
      (increase (total-cost) 1)
    )
  )
  (:action schedule-meeting-after13
    :parameters (?s - slot)
    :precondition (and
      (available gerald ?s)
      (available roy ?s)
      (available barbara ?s)
      (after13 ?s)
      (not (scheduled))
    )
    :effect (and
      (scheduled)
      (meeting-at ?s)
      (increase (total-cost) 0)
    )
  )
)