(define (problem player1-corrected)
  (:domain blocks)
  (:requirements :strips :typing)
  (:objects blue yellow red orange - block)
  ;; Initial state (player1, corrected: yellow is NOT clear initially)
  (:init
    (on blue yellow)
    (ontable yellow)
    (ontable red)
    (ontable orange)
    (clear blue)
    (clear red)
    (clear orange)
    (handempty)
  )
  ;; Goal: blue on orange AND yellow on red
  (:goal (and
    (on blue orange)
    (on yellow red)
  ))
)