(define (problem integrated-blocks-problem)
  (:domain integrated-blocks)
  (:objects A B C D E F G H I J - block)
  (:init
    ;; table positions
    (ontable A)
    (ontable J)

    ;; initial stack: J bottom, then I, H, G, F, E, D, C, B (B is top)
    (on I J)
    (on H I)
    (on G H)
    (on F G)
    (on E F)
    (on D E)
    (on C D)
    (on B C)

    ;; clear blocks (tops of stacks)
    (clear A)
    (clear B)

    ;; agent capabilities: vowels and consonants (as provided)
    (vowel A)
    (vowel E)
    (vowel I)

    (consonant B)
    (consonant C)
    (consonant D)
    (consonant F)
    (consonant G)
    (consonant H)
    (consonant J)
  )

  (:goal (and
    (on A B)
    (on B C)
    (on C D)
    (on D E)
    (on E F)
    (on F G)
    (on G H)
    (on H I)
    (on I J)
    (ontable J)
  ))
)