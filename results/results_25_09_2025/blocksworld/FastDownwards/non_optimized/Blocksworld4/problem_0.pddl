(define (problem multi_agent_blocks_problem)
  (:domain multi_agent_blocks)
  (:objects A B C D E F G H I J)
  (:init
    ;; Capabilities
    (vowel A)
    (vowel E)
    (vowel I)

    (consonant B)
    (consonant C)
    (consonant D)
    (consonant F)
    (consonant G)
    (consonant H)
    (consonant J)

    ;; Initial placement: A on table; chain B on C on D on E on F on G on H on I on J; J on table
    (ontable A)
    (on B C)
    (on C D)
    (on D E)
    (on E F)
    (on F G)
    (on G H)
    (on H I)
    (on I J)
    (ontable J)

    ;; Clear/top facts: only tops of stacks are clear initially (A and B)
    (clear A)
    (clear B)
  )
  (:goal (and
    (on A B)
    (on B C)
    (on C D)
    (on D E)
    (on E F)
    (on F G)
    (on G H)
    (on H I)
    (on I J)
    (ontable J)
  ))
)