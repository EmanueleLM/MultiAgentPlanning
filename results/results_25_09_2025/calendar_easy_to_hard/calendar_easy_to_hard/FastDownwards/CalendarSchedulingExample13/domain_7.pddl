(define (domain meeting-scheduling-multiagent)
  (:requirements :strips :typing)
  (:types agent timeslot)
  (:predicates
    (free ?a - agent ?t - timeslot)
    (gerald-pref ?t - timeslot)
    (gerald-nonpref ?t - timeslot)
    (roy-pref ?t - timeslot)
    (roy-nonpref ?t - timeslot)
    (barbara-pref ?t - timeslot)
    (barbara-nonpref ?t - timeslot)
    (meeting-scheduled ?t - timeslot)
    (unscheduled)
    (scheduled-by-gerald ?t - timeslot)
    (scheduled-by-roy ?t - timeslot)
    (scheduled-by-barbara ?t - timeslot)
  )

  (:action schedule-by-gerald-pref
    :parameters (?t - timeslot)
    :precondition (and
      (free gerald ?t)
      (free roy ?t)
      (free barbara ?t)
      (unscheduled)
      (gerald-pref ?t)
    )
    :effect (and
      (meeting-scheduled ?t)
      (not (unscheduled))
      (scheduled-by-gerald ?t)
      (not (free gerald ?t))
      (not (free roy ?t))
      (not (free barbara ?t))
    )
  )

  (:action schedule-by-gerald-nonpref
    :parameters (?t - timeslot)
    :precondition (and
      (free gerald ?t)
      (free roy ?t)
      (free barbara ?t)
      (unscheduled)
      (gerald-nonpref ?t)
    )
    :effect (and
      (meeting-scheduled ?t)
      (not (unscheduled))
      (scheduled-by-gerald ?t)
      (not (free gerald ?t))
      (not (free roy ?t))
      (not (free barbara ?t))
    )
  )

  (:action schedule-by-roy-pref
    :parameters (?t - timeslot)
    :precondition (and
      (free gerald ?t)
      (free roy ?t)
      (free barbara ?t)
      (unscheduled)
      (roy-pref ?t)
    )
    :effect (and
      (meeting-scheduled ?t)
      (not (unscheduled))
      (scheduled-by-roy ?t)
      (not (free gerald ?t))
      (not (free roy ?t))
      (not (free barbara ?t))
    )
  )

  (:action schedule-by-roy-nonpref
    :parameters (?t - timeslot)
    :precondition (and
      (free gerald ?t)
      (free roy ?t)
      (free barbara ?t)
      (unscheduled)
      (roy-nonpref ?t)
    )
    :effect (and
      (meeting-scheduled ?t)
      (not (unscheduled))
      (scheduled-by-roy ?t)
      (not (free gerald ?t))
      (not (free roy ?t))
      (not (free barbara ?t))
    )
  )

  (:action schedule-by-barbara-pref
    :parameters (?t - timeslot)
    :precondition (and
      (free gerald ?t)
      (free roy ?t)
      (free barbara ?t)
      (unscheduled)
      (barbara-pref ?t)
    )
    :effect (and
      (meeting-scheduled ?t)
      (not (unscheduled))
      (scheduled-by-barbara ?t)
      (not (free gerald ?t))
      (not (free roy ?t))
      (not (free barbara ?t))
    )
  )

  (:action schedule-by-barbara-nonpref
    :parameters (?t - timeslot)
    :precondition (and
      (free gerald ?t)
      (free roy ?t)
      (free barbara ?t)
      (unscheduled)
      (barbara-nonpref ?t)
    )
    :effect (and
      (meeting-scheduled ?t)
      (not (unscheduled))
      (scheduled-by-barbara ?t)
      (not (free gerald ?t))
      (not (free roy ?t))
      (not (free barbara ?t))
    )
  )
)