(define (domain meeting-scheduling)
  (:requirements :strips)
  (:predicates
    (slot ?s)
    (unscheduled)
    (meeting-scheduled)
    (meeting-at ?s)
    (available ?person ?s)
  )
  (:action schedule
    :parameters (?s ?d ?h ?a ?e ?b ?deb)
    :precondition (and (unscheduled) (slot ?s)
                       (available ?d ?s)
                       (available ?h ?s)
                       (available ?a ?s)
                       (available ?e ?s)
                       (available ?b ?s)
                       (available ?deb ?s))
    :effect (and (not (unscheduled))
                 (meeting-scheduled)
                 (meeting-at ?s))
  )
)