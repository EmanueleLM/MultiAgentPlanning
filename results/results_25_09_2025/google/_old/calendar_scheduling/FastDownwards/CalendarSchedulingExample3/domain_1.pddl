(define (domain orchestrator-scheduling)
  (:requirements :strips :typing :negative-preconditions)
  (:types slot hour_start)
  (:predicates
    (free_arthur ?s - slot)
    (free_michael ?h - hour_start)
    (free_samantha ?s - slot)
    (succ ?s1 ?s2 - slot)
    (hour_slots ?h - hour_start ?s1 ?s2 - slot)
    (covers_half ?s - slot ?h1 ?h2 - hour_start)
    (meeting_scheduled)
    (scheduled_at ?s - slot)
  )
  (:action schedule_arthur_michael_samantha_on_hour
    :parameters (?h - hour_start ?s1 - slot ?s2 - slot)
    :precondition (and
      (hour_slots ?h ?s1 ?s2)
      (succ ?s1 ?s2)
      (free_michael ?h)
      (free_arthur ?s1)
      (free_arthur ?s2)
      (free_samantha ?s1)
      (free_samantha ?s2)
    )
    :effect (and
      (meeting_scheduled)
      (scheduled_at ?s1)
      (not (free_arthur ?s1))
      (not (free_arthur ?s2))
      (not (free_samantha ?s1))
      (not (free_samantha ?s2))
      (not (free_michael ?h))
    )
  )
  (:action schedule_arthur_michael_samantha_on_half
    :parameters (?s - slot ?s2 - slot ?h1 - hour_start ?h2 - hour_start)
    :precondition (and
      (succ ?s ?s2)
      (covers_half ?s ?h1 ?h2)
      (free_michael ?h1)
      (free_michael ?h2)
      (free_arthur ?s)
      (free_arthur ?s2)
      (free_samantha ?s)
      (free_samantha ?s2)
    )
    :effect (and
      (meeting_scheduled)
      (scheduled_at ?s)
      (not (free_arthur ?s))
      (not (free_arthur ?s2))
      (not (free_samantha ?s))
      (not (free_samantha ?s2))
      (not (free_michael ?h1))
      (not (free_michael ?h2))
    )
  )
)