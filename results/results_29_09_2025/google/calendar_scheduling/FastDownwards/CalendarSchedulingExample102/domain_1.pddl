(define (domain calendar-scheduling)
  (:requirements :strips :typing)
  (:types person time)
  (:predicates
    (available ?p - person ?t - time)
    (next ?t1 ?t2 - time)
    (scheduled)
    (meeting-start ?t - time)
  )
  (:action schedule
    :parameters (?t ?t2 - time)
    :precondition (and
      (next ?t ?t2)
      (available dylan ?t) (available dylan ?t2)
      (available kathryn ?t) (available kathryn ?t2)
      (available hannah ?t) (available hannah ?t2)
      (available anna ?t) (available anna ?t2)
    )
    :effect (and (scheduled) (meeting-start ?t))
  )
)