(define (domain calendar-scheduling)
  (:requirements :strips :typing :negative-preconditions)
  (:types timeslot person)
  (:predicates
    (available ?p - person ?t - timeslot)
    (scheduled ?t - timeslot)
    (attending ?p - person ?t - timeslot)
  )
  (:action schedule-meeting
    :parameters (?t - timeslot ?p1 - person ?p2 - person ?p3 - person ?p4 - person)
    :precondition (and (not (scheduled ?t))
                       (available ?p1 ?t) (available ?p2 ?t) (available ?p3 ?t) (available ?p4 ?t))
    :effect (and (scheduled ?t)
                 (attending ?p1 ?t) (attending ?p2 ?t) (attending ?p3 ?t) (attending ?p4 ?t)))
)