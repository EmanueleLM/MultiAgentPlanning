(define (domain calendar-scheduling)
  (:requirements :typing)
  (:types person timeslot)
  (:predicates
    (available ?p - person ?t - timeslot)
    (scheduled ?t - timeslot)
  )
  (:action schedule
    :parameters (?t - timeslot)
    :precondition (and
      (available julia ?t)
      (available joshua ?t)
      (available nicholas ?t)
      (available david ?t)
      (available melissa ?t)
      (not (scheduled ?t))
    )
    :effect (and (scheduled ?t))
  )
)