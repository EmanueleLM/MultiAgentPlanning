(define (domain schedule-meeting)
  (:requirements :typing :strips :equality)
  (:types person time orchestrator-type)
  (:constants michael samuel aaron judith kevin - person)
  (:predicates
    (slot ?t - time)
    (free ?p - person ?t - time)
    (orchestrator ?o - orchestrator-type)
    (scheduled ?t - time)
    (meeting-scheduled)
  )
  (:action schedule-by-orchestrator
    :parameters (?o - orchestrator-type ?t - time)
    :precondition (and
      (orchestrator ?o)
      (slot ?t)
      (free michael ?t)
      (free samuel ?t)
      (free aaron ?t)
      (free judith ?t)
      (free kevin ?t)
    )
    :effect (and
      (scheduled ?t)
      (meeting-scheduled)
    )
  )
)