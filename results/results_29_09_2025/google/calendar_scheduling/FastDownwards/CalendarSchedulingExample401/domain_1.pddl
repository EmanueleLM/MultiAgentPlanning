(define (domain meeting-scheduling)
  (:requirements :typing)
  (:types person start)
  (:predicates
    (available ?p - person ?s - start)
    (all-available ?s - start)
    (scheduled ?s - start)
    (meeting-scheduled)
  )
  (:action schedule-at
    :parameters (?s - start)
    :precondition (and (all-available ?s))
    :effect (and (scheduled ?s) (meeting-scheduled))
  )
)