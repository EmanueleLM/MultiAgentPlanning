(define (domain itinerary)
  (:requirements :strips :typing)
  (:types agent city day)
  (:predicates
    (at ?a - agent ?c - city ?d - day)
    (next ?d1 - day ?d2 - day)
    (direct ?c1 - city ?c2 - city)
    (visited ?c - city)
    (met ?d - day)
    (allowed_meeting ?d - day)
    (meeting_city ?c - city)
  )
  (:action traveler-stay
    :parameters (?a - agent ?c - city ?d - day ?d2 - day)
    :precondition (and (at ?a ?c ?d) (next ?d ?d2))
    :effect (and (at ?a ?c ?d2) (visited ?c))
  )
  (:action traveler-fly
    :parameters (?a - agent ?from - city ?to - city ?d - day ?d2 - day)
    :precondition (and (at ?a ?from ?d) (next ?d ?d2) (direct ?from ?to))
    :effect (and (at ?a ?to ?d2) (visited ?to))
  )
  (:action traveler-meet
    :parameters (?a - agent ?c - city ?d - day)
    :precondition (and (at ?a ?c ?d) (allowed_meeting ?d) (meeting_city ?c))
    :effect (met ?d)
  )
)