(define (domain vault-cooperation)
  (:requirements :typing :strips :fluents)
  (:types agent object location)
  (:predicates
    (at ?a - agent ?l - location)
    (has-key ?a - agent)
    (vault-closed)
    (vault-open)
    (object-at ?o - object ?l - location)
    (object-grabbed ?o - object)
    (agent-large ?a - agent)
    (agent-small ?a - agent)
  )
  (:action move
    :parameters (?a - agent ?from - location ?to - location)
    :precondition (at ?a ?from)
    :effect (and (not (at ?a ?from)) (at ?a ?to))
  )
  (:action open-vault
    :parameters (?a - agent)
    :precondition (and (at ?a outside) (has-key ?a) (vault-closed))
    :effect (and (not (vault-closed)) (vault-open))
  )
  (:action transfer-key
    :parameters (?from - agent ?to - agent ?loc - location)
    :precondition (and (has-key ?from) (agent-large ?from) (agent-small ?to) (at ?from ?loc) (at ?to ?loc))
    :effect (and (not (has-key ?from)) (has-key ?to))
  )
  (:action enter-vault
    :parameters (?a - agent)
    :precondition (and (at ?a outside) (vault-open) (agent-small ?a))
    :effect (and (not (at ?a outside)) (at ?a vault))
  )
  (:action grab-object
    :parameters (?a - agent ?o - object)
    :precondition (and (at ?a vault) (object-at ?o vault) (agent-small ?a))
    :effect (and (object-grabbed ?o) (not (object-at ?o vault)))
  )
)