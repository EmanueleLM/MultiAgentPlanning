(define (domain multiagent-blocks)
  (:requirements :strips :negative-preconditions :equality)
  (:predicates
    (vowel ?b)
    (consonant ?b)
    (used ?s)
    (available ?s)
    (base ?s ?b)
    (top ?s ?b)
    (on ?b ?under)
    (is-table ?t)
    (clear ?b)
    (freeV)
    (freeC)
    (holdingV ?b)
    (holdingC ?b)
  )

  (:action v-pickup-from-table
    :parameters (?b ?s ?t)
    :precondition (and
      (vowel ?b)
      (used ?s)
      (top ?s ?b)
      (freeV)
      (on ?b ?t)
      (is-table ?t)
    )
    :effect (and
      (not (top ?s ?b))
      (not (on ?b ?t))
      (not (clear ?b))
      (holdingV ?b)
      (not (freeV))
      (not (used ?s))
      (available ?s)
      (not (base ?s ?b))
    )
  )

  (:action v-pickup-from-block
    :parameters (?b ?s ?under)
    :precondition (and
      (vowel ?b)
      (used ?s)
      (top ?s ?b)
      (freeV)
      (on ?b ?under)
      (not (is-table ?under))
    )
    :effect (and
      (not (top ?s ?b))
      (not (on ?b ?under))
      (not (clear ?b))
      (holdingV ?b)
      (not (freeV))
      (top ?s ?under)
      (clear ?under)
    )
  )

  (:action v-place-on-block
    :parameters (?b ?target ?s)
    :precondition (and
      (vowel ?b)
      (holdingV ?b)
      (used ?s)
      (top ?s ?target)
      (not (holdingC ?target))
    )
    :effect (and
      (on ?b ?target)
      (top ?s ?b)
      (clear ?b)
      (not (top ?s ?target))
      (not (clear ?target))
      (not (holdingV ?b))
      (freeV)
    )
  )

  (:action v-place-on-table
    :parameters (?b ?s ?t)
    :precondition (and
      (vowel ?b)
      (holdingV ?b)
      (available ?s)
      (is-table ?t)
    )
    :effect (and
      (not (holdingV ?b))
      (freeV)
      (used ?s)
      (not (available ?s))
      (base ?s ?b)
      (top ?s ?b)
      (on ?b ?t)
      (clear ?b)
    )
  )

  (:action c-pickup-from-table
    :parameters (?b ?s ?t)
    :precondition (and
      (consonant ?b)
      (used ?s)
      (top ?s ?b)
      (freeC)
      (on ?b ?t)
      (is-table ?t)
    )
    :effect (and
      (not (top ?s ?b))
      (not (on ?b ?t))
      (not (clear ?b))
      (holdingC ?b)
      (not (freeC))
      (not (used ?s))
      (available ?s)
      (not (base ?s ?b))
    )
  )

  (:action c-pickup-from-block
    :parameters (?b ?s ?under)
    :precondition (and
      (consonant ?b)
      (used ?s)
      (top ?s ?b)
      (freeC)
      (on ?b ?under)
      (not (is-table ?under))
    )
    :effect (and
      (not (top ?s ?b))
      (not (on ?b ?under))
      (not (clear ?b))
      (holdingC ?b)
      (not (freeC))
      (top ?s ?under)
      (clear ?under)
    )
  )

  (:action c-place-on-block
    :parameters (?b ?target ?s)
    :precondition (and
      (consonant ?b)
      (holdingC ?b)
      (used ?s)
      (top ?s ?target)
      (not (holdingV ?target))
    )
    :effect (and
      (on ?b ?target)
      (top ?s ?b)
      (clear ?b)
      (not (top ?s ?target))
      (not (clear ?target))
      (not (holdingC ?b))
      (freeC)
    )
  )

  (:action c-place-on-table
    :parameters (?b ?s ?t)
    :precondition (and
      (consonant ?b)
      (holdingC ?b)
      (available ?s)
      (is-table ?t)
    )
    :effect (and
      (not (holdingC ?b))
      (freeC)
      (used ?s)
      (not (available ?s))
      (base ?s ?b)
      (top ?s ?b)
      (on ?b ?t)
      (clear ?b)
    )
  )
)