(define (domain MeetingScheduler)
  (:requirements :strips :typing)
  (:types person location time)
  (:predicates 
      (at_location ?person - person ?location - location)
      (can_meet ?person1 - person ?person2 - person)
      (met ?person1 - person ?person2 - person)
      (current_time ?time - time)
      (available_time ?person - person ?time - time))
  
  (:action travel
    :parameters (?person - person ?from - location ?to - location ?current - time ?next - time)
    :precondition (and (at_location ?person ?from)
                       (current_time ?current))
    :effect (and (not (at_location ?person ?from))
                 (at_location ?person ?to)
                 (current_time ?next)
                 (not (available_time ?person ?current))
                 (available_time ?person ?next)))
  
  (:action meet
    :parameters (?person1 - person ?person2 - person ?location - location ?start-time - time ?end-time - time)
    :precondition (and (at_location ?person1 ?location) 
                       (at_location ?person2 ?location)
                       (can_meet ?person1 ?person2)
                       (current_time ?start-time)
                       (available_time ?person1 ?end-time)
                       (available_time ?person2 ?end-time))
    :effect (and (met ?person1 ?person2)
                 (current_time ?end-time)))
)