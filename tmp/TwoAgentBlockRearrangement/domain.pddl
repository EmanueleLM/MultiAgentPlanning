(define (domain orchestrator-blocks)
  (:requirements :strips :typing :negative-preconditions :action-costs)
  (:types block stack)
  (:predicates
    (block ?b - block)
    (stack ?s - stack)
    (vowel ?b - block)
    (consonant ?b - block)
    (on ?b - block ?x - (either block stack))
    (clear ?b - block)
    (empty ?s - stack)
    (holding-vowel ?b - block)
    (holding-cons ?b - block)
    (hand-free-vowel)
    (hand-free-cons)
  )
  (:functions (total-cost))
  ;; Vowel agent pickup from top of another block
  (:action pick-up-vowel-from-block
    :parameters (?b - block ?below - block)
    :precondition (and
      (vowel ?b)
      (hand-free-vowel)
      (on ?b ?below)
      (clear ?b)
      (not (= ?b ?below))
    )
    :effect (and
      (not (on ?b ?below))
      (holding-vowel ?b)
      (not (hand-free-vowel))
      (clear ?below)
      (increase (total-cost) 1)
    )
  )
  ;; Vowel agent pickup from stack (table)
  (:action pick-up-vowel-from-stack
    :parameters (?b - block ?s - stack)
    :precondition (and
      (vowel ?b)
      (hand-free-vowel)
      (on ?b ?s)
      (clear ?b)
    )
    :effect (and
      (not (on ?b ?s))
      (holding-vowel ?b)
      (not (hand-free-vowel))
      (empty ?s)
      (increase (total-cost) 1)
    )
  )
  ;; Vowel agent place on top of block
  (:action place-vowel-on-block
    :parameters (?b - block ?target - block)
    :precondition (and
      (vowel ?b)
      (holding-vowel ?b)
      (clear ?target)
      (not (holding-cons ?target))
      (not (= ?b ?target))
    )
    :effect (and
      (on ?b ?target)
      (not (clear ?target))
      (clear ?b)
      (not (holding-vowel ?b))
      (hand-free-vowel)
      (increase (total-cost) 1)
    )
  )
  ;; Vowel agent place on empty stack (on table)
  (:action place-vowel-on-empty-stack
    :parameters (?b - block ?s - stack)
    :precondition (and
      (vowel ?b)
      (holding-vowel ?b)
      (empty ?s)
    )
    :effect (and
      (on ?b ?s)
      (not (empty ?s))
      (clear ?b)
      (not (holding-vowel ?b))
      (hand-free-vowel)
      (increase (total-cost) 1)
    )
  )
  ;; Consonant agent pickup from top of another block
  (:action pick-up-cons-from-block
    :parameters (?b - block ?below - block)
    :precondition (and
      (consonant ?b)
      (hand-free-cons)
      (on ?b ?below)
      (clear ?b)
      (not (= ?b ?below))
    )
    :effect (and
      (not (on ?b ?below))
      (holding-cons ?b)
      (not (hand-free-cons))
      (clear ?below)
      (increase (total-cost) 1)
    )
  )
  ;; Consonant agent pickup from stack (table)
  (:action pick-up-cons-from-stack
    :parameters (?b - block ?s - stack)
    :precondition (and
      (consonant ?b)
      (hand-free-cons)
      (on ?b ?s)
      (clear ?b)
    )
    :effect (and
      (not (on ?b ?s))
      (holding-cons ?b)
      (not (hand-free-cons))
      (empty ?s)
      (increase (total-cost) 1)
    )
  )
  ;; Consonant agent place on top of block
  (:action place-cons-on-block
    :parameters (?b - block ?target - block)
    :precondition (and
      (consonant ?b)
      (holding-cons ?b)
      (clear ?target)
      (not (holding-vowel ?target))
      (not (= ?b ?target))
    )
    :effect (and
      (on ?b ?target)
      (not (clear ?target))
      (clear ?b)
      (not (holding-cons ?b))
      (hand-free-cons)
      (increase (total-cost) 1)
    )
  )
  ;; Consonant agent place on empty stack (on table)
  (:action place-cons-on-empty-stack
    :parameters (?b - block ?s - stack)
    :precondition (and
      (consonant ?b)
      (holding-cons ?b)
      (empty ?s)
    )
    :effect (and
      (on ?b ?s)
      (not (empty ?s))
      (clear ?b)
      (not (holding-cons ?b))
      (hand-free-cons)
      (increase (total-cost) 1)
    )
  )
)