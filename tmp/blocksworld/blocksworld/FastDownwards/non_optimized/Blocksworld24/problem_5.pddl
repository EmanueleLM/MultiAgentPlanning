(define (problem multi_agent_block_task)
  (:domain multi_agent_blocks)
  (:objects a b c d e f g h i j k l m n - block
            vowel_agent consonant_agent orchestrator - agent)
  (:init
    (ontable b)
    (on k b)
    (on f k)
    (on m f)
    (on l m)
    (on i l)

    (ontable g)
    (on n g)
    (on d n)
    (on a d)

    (ontable e)
    (on c e)
    (on h c)

    (ontable j)

    (clear i)
    (clear a)
    (clear h)
    (clear j)

    (handempty vowel_agent)
    (handempty consonant_agent)
    (handempty orchestrator)

    (agent-vowel vowel_agent)
    (agent-consonant consonant_agent)

    (is-vowel a)
    (is-vowel e)
    (is-vowel i)

    (is-consonant b)
    (is-consonant c)
    (is-consonant d)
    (is-consonant f)
    (is-consonant g)
    (is-consonant h)
    (is-consonant j)
    (is-consonant k)
    (is-consonant l)
    (is-consonant m)
    (is-consonant n)
  )
  (:metric minimize (total-cost))
  (:goal (and
    (on a b)
    (on b c)
    (on c d)
    (on d e)
    (on e f)
    (on f g)
    (on g h)
    (on h i)
    (on i j)
    (on j k)
    (on k l)
    (on l m)
    (on m n)
    (ontable n)
  ))
)