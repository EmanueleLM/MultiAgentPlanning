(define (domain meeting-scheduling-multiagent)
  (:requirements :strips :typing :negative-preconditions :equality)
  (:types slot)
  (:predicates
    (free-diane ?s - slot)
    (free-jack ?s - slot)
    (free-eugene ?s - slot)
    (free-patricia ?s - slot)
    (meeting-scheduled)
    (scheduled-slot ?s - slot)
    (meeting-with-diane)
    (meeting-with-jack)
    (meeting-with-eugene)
    (meeting-with-patricia)
  )
  (:action schedule-meeting
    :parameters (?s - slot)
    :precondition (and
      (free-diane ?s) (free-jack ?s) (free-eugene ?s) (free-patricia ?s)
      (not (meeting-scheduled))
    )
    :effect (and
      (meeting-scheduled)
      (scheduled-slot ?s)
      (meeting-with-diane) (meeting-with-jack) (meeting-with-eugene) (meeting-with-patricia)
      (not (free-diane ?s)) (not (free-jack ?s)) (not (free-eugene ?s)) (not (free-patricia ?s))
    )
  )
)