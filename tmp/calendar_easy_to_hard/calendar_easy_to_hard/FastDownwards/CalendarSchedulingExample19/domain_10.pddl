(define (domain meeting-scheduling)
  (:requirements :strips :typing)
  (:types person cslot)
  (:predicates
    (free ?p - person ?c - cslot)
    (next ?c - cslot ?c2 - cslot)
    (allowed_start ?c - cslot)
    (confirmed ?p - person ?c - cslot)
    (scheduled ?c - cslot)
    (meeting_scheduled)
  )
  (:action confirm
    :parameters (?p - person ?c - cslot ?c2 - cslot)
    :precondition (and (next ?c ?c2) (free ?p ?c) (free ?p ?c2))
    :effect (confirmed ?p ?c)
  )
  (:action orchestrator_schedule
    :parameters (?c - cslot ?c2 - cslot)
    :precondition (and (next ?c ?c2) (allowed_start ?c)
                       (confirmed stephen ?c)
                       (confirmed edward ?c)
                       (confirmed angela ?c))
    :effect (and (scheduled ?c) (meeting_scheduled))
  )
)