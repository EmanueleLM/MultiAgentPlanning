(define (domain meeting-scheduling)
  (:requirements :typing :negative-preconditions :equality)
  (:types person slot meeting)
  (:predicates
    (work-slot ?s - slot)
    (available ?p - person ?s - slot)
    (scheduled ?m - meeting)
    (scheduled-at ?m - meeting ?s - slot)
    (attending ?p - person ?m - meeting)
    (booked ?s - slot)
  )

  (:action schedule_by_jesse
    :parameters (?m - meeting ?s - slot)
    :precondition (and
      (work-slot ?s)
      (available jesse ?s)
      (available nancy ?s)
      (available isabella ?s)
      (available harold ?s)
      (available linda ?s)
      (not (booked ?s))
    )
    :effect (and
      (scheduled ?m)
      (scheduled-at ?m ?s)
      (attending jesse ?m)
      (attending nancy ?m)
      (attending isabella ?m)
      (attending harold ?m)
      (attending linda ?m)
      (booked ?s)
      (not (available jesse ?s))
      (not (available nancy ?s))
      (not (available isabella ?s))
      (not (available harold ?s))
      (not (available linda ?s))
    )
  )

