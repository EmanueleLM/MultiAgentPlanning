(define (domain orchestrator-meeting)
  (:requirements :typing :negative-preconditions)
  (:types participant time)
  (:constants christine janice bobby elizabeth tyler edward - participant)
  (:predicates
    (slot ?t - time)
    (work-time ?t - time)
    (day-monday ?t - time)
    (meeting-scheduled)
    (scheduled-at ?t - time)
    (meeting-confirmed)
    (available ?p - participant ?t - time)
    (preferred-janice ?t - time)
  )

  (:action schedule_christine
    :parameters (?t - time)
    :precondition (and
      (slot ?t)
      (work-time ?t)
      (day-monday ?t)
      (not (meeting-scheduled))
      (available christine ?t)
      (available janice ?t)
      (available bobby ?t)
      (available elizabeth ?t)
      (available tyler ?t)
      (available edward ?t)
    )
    :effect (and
      (meeting-scheduled)
      (scheduled-at ?t)
    )
  )

  (:action schedule_janice
    :parameters (?t - time)
    :precondition (and
      (slot ?t)
      (work-time ?t)
      (day-monday ?t)
      (not (meeting-scheduled))
      (available christine ?t)
      (available janice ?t)
      (available bobby ?t)
      (available elizabeth ?t)
      (available tyler ?t)
      (available edward ?t)
    )
    :effect (and
      (meeting-scheduled)
      (scheduled-at ?t)
    )
  )

  (:action schedule_bobby
    :parameters (?t - time)
    :precondition (and
      (slot ?t)
      (work-time ?t)
      (day-monday ?t)
      (not (meeting-scheduled))
      (available christine ?t)
      (available janice ?t)
      (available bobby ?t)
      (available elizabeth ?t)
      (available tyler ?t)
      (available edward ?t)
    )
    :effect (and
      (meeting-scheduled)
      (scheduled-at ?t)
    )
  )

  (:action schedule_elizabeth
    :parameters (?t - time)
    :precondition (and
      (slot ?t)
      (work-time ?t)
      (day-monday ?t)
      (not (meeting-scheduled))
      (available christine ?t)
      (available janice ?t)
      (available bobby ?t)
      (available elizabeth ?t)
      (available tyler ?t)
      (available edward ?t)
    )
    :effect (and
      (meeting-scheduled)
      (scheduled-at ?t)
    )
  )

  (:action schedule_tyler
    :parameters (?t - time)
    :precondition (and
      (slot ?t)
      (work-time ?t)
      (day-monday ?t)
      (not (meeting-scheduled))
      (available christine ?t)
      (available janice ?t)
      (available bobby ?t)
      (available elizabeth ?t)
      (available tyler ?t)
      (available edward ?t)
    )
    :effect (and
      (meeting-scheduled)
      (scheduled-at ?t)
    )
  )

  (:action schedule_edward
    :parameters (?t - time)
    :precondition (and
      (slot ?t)
      (work-time ?t)
      (day-monday ?t)
      (not (meeting-scheduled))
      (available christine ?t)
      (available janice ?t)
      (available bobby ?t)
      (available elizabeth ?t)
      (available tyler ?t)
      (available edward ?t)
    )
    :effect (and
      (meeting-scheduled)
      (scheduled-at ?t)
    )
  )

  (:action confirm_meeting
    :parameters (?t - time)
    :precondition (and
      (scheduled-at ?t)
      (available christine ?t)
      (available janice ?t)
      (available bobby ?t)
      (available elizabeth ?t)
      (available tyler ?t)
      (available edward ?t)
    )
    :effect (meeting-confirmed)
  )
)