(define (domain meeting-scheduling)
  (:requirements :strips)
  (:constants diane helen arthur ethan beverly deborah orchestrator)
  (:predicates
    (slot ?s)
    (unscheduled)
    (meeting-scheduled)
    (meeting-at ?s)
    (available ?person ?s)
  )
  (:action schedule
    :parameters (?s)
    :precondition (and (unscheduled) (slot ?s)
                       (available diane ?s)
                       (available helen ?s)
                       (available arthur ?s)
                       (available ethan ?s)
                       (available beverly ?s)
                       (available deborah ?s))
    :effect (and (not (unscheduled))
                 (meeting-scheduled)
                 (meeting-at ?s))
  )
)