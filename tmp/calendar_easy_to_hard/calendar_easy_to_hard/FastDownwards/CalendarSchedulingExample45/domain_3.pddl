(define (domain meeting-scheduling-combined)
  (:requirements :strips :typing :negative-preconditions)
  (:types agent slot)
  (:constants emily brian gerald julia logan judith michael - agent)
  (:predicates
    (available ?a - agent ?s - slot)
    (scheduled ?s - slot)
    (meeting-scheduled)
    (scheduled-by ?a - agent ?s - slot)
  )

  (:action schedule_by_emily
    :parameters (?s - slot)
    :precondition (and
      (not (meeting-scheduled))
      (available emily ?s)
      (available brian ?s)
      (available gerald ?s)
      (available julia ?s)
      (available logan ?s)
      (available judith ?s)
      (available michael ?s)
    )
    :effect (and
      (scheduled ?s)
      (meeting-scheduled)
      (scheduled-by emily ?s)
      (not (available emily ?s))
      (not (available brian ?s))
      (not (available gerald ?s))
      (not (available julia ?s))
      (not (available logan ?s))
      (not (available judith ?s))
      (not (available michael ?s))
    )
  )

