(define (domain vault-domain)
  (:requirements :typing)
  (:types robot vault object key - atom)
  (:predicates
    (closed ?v - vault)
    (open ?v - vault)
    (requires-key ?v - vault)
    (has-key ?r - robot ?k - key)
    (can-enter ?r - robot ?v - vault)
    (inside ?r - robot ?v - vault)
    (object ?o - object)
    (grabbed ?o - object)
  )
  (:action open-vault
    :parameters (?r - robot ?v - vault ?k - key)
    :precondition (and (closed ?v) (requires-key ?v) (has-key ?r ?k))
    :effect (and (open ?v) (not (closed ?v)))
  )
  (:action enter-vault
    :parameters (?r - robot ?v - vault)
    :precondition (and (open ?v) (can-enter ?r ?v) (not (inside ?r ?v)))
    :effect (inside ?r ?v)
  )
  (:action grab-object
    :parameters (?r - robot ?o - object ?v - vault)
    :precondition (and (inside ?r ?v) (not (grabbed ?o)))
    :effect (grabbed ?o)
  )
)