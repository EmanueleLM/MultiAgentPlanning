(define (domain vault-collab)
  (:requirements :typing :strips)
  (:types robot vault key object)
  (:predicates
    (big ?r - robot)
    (small ?r - robot)
    (fits ?r - robot ?v - vault)
    (at-entrance ?r - robot ?v - vault)
    (inside ?r - robot ?v - vault)
    (has-key ?r - robot ?k - key)
    (closed ?v - vault)
    (open ?v - vault)
    (object-inside ?o - object ?v - vault)
    (grabbed ?o - object))
  (:action open-vault
    :parameters (?r - robot ?v - vault ?k - key)
    :precondition (and (has-key ?r ?k) (closed ?v))
    :effect (and (open ?v) (not (closed ?v))))
  (:action enter-vault
    :parameters (?r - robot ?v - vault)
    :precondition (and (open ?v) (at-entrance ?r ?v) (fits ?r ?v))
    :effect (and (inside ?r ?v) (not (at-entrance ?r ?v))))
  (:action grab-object
    :parameters (?r - robot ?o - object ?v - vault)
    :precondition (and (inside ?r ?v) (object-inside ?o ?v))
    :effect (grabbed ?o)))