(define (domain composite-meeting-scheduling)
  (:requirements :typing)
  (:types person slot)

  (:constants diana ethan janet - person)

  (:predicates
    (available ?p - person ?s - slot)
    (occupied ?s - slot)
    (meeting ?s - slot)
    (janet_before12 ?s - slot)
    (proposed-diana ?s - slot)
    (proposed-ethan ?s - slot)
    (proposed-janet ?s - slot)
    (janet_scheduled ?s - slot)
  )

  (:action propose-diana
    :parameters (?s - slot)
    :precondition (and (available diana ?s)
                       (not (occupied ?s))
                       (not (meeting ?s))
                       (not (proposed-diana ?s)))
    :effect (proposed-diana ?s)
  )

  (:action propose-ethan
    :parameters (?s - slot)
    :precondition (and (available ethan ?s)
                       (not (occupied ?s))
                       (not (meeting ?s))
                       (not (proposed-ethan ?s)))
    :effect (proposed-ethan ?s)
  )

  (:action propose-janet
    :parameters (?s - slot)
    :precondition (and (janet_before12 ?s)
                       (available janet ?s)
                       (not (occupied ?s))
                       (not (meeting ?s))
                       (not (proposed-janet ?s)))
    :effect (proposed-janet ?s)
  )

  (:action finalize-meeting
    :parameters (?s - slot)
    :precondition (and (proposed-diana ?s)
                       (proposed-ethan ?s)
                       (proposed-janet ?s)
                       (not (meeting ?s))
                       (not (occupied ?s)))
    :effect (and (meeting ?s)
                 (janet_scheduled ?s)
                 (occupied ?s)
                 (not (available diana ?s))
                 (not (available ethan ?s))
                 (not (available janet ?s)))
  )
)