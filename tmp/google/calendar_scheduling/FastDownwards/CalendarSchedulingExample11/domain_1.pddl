(define (domain meeting-scheduling)
  (:requirements :typing :negative-preconditions)
  (:types person slot)

  (:predicates
    (busy ?p - person ?s - slot)
    (free ?p - person ?s - slot)
    (available ?p - person ?s - slot)
    (scheduled ?s - slot)
    (scheduled-for ?p - person ?s - slot)
    (meeting-scheduled)
    (meeting-at ?s - slot)
    (meeting-unscheduled)
    (meeting-scheduled-slot ?s - slot)
  )

  (:action schedule_from_agent1
    :parameters (?s - slot ?p1 - person ?p2 - person ?p3 - person)
    :precondition (and
      (not (scheduled ?s))
      (not (busy ?p1 ?s))
      (not (busy ?p2 ?s))
      (not (busy ?p3 ?s))
      (not (meeting-scheduled))
    )
    :effect (and
      (scheduled ?s)
      (meeting-scheduled)
      (meeting-at ?s)
      (meeting-scheduled-slot ?s)
      (scheduled-for ?p1 ?s)
      (scheduled-for ?p2 ?s)
      (scheduled-for ?p3 ?s)
      (not (meeting-unscheduled))
    )
  )

  (:action schedule_from_agent2
    :parameters (?s - slot ?p1 - person ?p2 - person ?p3 - person)
    :precondition (and
      (meeting-unscheduled)
      (free ?p1 ?s)
      (free ?p2 ?s)
      (free ?p3 ?s)
    )
    :effect (and
      (meeting-scheduled-slot ?s)
      (meeting-scheduled)
      (meeting-at ?s)
      (scheduled ?s)
      (scheduled-for ?p1 ?s)
      (scheduled-for ?p2 ?s)
      (scheduled-for ?p3 ?s)
      (not (meeting-unscheduled))
    )
  )

  (:action schedule_from_agent3
    :parameters (?s - slot ?p1 - person ?p2 - person ?p3 - person)
    :precondition (and
      (available ?p1 ?s)
      (available ?p2 ?s)
      (available ?p3 ?s)
      (not (meeting-scheduled-slot ?s))
    )
    :effect (and
      (meeting-scheduled-slot ?s)
      (meeting-scheduled)
      (meeting-at ?s)
      (scheduled ?s)
      (scheduled-for ?p1 ?s)
      (scheduled-for ?p2 ?s)
      (scheduled-for ?p3 ?s)
      (not (meeting-unscheduled))
    )
  )
)