(define (domain multi_agent_meeting_scheduler)
  (:requirements :typing)
  (:types person time)
  (:predicates
     (available ?p - person ?t - time)
     (global-scheduled ?t - time)
  )
  (:action schedule_meeting
     :parameters (?start - time ?p1 - person ?p2 - person ?p3 - person)
     :precondition (and (available ?p1 ?start)
                        (available ?p2 ?start)
                        (available ?p3 ?start))
     :effect (and (not (available ?p1 ?start))
                  (not (available ?p2 ?start))
                  (not (available ?p3 ?start))
                  (global-scheduled ?start))
  )
)