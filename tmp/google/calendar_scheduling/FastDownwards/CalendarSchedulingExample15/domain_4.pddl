(define (domain calendar-scheduling)
  (:requirements :typing)
  (:types slot)
  (:predicates
    (joyce_available ?s - slot)
    (christine_available ?s - slot)
    (alexander_available ?s - slot)
    (meeting_at ?s - slot)
  )
  (:action schedule_meeting
     :parameters (?s - slot)
     :precondition (and (joyce_available ?s)
                        (christine_available ?s)
                        (alexander_available ?s)
                        (not (meeting_at ?s)))
     :effect (and (not (joyce_available ?s))
                  (not (christine_available ?s))
                  (not (alexander_available ?s))
                  (meeting_at ?s))
  )
)