(define (domain calendar-scheduling)
  (:requirements :typing)
  (:types slot)
  (:predicates
    (joyce_free ?s - slot)
    (christine_free ?s - slot)
    (alexander_free ?s - slot)
    (meeting_at ?s - slot)
  )
  (:action schedule_meeting
     :parameters (?s - slot)
     :precondition (and (joyce_free ?s)
                        (christine_free ?s)
                        (alexander_free ?s))
     :effect (meeting_at ?s)
  )
)