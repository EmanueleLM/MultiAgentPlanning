(define (domain meeting-scheduling)
  (:requirements :strips :typing)
  (:types agent slot)
  (:predicates
    (free ?a - agent ?s - slot)
    (preferred-amy ?s - slot)
    (committed-roy ?s - slot)
    (committed-kathryn ?s - slot)
    (committed-amy ?s - slot)
    (meeting-scheduled ?s - slot)
  )
  (:action roy-commit
    :parameters (?s - slot)
    :precondition (free roy ?s)
    :effect (committed-roy ?s)
  )
  (:action kathryn-commit
    :parameters (?s - slot)
    :precondition (free kathryn ?s)
    :effect (committed-kathryn ?s)
  )
  (:action amy-commit
    :parameters (?s - slot)
    :precondition (free amy ?s)
    :effect (committed-amy ?s)
  )
  (:action finalize-meeting
    :parameters (?s - slot)
    :precondition (and (committed-roy ?s) (committed-kathryn ?s) (committed-amy ?s))
    :effect (and
      (meeting-scheduled ?s)
      (not (free roy ?s))
      (not (free kathryn ?s))
      (not (free amy ?s))
      (not (committed-roy ?s))
      (not (committed-kathryn ?s))
      (not (committed-amy ?s))
    )
  )
)