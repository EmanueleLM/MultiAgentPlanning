(define (domain meeting-scheduler-integrated)
  (:requirements :typing)
  (:types person time meeting)
  (:constants Brittany Emily Doris - person
              M1 - meeting
              t0900 t1100 t1430 - time)
  (:predicates
     (available ?p - person ?t - time)
     (free ?p - person ?t - time)
     (meeting_scheduled_at ?t - time)
     (scheduled ?m - meeting ?t - time)
  )
  ;; Agent 1: domain1-style scheduling (generic time parameter)
  (:action agent1-schedule-meeting
     :parameters (?t - time)
     :precondition (and (not (meeting_scheduled_at ?t))
                        (available Brittany ?t)
                        (available Emily ?t)
                        (available Doris ?t))
     :effect (and (meeting_scheduled_at ?t)
                  (not (available Brittany ?t))
                  (not (available Emily ?t))
                  (not (available Doris ?t)))
  )
  ;; Agent 2: domain2-style scheduling (needs a specific meeting and time)
  (:action agent2-schedule-meeting
     :parameters (?m - meeting ?t - time)
     :precondition (and (not (scheduled ?m ?t))
                       (free Brittany ?t)
                       (free Emily ?t)
                       (free Doris ?t))
     :effect (and (not (free Brittany ?t))
                  (not (free Emily ?t))
                  (not (free Doris ?t))
                  (scheduled ?m ?t)
                  (meeting_scheduled_at ?t))
  )
  ;; Agent 3: specific time-slot actions (distinct actions for 09:00, 11:00, 14:30)
  (:action agent3-schedule-m1-at-09-00
     :parameters ()
     :precondition (and (not (scheduled M1 t0900))
                        (free Brittany t0900)
                        (free Emily t0900)
                        (free Doris t0900))
     :effect (and (not (free Brittany t0900))
                  (not (free Emily t0900))
                  (not (free Doris t0900))
                  (scheduled M1 t0900)
                  (meeting_scheduled_at t0900))
  )
  (:action agent3-schedule-m1-at-11-00
     :parameters ()
     :precondition (and (not (scheduled M1 t1100))
                        (free Brittany t1100)
                        (free Emily t1100)
                        (free Doris t1100))
     :effect (and (not (free Brittany t1100))
                  (not (free Emily t1100))
                  (not (free Doris t1100))
                  (scheduled M1 t1100)
                  (meeting_scheduled_at t1100))
  )
  (:action agent3-schedule-m1-at-14-30
     :parameters ()
     :precondition (and (not (scheduled M1 t1430))
                        (free Brittany t1430)
                        (free Emily t1430)
                        (free Doris t1430))
     :effect (and (not (free Brittany t1430))
                  (not (free Emily t1430))
                  (not (free Doris t1430))
                  (scheduled M1 t1430)
                  (meeting_scheduled_at t1430))
  )
)