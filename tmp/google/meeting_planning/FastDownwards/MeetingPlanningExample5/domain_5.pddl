(define (domain MeetingSF)
  (:requirements :typing :durative-actions :numeric-fluent)
  (:types location person)
  (:constants NobHill TheCastro - location
              visitor william - person)
  (:predicates
     (at ?p - person ?l - location)
     (met ?p - person ?w - person)
  )
  (:functions time)
  (:durative-action visitor-wait-12_15
     :parameters ()
     :duration (= ?dur 195)
     :condition (at start (at visitor NobHill))
     :effect (increase (time) 195)
  )
  (:durative-action visitor-travel-nobToCastro
     :parameters ()
     :duration (= ?dur 17)
     :condition (and (at start (at visitor NobHill))
                     (>= (time) 195))
     :effect (and (at end (not (at visitor NobHill)))
                  (at end (at visitor TheCastro))
                  (increase (time) 17))
  )
  (:durative-action visitor-travel-castroToNobHill
     :parameters ()
     :duration (= ?dur 16)
     :condition (and (at start (at visitor TheCastro))
                     (>= (time) 195))
     :effect (and (at end (not (at visitor TheCastro)))
                  (at end (at visitor NobHill))
                  (increase (time) 16))
  )
  (:durative-action visitor-meet-william
     :parameters ()
     :duration (= ?dur 75)
     :condition (and (at start (at visitor TheCastro))
                     (at start (at william TheCastro))
                     (>= (time) 195))
     :effect (and (met visitor william)
                  (increase (time) 75))
  )
)